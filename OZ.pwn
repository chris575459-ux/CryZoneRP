MensajeEx(playerid, color, form[], {Float, _}: ...)
{
	#pragma unused form

	static
		tmp[145]
	;
	new
		t1 = playerid,
		t2 = color
	;
	const
		n4 = -4,
		n16 = -16,
		size = sizeof tmp
	;
	#emit stack 28
	#emit push.c size
	#emit push.c tmp
	#emit stack n4
	#emit sysreq.c format
	#emit stack n16

	return (t1 == -1 ? (SendClientMessageToAll(t2, tmp)) : (SendClientMessage(t1, t2, tmp)) );
}

