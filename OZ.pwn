{
	if(PlayerInfo[playerid][pGPS] != 1) return Mensaje(playerid, -1, "Tu no posees un GPS. Puedes comprar uno en cualquier {DBED15}24/7");
	ShowPlayerDialog(playerid, GPS, DIALOG_STYLE_LIST, "GPS: Puntos de interés", "Espacios de trabajo\nEspacios Públicos\nTiendas de ropa\nConcesionarios\nBancos\nTiendas 24/7\nModificación de vehículos\nPay N Spray\nPay N Spray Aereo\nPay N Spray Maritimo\nFerreterias\nIglesias\nEstadios\nGranjas\nVer Mapa", "Seleccionar", "Cancelar");
	new string[40];
	format(string, 167,"%s mira su GPS.", NombreJugador(playerid));
	ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
	return 1;
}

CMD:esposar(playerid, params[])
{
	new data[144], Float:pos[3], id;
	if(sscanf(params, "r", id))return Mensaje(playerid,COLORCARGO0,"Usa: {F4FA58}/esposar [id/nombre]");
	if(IsPlayerNPC(id)) return 1;
	if(PlayerInfo[playerid][pDuty] != 1) return 1;
	if(!IsPlayerConnected(id)) return Mensaje(playerid,COLORCARGO0,"Jugador desconectado o id incorrecta.");
	if(PlayerInfo[id][pCargos] == 0) return Mensaje(playerid,-1,"Este jugador no tiene nivel de busqueda.");
	GetPlayerPos(id, pos[0], pos[1], pos[2]);
	if(!EstaEn(playerid, 5.0, pos[0], pos[1], pos[2])) return Mensaje(playerid,-1,"Debes estar cerca de esta persona para esposarla.");
	{
		Esposando[id] = 1;
		Oficial[id] = playerid;
		CongelarEx(id);
		TimerEsposar[id] = SetTimerEx("TiempoArresto", 10000, false, "d", id);
		EnPatrulla[id] = 0;
		SetPlayerSpecialAction(id,SPECIAL_ACTION_HANDSUP);
		Mensaje(id,-1,"El policía intenta esposarte, tiene 10 segundos para {DBED15}/resistirse{FFFFFF}.");
		format(data, sizeof(data), "%s %s intenta esposar a %s", RangoPolicia(playerid),NombreJugador(playerid), NombreJugador(id));
		ProxDetector(10.0, playerid, data, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
	}
	return true;
}

CMD:resistirse(playerid, params[])
{
	new string[126];
	if(Esposando[playerid] == 1)
	{
		if(PlayerInfo[playerid][pCargos] == 1)
		{
			DarEstrella(playerid, PlayerInfo[playerid][pCargos] = 2);
			MarcarJugador(playerid,COLORCARGO2,1);
		}
		DescongelarEx(playerid);
		Esposando[playerid] = 0;
		SetPlayerSpecialAction(playerid, SPECIAL_ACTION_NONE);
		KillTimer(TimerEsposar[playerid]);
		format(string, sizeof(string), "%s se resistió al arresto.",NombreJugador(playerid));
		ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
		format(string,sizeof(string),"[Radio de policía] (%s %s) dice: %s se resistió al arresto, inicio persecución.",RangoPolicia(Oficial[playerid]),NombreJugador(Oficial[playerid]), NombreJugador(playerid));
		ProxDetector(10.0, playerid, string,-1, ColorChatNormal1,ColorChatNormal2,ColorChatNormal3,ColorChatNormal4);
		format(string, sizeof(string), "[Radio de policía]{ffffff} (%s %s): %s se resistió al arresto, inicio persecución.", RangoPolicia(Oficial[playerid]),NombreJugador(Oficial[playerid]),NombreJugador(playerid));
		SendRadioMessage(1, TEAM_BLUE_COLOR, string,Oficial[playerid], 1);

	}
	else Mensaje(playerid, -1,"No estas siendo esposado para resistirte.");
	return 1;
}

CMD:entregar(playerid,params[])
{
	new string[158],patrulla;
	patrulla = GetPlayerVehicleID(playerid);
	if(IsPlayerInAnyVehicle(playerid))
	if(PlayerInfo[playerid][pDuty] != 1) return Mensaje(playerid, COLORCARGO0, "No estás autorizado para usar este comando.");
	if(!EnComisaria(playerid)) return Mensaje(playerid,-1,"No estás en el área de entrega de ninguna comisaria.");
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_GLOBAL2)
	{
		if(!SAPD(patrulla)) return Mensaje(playerid,-1,"Debes estar en una patrulla o helicoptero policial.");
	}
	for(new i, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if(EnAuto[i] == patrulla && PlayerInfo[i][pCargos] > 0 && IsPlayerConnected(i) && IsPlayerInVehicle(i,patrulla))
		{
			RemovePlayerFromVehicle(i);
			SetTimerEx("Encerrando",1500,false,"dd",i,playerid);
			format(string, sizeof(string), "%s baja a %s de su vehículo.", NombreJugador(playerid),NombreJugador(i));
			ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
		}
	}
	return 1;
}


CMD:liberar(playerid,params[])
{
	new id,Float:x,Float:y,Float:z,string[128];
	if(!sscanf(params,"d",id))
	{
		if(PlayerInfo[playerid][pDuty] == 1 && PlayerInfo[playerid][jMiembro] == 1)
		{
			GetPlayerPos(id, x,y,z);
			if(!EstaEn(playerid, 5.0, x,y,z)) return Mensaje(playerid, -1, "No estas cerca de este jugador");
			if((VariableCaido[id] == 1 || Esposando[id] == 2 || EnPatrulla[id] == 1) && PlayerInfo[id][pCargos] > 0)
			{
				VariableCaido[id] = 0;
				Esposando[id] = 0;
				SetPlayerSpecialAction(id, SPECIAL_ACTION_NONE);
				PlayerInfo[id][pCargos] = 0;
				DarEstrella(id,PlayerInfo[id][pCargos]);
				DescongelarEx(id);
				format(string, 128, "%s %s libera al sospechoso",RangoPolicia(playerid),NombreJugador(playerid));
				ProxDetector(10.0,playerid,string,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
				if(IsPlayerInAnyVehicle(id)) RemovePlayerFromVehicle(id);
			}
			else return Mensaje(playerid, -1, "El jugador debe estar abatido o esposado.");
		}
	}
	else return Mensaje(playerid, -1, "Modo de uso /liberar id");
	return 1;
}


CMD:arrestar(playerid, params[])
{
	new string[126];
	new ID, patrulla, Float:x, Float:y, Float:z;
	patrulla = GetPlayerVehicleID(playerid);
	if(sscanf(params, "d", ID))return Mensaje(playerid,COLORCARGO0,"Uso: /arrestar [ID]");
	if(!IsPlayerConnected(ID)) return Mensaje(playerid,COLORCARGO0,"Jugador desconectado o id incorrecta.");
	if(PlayerInfo[playerid][pDuty] != 1) return Mensaje(playerid, COLORCARGO0, "No estás autorizado para usar este comando.");
	if(VariableCaido[ID] != 1 && Esposando[ID] != 2) return Mensaje(playerid,COLORCARGO0,"El sospechoso no esta esposado o abatido.");
	if(PlayerInfo[ID][pCargos] == 0) return Mensaje(playerid,-1,"Este jugador no tiene nivel de busqueda.");
	if(!IsPlayerInAnyVehicle(playerid)) return Mensaje(playerid,-1,"Debes estar en una patrulla o helicoptero policial.");
	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return Mensaje(playerid,-1,"Debes estar conduciendo la patrulla.");
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_GLOBAL2)
	{
		if(!SAPD(patrulla)) return Mensaje(playerid,-1,"Debes estar en una patrulla o helicoptero policial.");
 	}
	GetPlayerPos(ID, x,y,z);
	if(EstaEn(playerid, 40.0,  x, y,z))
	{
		if(!IsVehicleSeatInUse(patrulla,2))
		{
			PutPlayerInVehicle(ID, patrulla, 2);
			SetPlayerArmedWeapon(ID, 0);
			BajandoV[ID] = 0;
			EnAuto[ID] = patrulla;
			EnPatrulla[ID] = 1;
			VariableCaido[ID] = 0;
			CongelarEx(ID);
			format(string, sizeof(string), "%s %s sube al detenido a su patrullero.", RangoPolicia(playerid),NombreJugador(playerid));
			ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
			return 1;
		}
		if(!IsVehicleSeatInUse(patrulla,3))
		{
			PutPlayerInVehicle(ID, patrulla, 3);
			SetPlayerArmedWeapon(ID, 0);
			BajandoV[ID] = 0;
			EnAuto[ID] = patrulla;
			EnPatrulla[ID] = 1;
			VariableCaido[ID] = 0;
			CongelarEx(ID);
			format(string, sizeof(string), "%s %s sube al detenido a su patrullero.", RangoPolicia(playerid),NombreJugador(playerid));
			ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
			return 1;
		}
		else
		{
			Mensaje(playerid, -1,"Ya no puede subir a nadie mas a tu patrulla.");
			return 1;
		}
	}
	else Mensaje(playerid, -1, "No estas cerca de ese jugador.");
	return 1;
}

CMD:entrar(playerid, params[])
{
	if(PlayerTemp[playerid][pt_Congelado]) return 1;
	if(VariableCaido[playerid]) return 1;
	if(PlayerInfo[playerid][pDuty] != 1)
 	{
 		if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_JUEGO1)
 		{
			if(EnCombate[playerid] == 1) return Mensaje(playerid,-1,"No puedes hacer esto en combate, espera unos segundos y reintenta.");
		}
	}
	if(EstaEn(playerid, 2.0,186.7460,1428.8141,10.5912) && PlayerInfo[playerid][pTipoCarcel] == 4 || EstaEn(playerid, 2.0,186.7460,1428.8141,10.5912) && PlayerInfo[playerid][pDuty] == 1) 	// FC
	{
		StatusFC[playerid] = 0;
		SetPosEx(playerid,168.1586,1436.1012,1084.8462,0,15,15);
		return 1;
	}
	if(PlayerInfo[playerid][pTiempoJail] == 0 && PlayerInfo[playerid][pTiempoCarcel] == 0)
	{
		if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT) return Ingresar(playerid, 1);
		else if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER) return IngresarAuto(playerid);
		else return 1;
	}
	else Mensaje(playerid, -1, "No puedes hacer esto si estas en prisión.");
	return 1;
}

