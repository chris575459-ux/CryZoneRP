CMD:mute(playerid,params[])
{
	new id,tiempo,string[128],razon[60],string2[128];
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_DUDAS) return 0;
	if(!sscanf(params,"rds[60]",id,tiempo,razon))
	{
		if(!IsPlayerConnected(id)) return Mensaje(playerid,COLORCARGO0,"No se encontró ningún jugador.");
		if(GetPlayerScore(id) == 0) return Mensaje(playerid, -1, "El jugador que quieres sancionar está conectado pero aún no ha ingresado al juego.");
		if(tiempo <= 4) return Mensaje(playerid,-1,"El tiempo minimo de muteo es de 5 minutos.");
		if(TiempoDuda[id] >= 300) return Mensaje(playerid,-1,"Ese jugador ya esta muteado.");
		if(!strcmp(NombreJugador(id), "Hahn") || !strcmp(NombreJugador(id), "Kevin_Mcfly")) return Mensaje(playerid,COLORCARGO0,"No puedes usar este comando con ese usuario.");
		if(PlayerInfo[id][jAdminNvl] >= PlayerInfo[playerid][jAdminNvl])
		{
			format(string, sizeof(string), "%s(%d) intento mutear a un ADM /n (%s).",NombreJugador(playerid), playerid, NombreJugador(id));
			MensajeAdmin(COLOR_GREY, string, PlayerInfo[playerid][jAdminNvl]);
			return 1;
		}
		//new calculo = tiempo*60*1000;
		TiempoDuda[id] = tiempo*60;
		//SetTimerEx("DudaOn",calculo,0,"d",id);
		format(string, sizeof(string), "A %s no se le permite hablar en el chat de ayuda. Razón: %s.",NombreJugador(id), razon);
		for(new l, p = GetPlayerPoolSize(); l <= p; l++)
		{
			if(PlayerConectado(l) && ShowMutes[l] == 1) Mensaje(l,0xAA3333FF,string);
		}

		format(string, 128, "(%d) Tiempo de muteo: %d minutos, Administrador: %s",id,tiempo,NombreJugador(playerid));
		MensajeAdmin(COLOR_GREY, string, PlayerInfo[playerid][jAdminNvl]);

		format(string2,128,"Se te descontó $%s por el bloqueo del canal (/n)",ConvertirNumero(tiempo * 10));
		Mensaje(id,0xDEA600FF,string2);
		QuitarDinero(id,(tiempo * 10));

		new message[120];
		format(message, 120, "%s muteo a %s por %s /n (Tiempo: %d).", NombreJugador(playerid), NombreJugador(id), razon, tiempo);
		Log("logs/mute.log", message);
	}
	else return Mensaje(playerid,-1,"Usa: /mute [id] [minutos] [razón]");
	return 1;
}

CMD:nmute(playerid,params[])
{
	new id,string[128],razon[50];
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_DUDAS) return 0;
	if(!sscanf(params,"rs[50]",id,razon))
	{
		if(!IsPlayerConnected(id)) return Mensaje(playerid,COLORCARGO0,"No se encontró ningún jugador.");
