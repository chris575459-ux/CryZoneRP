CMD:ficha(playerid,params[])
{
	new monto,autoid,string[256];
	autoid = GetPlayerVehicleID(playerid);
	if(PlayerInfo[playerid][jJob] != TAXISTA && PlayerInfo[playerid][jJob2] != TAXISTA) return Mensaje(playerid,-1,"Necesitas ser taxista para utilizar este comando.");
	if(TAXI(autoid) && GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
	{
		if(!sscanf(params,"d",monto))
		{
			if(monto == 0)
			{
				Ficha[playerid] = monto;
				format(string,256,"({00CCFF}%s{FFFFFF}) apagó el taxímetro.", NombreJugador(playerid));
				ProxDetector(10.0, playerid, string, COLOR_BLANCO,COLOR_BLANCO,COLOR_BLANCO,COLOR_BLANCO,COLOR_BLANCO);
				return 1;
			}
			if(monto >= 2 && monto <= 100)
			{
				Ficha[playerid] = monto;
				format(string,256,"El taxímetro de ({00CCFF}%s{FFFFFF}) fue configurado con un precio de {37DB45}$%s{FFFFFF} por ficha.", NombreJugador(playerid), ConvertirNumero(monto));
				ProxDetector(10.0, playerid, string, COLOR_BLANCO,COLOR_BLANCO,COLOR_BLANCO,COLOR_BLANCO,COLOR_BLANCO);
			}
			else return Mensaje(playerid,-1,"El precio de la ficha tiene que ser entre {37DB45}$2{FFFFFF} a {37DB45}$100{FFFFFF}.");
		}
		else return Mensaje(playerid,-1,"Modo de uso: /ficha [2-100]");
	}
	else return Mensaje(playerid,-1,"Debes estar conduciendo un taxi para utilizar este comando.");
	return 1;
}

