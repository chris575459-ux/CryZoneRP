	{
		StatusFC[playerid] = 1;
		SetPosEx(playerid,186.7460,1428.8141,10.5912,0,0,0);
		return 1;
	}
	if(PlayerInfo[playerid][pTiempoJail] == 0 && PlayerInfo[playerid][pTiempoCarcel] == 0)
