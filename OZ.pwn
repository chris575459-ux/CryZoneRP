public OnPlayerModelSelection(playerid, response, listid, modelid)
{
	if(listid == objetos)
	{
		if(response)
		{
			if(NPrendas[playerid] >= 4) return Mensaje(playerid,-1,"No puedes comprar más de 4 prendas.");
			if(PlayerInfo[playerid][Totems] < 3) return Mensaje(playerid,-1,"Comprar una prenda cuesta {FF3300}3{FFFFFF} Totems.");
			if(PlayerInfo[playerid][pObj] == modelid || PlayerInfo[playerid][pObj2] == modelid || PlayerInfo[playerid][pObj3] == modelid || PlayerInfo[playerid][pObj4] == modelid) return Mensaje(playerid,-1,"Ya tienes esa prenda en tu inventario.");
			if(PlayerInfo[playerid][pObj] == 0 || PlayerInfo[playerid][pObj] == 11392)
			{
				new Query[200];
				PlayerInfo[playerid][Totems] -= 3;
				Mensaje(playerid,-1,"{DBED15}¡Felicidades!{FFFFFF} Compraste una prenda, ahora puedes {DBED15}/usar prendas");
				NPrendas[playerid]++;
				UsandoObj[playerid] = 0;
				PlayerInfo[playerid][pObj] = modelid;
				PlayerInfo[playerid][pfOffsetX] = 0.085;
				PlayerInfo[playerid][pfOffsetY] = 0.017999;
				PlayerInfo[playerid][pfOffsetZ] = 0.000999;
				PlayerInfo[playerid][pfRotX] = 87.2;
				PlayerInfo[playerid][pfRotY] = 88.1;
				PlayerInfo[playerid][pfRotZ] = -7.5;
				PlayerInfo[playerid][pfScaleX] = 1;
				PlayerInfo[playerid][pfScaleY] = 1;
