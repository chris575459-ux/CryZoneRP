				case 3: MensajeEx(playerid, -1, "Camión de Carga Número: {00CC00}%d{FFFFFF} | Contenido: {FF3300}Drogas{FFFFFF} | Chofer: {DBED15}%s", vehicleid-28,NombreJugador(Chofer[vehicleid]));
				case 4: MensajeEx(playerid, -1, "Camión de Carga Número: {00CC00}%d{FFFFFF} | Contenido: {FF3300}Armas{FFFFFF} | Chofer: {DBED15}%s", vehicleid-28,NombreJugador(Chofer[vehicleid]));
			}
