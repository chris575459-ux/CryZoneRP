		{
			if(PlayerConectado(l) && ShowMutes[l] == 1) Mensaje(l,0xAA3333FF,string);
		}

		format(string, 128, "(%d) Administrador: %s (%d)", id, NombreJugador(playerid), TiempoDuda[id]);
		MensajeAdmin(COLOR_GREY, string, PlayerInfo[playerid][jAdminNvl]);

		Mensaje(id,0xDEA600FF,"Se te quito la restricción del canal de dudas.");

		new message[120];
