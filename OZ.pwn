function AntiClones()
{
	new bool: occupied[MAX_VEHICLES];
	for(new v = 0; v < MAX_VEHICLES; v++)
	{
		if(v > AUTOS)
		{
			foreach(new i : Player)
			{
				if (v == autos1[i] || v == autos2[i] || v == autos3[i] || v == autos4[i] || v == GetPlayerVehicleID(i)) occupied[v] = true;
			}
			for(new a; a < sizeof(VehiculoAsalto); a++)
			{
				if(VehiculoAsalto[a] == v) occupied[v] = true;
			}
			for(new a; a < sizeof(VehiculoCarrera); a++)
			{
				if(VehiculoCarrera[a] == v) occupied[v] = true;
			}
			if(!occupied[v])
			{
				MotorAuto[v] = 0;
				DestroyVehicle(v);
			}
		}
	}
	return 1;
}

