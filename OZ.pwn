		if(PlayerInfo[playerid][vModelo] > 0 || PlayerInfo[playerid][vModelo2] > 0 || PlayerInfo[playerid][vModelo3] > 0 || PlayerInfo[playerid][vModelo4] > 0) SetTimerEx("Autos",1000,false,"%d",playerid);
		if(PlayerInfo[playerid][VIP] == 1)
		{
			if(PlayerInfo[playerid][jTiempoVip] > gettime()) PlayerInfo[playerid][VIP] = 1;
			else PlayerInfo[playerid][VIP] = 0;
		}
		Mensaje(playerid, 0xFFFFFFAA, "Cargando datos del juego...");
		Mensaje(playerid, 0xFFFFFFAA, "{DBED15}El servidor está en constante crecimiento. Publica tus sugerencias en el foro.");
		Mensaje(playerid, 0xFFFFFFAA, "{FFFFFF}Escribe {DBED15}/ayuda{FFFFFF} para recibir ayuda.");
		Mensaje(playerid, 0xFFFFFFAA, "Para recibir ayuda de otros jugadores, usa {DBED15}/n {FFFFFF}({DBED15}y tu pregunta{FFFFFF}).");

		format(string2, sizeof(string2), "%02d/%02d/%02d %02d:%02d:%02d", day,month,year,hour,minuite,second);

		mysql_format(Conecction, Query, sizeof(Query),"INSERT INTO `log_ingresos` (`UserID`, `Pais`, `IP` , `Host`, `Fecha`) VALUES ('%d', '%s', INET_ATON('%s'), '%s', UNIX_TIMESTAMP())", PlayerInfo[playerid][p_DBID], country, PlayerInfo[playerid][pi_IP], isp);
		mysql_pquery(Conecction, Query);

		mysql_format(Conecction, Query, sizeof(Query),"UPDATE `usuarios` SET `IP`='%s',`Pais`='%s' WHERE `ID`='%d'", PlayerInfo[playerid][pi_IP], country, PlayerInfo[playerid][p_DBID]);
		mysql_pquery(Conecction, Query);

		if(PlayerInfo[playerid][jAdminNvl] > 0 || PlayerInfo[playerid][jMiembro] == 1)
		{
			if(strcmp(PlayerInfo[playerid][pPais], country, true ) != 0)
			{
				new Str[100];
				format(Str, sizeof(Str), "%s / Viejo: %s/%s - Nuevo: %s/%s", NombreJugador(playerid), PlayerInfo[playerid][pPais], PlayerInfo[playerid][pCiudad], country, city);
				Log("logs/vendido.log", Str);
			}
		}
		format(PlayerInfo[playerid][pPais],32,"%s", country);
		format(PlayerInfo[playerid][pCiudad],32,"%s", city);
		CalcularNivel(playerid);
		MostrarMapas(playerid);
		LoadAnimLibs(playerid);
		TextDrawShowForPlayer(playerid, CN5);
		StopAudioStreamForPlayer(playerid);
		ClearAnimations(playerid);
		SetPlayerTime(playerid,hour,minuite);
		SetPlayerInterior(playerid, PlayerInfo[playerid][pInterior]);
		SetPlayerVirtualWorld(playerid, PlayerInfo[playerid][pMundoVirtual]);

		BarraExp(playerid, 1);

		if(SedOculta[playerid] == 1)
		{
			BarraHambre(playerid, 1);
			BarraSed(playerid, 1);
		}

		ResetPlayerWeapons(playerid);
		DarArmas(playerid);
		TextDrawHideForPlayer(playerid, CN);
		TextDrawHideForPlayer(playerid, CN2);
		TextDrawHideForPlayer(playerid, CN3);
		TextDrawHideForPlayer(playerid, CN4);
		ColorJugadorDefault(playerid);
		ResetPlayerMoney(playerid);
		DarRopa(playerid);
		LoadPrendas(playerid);
		LoadLogros(playerid);
		SetPlayerStats(playerid);
		SetPlayerArmedWeapon(playerid, 0);
		CancelSelectTextDraw(playerid);
		if(PlayerInfo[playerid][jMiembro] == 1 && PlayerInfo[playerid][pDuty] == 1)
		{
			Frecuencia[playerid] = 1;
			frecuencias[1]++;
		}
		SetTimerEx("Logeado",1000,false,"d",playerid);
		new ipp[30];
		if(PlayerInfo[playerid][jMiembro] > 0)
		{
			format(ipp,sizeof(ipp),"%s",NombreJugador(playerid));
			for(new i, j = GetPlayerPoolSize(); i <= j; i++)
			{
   				if(IsPlayerConnected(i) && i != playerid && PlayerInfo[playerid][jMiembro] == PlayerInfo[i][jMiembro] && ShowCone[i] == 1)
				{
	   				PlayerTextDrawBoxColor(i, NotiConnect[i][0], 255);
					PlayerTextDrawColor(i, NotiConnect[i][1], 8388863);
					PlayerTextDrawColor(i, NotiConnect[i][2], -1);
					PlayerTextDrawColor(i, NotiConnect[i][3], -1);

					PlayerTextDrawSetString(i,NotiConnect[i][2],ipp);
					PlayerTextDrawSetPreviewModel(i, NotiConnect[i][3], PlayerInfo[playerid][jRopa]);

					PlayerTextDrawShow(i,NotiConnect[i][0]);
					PlayerTextDrawShow(i,NotiConnect[i][1]);
					PlayerTextDrawShow(i,NotiConnect[i][2]);
					PlayerTextDrawShow(i,NotiConnect[i][3]);

					NotificacionConectado[i] = 10;
				}
			}
		}
		DescongelarEx(playerid);
		JugadorLogeado[playerid] = 1;
	}
	if(VariableCaido[playerid] == 1 && PlayerInfo[playerid][pCargos] == 1)
	{
		PlayerInfo[playerid][pCargos] = 0;
		DarEstrella(playerid, PlayerInfo[playerid][pCargos]);
		HeridoJugador(playerid);
		return 1;
	}
	if(PlayerInfo[playerid][Agonizando] == 1)
	{
		HeridoJugador(playerid);
		return 1;
	}
	if(PlayerInfo[playerid][Agonizando] == 2)
	{
		ApareceHospital(playerid);
		return 1;
	}
	if(VariableCaido[playerid] == 1 && PlayerInfo[playerid][pCargos] > 1)
	{
		DarPrendas(playerid);
		PlayerInfo[playerid][Agonizando] = 0;
		ResetPlayerWeapons(playerid);
		DarArmas(playerid);
		DarRopa(playerid);
		CongelarEx(playerid);
		EstablecerVida(playerid, 10.0);
		Mensaje(playerid, -1, "{DBED15}Los médicos no van a tratar con un delincuente.");
		Mensaje(playerid, -1, "{DBED15}Espera a que la policía te traslade.");
		ClearAnimations(playerid);
		ApplyAnimation(playerid, "SWEET", "Sweet_injuredloop", 4.0, 1, 0, 0, 0, 0, 1);
		ApplyAnimation(playerid, "SWEET", "Sweet_injuredloop", 4.0, 1, 0, 0, 0, 0, 1);
		for(new i, j = GetPlayerPoolSize(); i <= j; i++)
		{
			if(PlayerInfo[i][pDuty] == 1)
			{
				SetPlayerMarkerForPlayer(i, playerid, COLOR_GREEN2);
			}
		}
		return 1;
	}
	if(PlayerInfo[playerid][pTiempoCarcel] > 0)
	{
		DarPrendas(playerid);
		EnviarCarcel(playerid);
		Jail[playerid] = 1;
		return 1;
	}
	if(PlayerInfo[playerid][pTiempoJail] > 0)
	{
		DarPrendas(playerid);
		San[playerid] = 1;
		SetTimerEx("EnviarJail", 2000, false, "d", playerid);
		return 1;
	}
	if(AceptacionBlindado[playerid] == 3)
	{
   		SetPlayerColor(playerid,COLORCARGO0);
   		AceptacionBlindado[playerid] = 3;
		SacarDeMision(playerid,1);
		switch(GrupoRobo[playerid])
		{
	  		case 1:
			{
		  		SetPlayerPosEx(playerid,1241.8650,371.2643,19.5547,333.0306);
		  		Mensaje(playerid,-1,"Fuiste atendido en una clinica clandestina y los organizadores del golpe se hicieron cargo de los gastos.");
	  		}
	  		case 2,3:
	  		{
		  		SetPlayerPosEx(playerid,-318.5561,1050.0988,20.3403,354.3885);
		  		Mensaje(playerid,-1,"Fuiste atendido en una clínica privada con influencias del casino.");
	  		}
		}
	}
	if(Specteando[playerid] == 1)
	{
		DarRopa(playerid);
		DarArmas(playerid);
		DarPrendas(playerid);
		SetPlayerPos(playerid,SpecX[playerid],SpecY[playerid],SpecZ[playerid]);
		SetPlayerInterior(playerid,SpecInt[playerid]);
		SetPlayerVirtualWorld(playerid,SpecVW[playerid]);
		Specteando[playerid] = 0;
		SpecJugador[playerid] = -1;
		EstablecerVida(playerid,VidaSpec[playerid]);
		EstablecerChaleco(playerid,ChalecoSpec[playerid]);
		CargandoEx(playerid);
		return 1;
	}
	return 1;
}

function OnPlayerDataLoaded(playerid, race_check)
{
	#if DEBUG_GM == 1
	printf("(OnPlayerDataLoaded) PlayerID: %d | RaceCheck: %d", playerid, race_check);
	#endif
	if(race_check != g_MysqlRaceCheck[playerid])
	{
		printf("Login(%s:%d): RC[%d] - Mysql_CK[%d]", NombreJugador(playerid), playerid, race_check, g_MysqlRaceCheck[playerid]);
		Kick(playerid);
		return 1;
	}
	SetPlayerPos(playerid, 1734.457153, -1873.322266, 21.658100);
	SetPlayerCameraPos(playerid, 1707.097290, -1873.243896, 13.568500);
	SetPlayerCameraLookAt(playerid, 1713.585083, -1883.411011, 13.566900, 2);
	if(GetPlayerScore(playerid) != 0) return ExpulsarJugador(playerid, "Posible bug");
	LimpiarChat(playerid, 20, 1);
	if(cache_num_rows() == 1) return ShowPlayerDialog(playerid, LOGIN, DIALOG_STYLE_PASSWORD,"Esta cuenta está registrada","Ingresa tu contraseña:","Enviar","Salir");
	else return ShowPlayerDialog(playerid, REGISTRO, DIALOG_STYLE_PASSWORD,"Registra una nueva cuenta","Ingresa tu contraseña:","Enviar","Salir");
}

