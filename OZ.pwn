	PlayerInfo[id][pCargos] = cargos;
	DarEstrella(id,PlayerInfo[id][pCargos]);

	if(cargos == 0) strcpy(PlayerTemp[id][pt_QuienCargos], "Nadie"), Advertido[id] = 0;
	else strcpy(PlayerTemp[id][pt_QuienCargos], NombreJugador(playerid));

   	return 1;
}

