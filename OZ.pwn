stock EnviarBus(playerid, bus)
{
	Mensaje(playerid,-1,"Estas siendo trasladado a la penitenciaria de Fort Carson.");
	SetPlayerVirtualWorld(playerid, 0);
	SetPlayerInterior(playerid, 0);
	StatusFC[playerid] = 0;
	CongelarEx(playerid);
	new asiento = random(8);
	switch(bus)
	{
