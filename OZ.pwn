public OnPlayerExitVehicle(playerid, vehicleid)
{
	#if DEBUG_GM == 1
	printf("(OnPlayerExitVehicle) PlayerID: %d | VehicleID: %d", playerid, vehicleid);
	#endif
	Saliendo[playerid] = gettime() + (1/2);//Anti super gun
	switch(Seatbelt[playerid])
	{
		case 1:
		{
			new mensajecasco[80];
			if(MENORES(vehicleid)) format(mensajecasco, sizeof(mensajecasco), "%s se quita el casco que lleva puesto.", NombreJugador(playerid));
			else format(mensajecasco, sizeof(mensajecasco), "%s se saca el cinturón de seguridad que lleva puesto.", NombreJugador(playerid));
			ProxDetector(10.0, playerid, mensajecasco, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
   			Seatbelt[playerid] = 0;
		}
	}
	if(vehicleid >= 2000)
	{
		new string[90];
		format(string, sizeof(string), "%s(ID:%d) posible cheat de vehículos (ID Vehículo: %d).", NombreJugador(playerid), playerid, vehicleid);
		MensajeAdmin(COLOR_YELLOW, string, 3);
		ExpulsarJugador(playerid, "Cheat de vehiculos (2)");
	}
	if(VehiculosInfo[vehicleid][vcomprable] == 1)
	{
		SetTimerEx("Respawnear",5000,false,"d",vehicleid);
	}
	if(COSECHA(vehicleid))
	{
		Respawnear(vehicleid);
	}
	if(EmisoraEscuchando[vehicleid] != -1)
	{
		StopAudioStreamForPlayer(playerid);
	}
	if(BajandoV[playerid] == 0)
	{
		BajandoV[playerid] = 1;
	}
	if(Taxista[playerid] != INVALID_PLAYER_ID)
	{
		new string[128];
		TextDrawHideForPlayer(playerid,TAXIMET);
		TextDrawHideForPlayer(playerid,TAXIMET2[Taxista[playerid]]);
		KillTimer(TM[playerid]);
		format(string,128,"Has pagado al conductor un total de {00CC00}$%s{FFFFFF}.",ConvertirNumero(Monto[Taxista[playerid]]));
		Mensaje(playerid,-1,string);
		format(string,128,"%s Te ha pagado {00CC00}$%s{FFFFFF} por el recorrido.",NombreJugador(playerid),ConvertirNumero(Monto[Taxista[playerid]]));
		Mensaje(Taxista[playerid],-1,string);
		Monto[Taxista[playerid]] = 0;
		Pasajero[Taxista[playerid]] = INVALID_PLAYER_ID;
		Taxista[playerid] = INVALID_PLAYER_ID;
	}
	EnAuto[playerid] = INVALID_VEHICLE_ID;
	return 1;
}

function DudaAdmin(playerid, params[])
{
	if(isnull(params)) return Mensaje(playerid, COLOR_GRAD2, "Usa {DBED15}/duda [Pregunta]{FFFFFF} para preguntar/responder a otros jugadores");
	{
		new string[300], arank[64];
		switch(PlayerInfo[playerid][jAdminNvl])
		{
			case 1: arank = " [Ayudante]";
			case 2,3,4,5:
			{
				if(PlayerInfo[playerid][jSexo] == 2) arank = " [Moderadora]";
				else arank = " [Moderador]";
			}
			case 6,7: arank = " [ADM]";
		}
		format(string, sizeof(string), "({009966}Canal /n{009999})%s %s (%d): %s", arank, NombreJugador(playerid),playerid,params[0]);
		for(new i, j = GetPlayerPoolSize(); i <= j; i++)
		{
			if(CanalDudas[i] == 1)
			{
				Mensaje(i,0x009999FF,string);
			}
		}
	}
	return 1;
}

function AnuncioUser(playerid, params[])
{
	if(!sscanf(params, "s[300]", params[0]))
	{
		new string[300];
		new s = GetSpam(playerid, params);
		if(s == 2 || s == 3)
		{
			format(string,sizeof(string),"{C02EDC}({DBED15}Bloqueado{C02EDC}) %s(%d) [N:%d]: %s", NombreJugador(playerid), playerid, PlayerInfo[playerid][jNivel], params);
			MensajeAdmin(COLOR_YELLOW, string, 3);
			TiempoAnuncio[playerid] = 300;
			ColgarLaLlamada(playerid);
			return 0;
		}

		format(string, sizeof(string), ">%s:{FFFFFF} %s {C02EDC}[#%d]", tipoanunciollama[playerid], params, PlayerInfo[playerid][pNumero]);

		for(new i, j = GetPlayerPoolSize(); i <= j; i++)
		{
			if(CanalAnuncios[i] == 1)
			{
				Mensaje(i,0xC02EDCDE,string);
			}
		}

		format(string, 300, "%s (%d)", NombreJugador(playerid), playerid);
		MensajeAdmin(COLOR_GREY, string, 2);

		TiempoAnuncio[playerid] = 300;
		ColgarLaLlamada(playerid);
	}
	return 1;
}

