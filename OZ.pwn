CMD:remolcar(playerid, params[])
{
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_GLOBAL2) return 0;
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
	{
		new Float:PosvX,Float:PosvY,Float:PosvZ,Found=0,vid=0,idcar = GetPlayerVehicleID(playerid);
		if(GetVehicleModel(idcar) == 525)
		{
			new Float:PospX,Float:PospY,Float:PospZ;
			GetPlayerPos(playerid,PospX,PospY,PospZ);
			while((vid<MAX_VEHICLES)&&(!Found))
			{
				vid++;
				GetVehiclePos(vid,PosvX,PosvY,PosvZ);
				if ((floatabs(PospX-PosvX)<7.0)&&(floatabs(PospY-PosvY)<7.0)&&(floatabs(PospZ-PosvZ)<7.0)&&(vid!=GetPlayerVehicleID(playerid)))
				{
					Found=1;
					if(IsTrailerAttachedToVehicle(GetPlayerVehicleID(playerid)))
					{
	   					DetachTrailerFromVehicle(GetPlayerVehicleID(playerid));
					}
		   			else
		   			{
	   					AttachTrailerToVehicle(vid,GetPlayerVehicleID(playerid));
					}
				}
			}
			if(!Found)
			{
				Mensaje(playerid,-1,"No hay ningún vehículo cerca!");
			}
		}
		else return Mensaje(playerid, -1, "No puedes remolcar con este vehículo!");
	}
	return 1;
}

