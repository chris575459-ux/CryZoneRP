function VerificarCorreo(playerid)
{
	if(cache_num_rows() == 0)
	{
		ShowPlayerDialog(playerid, SEXO, DIALOG_STYLE_LIST, "¿Eres hombre o mujer?", "Hombre\nMujer", "Registrar", "Salir");
	}
	else
	{
		Mensaje(playerid, COLOR_ORANGE, "Ya hay una cuenta registrada con ese email.");
		Mensaje(playerid, COLOR_ORANGE, "Para recuperar tu contraseña, ingresa a https://rol.omegazone.net/perdida.php");
		ExpulsarJugador(playerid, "Correo en uso");
	}
	return true;
}

function UpdateUltimoUsuarioForo(name[])
{
	new DB_Query[256];
	mysql_format(Conecction2, DB_Query, sizeof DB_Query,
		"\
			UPDATE `smf_settings` SET `value`=%d WHERE `variable`='latestMember';\
			UPDATE `smf_settings` SET `value`='%s' WHERE `variable`='latestRealName';\
		",
	cache_insert_id(), name);

	mysql_pquery(Conecction2, DB_Query);
}

function UsuarioRegistrado(playerid)
{
	new DBID = PlayerInfo[playerid][p_DBID] = cache_insert_id();

	new string[64];
	format(string, sizeof string, "INSERT INTO `logros` (`ID`) VALUES ('%d');", DBID);
	mysql_pquery(Conecction, string);

	new DB_Query[128];

	switch(PlayerInfo[playerid][jSexo])
	{
		case 2:
		{
			mysql_format(Conecction2, DB_Query, sizeof DB_Query, "UPDATE `smf_members` SET `avatar`='personajes/11.png',`gender`='2' WHERE `member_name`='%s'", NombreJugador(playerid));
			mysql_pquery(Conecction2, DB_Query);

			mysql_format(Conecction, DB_Query, sizeof DB_Query, "UPDATE `usuarios` SET `Skin`='11',`Sexo`='%d' WHERE `ID`='%d'",PlayerInfo[playerid][jSexo], DBID);
			mysql_pquery(Conecction, DB_Query);
		}
		default:
		{
			mysql_format(Conecction2, DB_Query, sizeof DB_Query, "UPDATE `smf_members` SET `avatar`='personajes/250.png',`gender`='1' WHERE `member_name`='%s'", NombreJugador(playerid));
			mysql_pquery(Conecction2, DB_Query);

			mysql_format(Conecction, DB_Query, sizeof DB_Query, "UPDATE `usuarios` SET `Skin`='250',`Sexo`='%d' WHERE `ID`='%d'",PlayerInfo[playerid][jSexo], DBID);
			mysql_pquery(Conecction, DB_Query);
		}
	}

	return 1;
}

