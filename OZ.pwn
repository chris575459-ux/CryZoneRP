function ServerMicrobeat()
{
	new Float:health, carid, arrDamage[4];
	foreach(new i : Player)
	{
		if(GetPlayerState(i) == PLAYER_STATE_DRIVER)
		{
			iVehicle[i] = carid = GetPlayerVehicleID(i);
			for(new s=0;s<20;s++)
			{
				if(SpikeStrips[s][sX] != 0)
				{
					if(VehicleToPoint(3.0, carid, SpikeStrips[s][sX], SpikeStrips[s][sY], SpikeStrips[s][sZ]))
					{
						GetVehicleDamageStatus(carid, arrDamage[0], arrDamage[1], arrDamage[2], arrDamage[3]);
						arrDamage[3] = encode_tires(1, 1, 1, 1);
						UpdateVehicleDamageStatus(carid, arrDamage[0], arrDamage[1], arrDamage[2], arrDamage[3]);
					}
				}
			}
			GetVehicleHealth(iVehicle[i], health);
			if(health < 375.0)
			{
				SetVehicleHealth(iVehicle[i], 375.0);
				GetVehicleParamsEx(iVehicle[i], engine, lights, alarm, doors, bonnet, boot, objective);
				SetVehicleParamsEx(iVehicle[i], 0, lights, alarm, doors, bonnet, boot, objective);
				averiado[carid] = 1;
				MotorAuto[carid] = 0;
				if(EnCarreras[i] > 0 && VehiculoCarrera[EnCarreras[i]-1] == iVehicle[i])
				{
					SacarCarrera(i, 2);
					if(ParticipantesCarrera <= 0)
					{
						SetTimer("VerEnCarreras",(1*60*1000),0);
   						ResetearCarrera();
						HabilitarCarreras = 0;
						UpdateDynamic3DTextLabelText(TextoCarrera,COLOR_BLANCO,"{FF3300}Boleteria cerrada\n{DBED15}Horarios de los eventos:{FFFFFF} Carreras a cada xx:02 horas!");
					}
					Mensaje(i,-1,"Perdiste la carrera por malograr tu vehículo.");
				}
				if(MotoCross[i] > 0 && GetVehicleModel(GetPlayerVehicleID(i)) == 468)
				{
					SacarCarrera(i, 1);
					Mensaje(i,-1,"Perdiste la carrera por malograr tu vehículo.");
				}
			}
			if(VehicleFuel[iVehicle[i]] == 0)
			{
				GameTextForPlayer(i, "~r~Sin combustible", 2500, 3);
				GetVehicleParamsEx(iVehicle[i], engine, lights, alarm, doors, bonnet, boot, objective);
				SetVehicleParamsEx(iVehicle[i], 0, lights, alarm, doors, bonnet, boot, objective);
				MotorAuto[carid] = 0;
			}
			else if(health > 375.0 && averiado[carid] == 1)
			{
				averiado[carid] = 0;
			}
			if(Manejando[i] == 1)
			{
				if(!BICI(carid) && IsPlayerInAnyVehicle(i))
				{
					new td_str[16];
					format(td_str, sizeof td_str, "%d/%d", VehicleFuel[carid], GetVehicleMaxCombustible(carid));
					PlayerTextDrawSetString(i, PlayerVelocimetro[i][ptdCombustible][0], td_str);

					new Float:barraVel = 547.000000 + floatmul(floatdiv(float(ObtenerVelocidad(i)), 100.0), 66.349976); // Velocidad maxima
					if(barraVel > 613.349976) barraVel = 613.349976;

					PlayerTextDrawTextSize(i, PlayerVelocimetro[i][ptdVelocidad][1], barraVel, 0.000000);
					PlayerTextDrawUseBox(i, PlayerVelocimetro[i][ptdVelocidad][1], (ObtenerVelocidad(i) > 0));

					format(td_str, sizeof td_str, "%d", ObtenerVelocidad(i));
					PlayerTextDrawSetString(i, PlayerVelocimetro[i][ptdVelocidad][0], td_str);

					PlayerTextDrawShow(i, PlayerVelocimetro[i][ptdVelocidad][1]);
				}
			}
		}
	}
	return true;
}

public OnPlayerStateChange(playerid, newstate, oldstate)
{
	#if DEBUG_GM == 1
	printf("(OnPlayerStateChange) PlayerID: %d | NewState: %d | OldState: %d", playerid, newstate, oldstate);
	#endif
	/*
	// Detectar estados
	new mensj[128];
	format(mensj, 128, "Tu estado actual es: %i. (Viejo: %i)", newstate, oldstate);
	Mensaje(playerid, 0xFFFFFFFF, mensj);
	*/

	if(IsPlayerNPC(playerid)) return 1;
	new autoid = GetPlayerVehicleID(playerid);
	if(oldstate == PLAYER_STATE_DRIVER && MotoCross[playerid] > 0)
	{
		SacarCarreraCross(playerid,1);
		SendClientMessage(playerid,-1,"Perdiste la carrera por que saliste de tu vehículo.");
	}
	if(newstate == PLAYER_STATE_DRIVER)
	{
		new Float:Vida;
		GetVehicleHealth(autoid,Vida);
		VidaVehiculo[autoid] = Vida;
	}
	Entrando[playerid] = 0;
	Saliendo[playerid] = 0;
	if((oldstate == PLAYER_STATE_ONFOOT && newstate == PLAYER_STATE_DRIVER) && PlayerInfo[playerid][horasjugadas] <= 6)
	{
		AreaCheat[playerid] = CreateDynamicCircle(0.0, 0.0, 30.0);
		AttachDynamicAreaToPlayer(AreaCheat[playerid], playerid);
		Streamer_SetIntData(STREAMER_TYPE_AREA, AreaCheat[playerid], E_STREAMER_EXTRA_ID, AREA_CHEAT);
		DisableRemoteVehicleCollisions(playerid, true);
	}
	if(oldstate == PLAYER_STATE_DRIVER && newstate == PLAYER_STATE_ONFOOT)
	{
		if(IsValidDynamicArea(AreaCheat[playerid])) DestroyDynamicArea(AreaCheat[playerid]);
		AreaCheat[playerid] = INVALID_STREAMER_ID;
		DisableRemoteVehicleCollisions(playerid, false);
	}
	/* anti super gun */
	if(newstate == PLAYER_STATE_DRIVER && Entrando[playerid] > gettime() && !BOTE(GetPlayerVehicleID(playerid)) && !BICI(GetPlayerVehicleID(playerid)))
	{
		new string[200];
		format(string,sizeof(string),"%s(ID:%d) posible cheater, super gun (1). (Nivel:%d - %dhs)",NombreJugador(playerid),playerid,PlayerInfo[playerid][jNivel],PlayerInfo[playerid][horasjugadas]);
		MensajeAdmin(COLOR_YELLOW, string, 3);
		ExpulsarJugador(playerid, "Posible cheater de super gun (1)");
	}
	if(newstate == PLAYER_STATE_DRIVER)
	{
		KillTimer(TimerSiEntro[playerid]);
	}
	if(newstate == PLAYER_STATE_DRIVER || newstate == PLAYER_STATE_PASSENGER)
	{
		foreach(new i : Player)
 		{
 			if(SpecJugador[i] == playerid) PlayerSpectateVehicle(i, GetPlayerVehicleID(playerid));
		}
	}
	if(newstate == PLAYER_STATE_ONFOOT)
	{
		foreach(new i : Player)
   		{
			if(SpecJugador[i] == playerid) PlayerSpectatePlayer(i, playerid);
		}
		EntrandoAV[playerid] = -1;
	}
	if(newstate == PLAYER_STATE_DRIVER || newstate == PLAYER_STATE_PASSENGER)
	{
		if(EmisoraEscuchando[GetPlayerVehicleID(playerid)] != -1)
		{
			new emisoraid;
			emisoraid = EmisoraEscuchando[GetPlayerVehicleID(playerid)];
			for(new id, j = GetPlayerPoolSize(); id <= j; id++)
			{
				if(Emisoras[id][CabinaOcupada] == emisoraid)
				{
					if(Emisoras[id][TransEmisora] == 1)
					{
						new stringa[128];
						format(stringa, sizeof(stringa), "{FFFFFF}Escuchando radio {DBED15}%s{FFFFFF}, para cambiar de emisora usa {DBED15}/emisoras", Emisoras[id][NombreEmisora]);
						Mensaje(playerid, COLOR_BLANCO, stringa);
						format(stringa, sizeof(stringa),"http://radio.omegazone.net:8000/listen.pls?sid=%d", Emisoras[id][EmisoraID]);
						if(ShowSound[playerid] == 1) PlayAudioStreamForPlayer(playerid, stringa);
					}
				}
			}
		}
	}
	if(newstate == PLAYER_STATE_ONFOOT && oldstate == PLAYER_STATE_DRIVER)
	{
		if(EnCarreras[playerid] > 0 && EmpezoCarrera == 1)
 		{
	  		DisablePlayerRaceCheckpoint(playerid);
			SetPlayerPos(playerid,-1405.2959,-309.6738,1052.0900);
			SetPlayerFacingAngle(playerid,190.0000);

			TextDrawHideForPlayer(playerid, CarreraT[0]);
			TextDrawHideForPlayer(playerid, CarreraT[1]);
			TextDrawHideForPlayer(playerid, CarreraT[2]);
			TextDrawHideForPlayer(playerid, CarreraT2[playerid][3]);
			TextDrawHideForPlayer(playerid, CarreraT[4]);
			TextDrawHideForPlayer(playerid, CarreraT2[playerid][5]);
			TextDrawHideForPlayer(playerid, CarreraT[6]);
			TextDrawHideForPlayer(playerid, CarreraT2[playerid][7]);
			TextDrawHideForPlayer(playerid, CarreraT[7]);

			SacarCarrera(playerid, 2);
			if(ParticipantesCarrera <= 0)
			{
				SetTimer("VerEnCarreras",(1*60*1000),0);
   				ResetearCarrera();
				HabilitarCarreras = 0;
				UpdateDynamic3DTextLabelText(TextoCarrera,COLOR_BLANCO,"{FF3300}Boleteria cerrada\n{DBED15}Horarios de los eventos:{FFFFFF} Carreras a cada xx:02 horas!");
			}
			Mensaje(playerid,-1,"Perdiste la carrera por salir de tu vehículo.");
		}
	}
	if(newstate == PLAYER_STATE_PASSENGER)
	{
		if(PlayerInfo[playerid][Agonizando] == 1 && PacienteAmbu[playerid] == 0)
		{
			RemovePlayerFromVehicle(playerid);
		}
		//SetPlayerArmedWeapon(playerid, 0);
		if(Objeto[playerid] > 1100)
		{
			Mensaje(playerid,-1,"No puedes subir a un vehículo con un objeto, usa {DBED15}/tirar objeto.");
			RemovePlayerFromVehicle(playerid);
		}
		if(GetPlayerWeapon(playerid) == 0 || GetPlayerWeapon(playerid) == 29 || GetPlayerWeapon(playerid) == 25 || GetPlayerWeapon(playerid) == 31 || GetPlayerWeapon(playerid) == 30) { }
		else
		{
			if((PlayerInfo[playerid][pWP3] == 25 || PlayerInfo[playerid][pWP4] == 29 || PlayerInfo[playerid][pWP5] == 30 ||PlayerInfo[playerid][pWP5] == 31) && AceptacionBlindado[playerid] != 2) DarArmas(playerid);
			else
			{
				Manejando[playerid] = 1;
				if(AceptacionBlindado[playerid] != 2) ResetPlayerWeapons(playerid);
			}
		}
	}
	if(oldstate == PLAYER_STATE_PASSENGER)
	{
		if(Manejando[playerid] == 1)
		{
			if(AceptacionBlindado[playerid] != 2) DarArmas(playerid);
			Manejando[playerid] = 0;
		}
	}
	if(newstate == PLAYER_STATE_DRIVER || newstate == PLAYER_STATE_ENTER_VEHICLE_DRIVER)
	{
		EnAuto[playerid] = autoid;
		if(PlayerInfo[playerid][Agonizando] == 1 && PacienteAmbu[playerid] == 0) RemovePlayerFromVehicle(playerid);
		SetPlayerArmedWeapon(playerid, 0);
		if(Objeto[playerid] > 1100)
		{
			Mensaje(playerid,-1,"No puedes subir a un vehículo con un objeto, usa {DBED15}/tirar objeto.");
			RemovePlayerFromVehicle(playerid);
		}
		// Alarma
		if((autos1[playerid] == autoid || autos2[playerid] == autoid || autos3[playerid] == autoid || autos4[playerid] == autoid) && TestSodas[playerid] == 0) Mensaje(playerid, -1, "Este vehículo te pertenece.");
		else
		{
			if(alarma[autoid] == 1 && alarmaon[autoid] == 0)
			{
				GetVehicleParamsEx(autoid,engine,lights,alarm,doors,bonnet,boot,objective);
				SetVehicleParamsEx(autoid,engine,lights,true,doors,bonnet,boot,objective);
				SetTimerEx("DesactivarAlarma", minutos(1), false, "d", autoid);
				TimerLuces[autoid] = SetTimerEx("TimerLucesIntermitentes", 500, false, "d", autoid);
				alarmaon[autoid] = 1;
			}
		}
		if(!BICI(autoid) && MotorAuto[autoid] == 0 && TestSodas[playerid] == 0)
		{
			if(VehiculosInfo[autoid][vcomprable] != 1) Mensaje(playerid,-1,"Para encender el motor del vehículo, utiliza {DBED15}/arrancar{FFFFFF} o pulsa {DBED15}'2'{FFFFFF}.");
		}
		if(EmisoraEscuchando[autoid] == -1 && TestSodas[playerid] == 0)
		{
			Mensaje(playerid, COLOR_BLANCO, "Para escuchar alguna emisora de radio de San Andreas, usa {D9ED15}/emisoras");
		}
		if(VehiculosInfo[autoid][vcomprable] == 1)
		{
            new item[500];
			format(item, sizeof(item), "{FFFFFF}Nombre: {DBED15}%s{FFFFFF}\n",							GetVehicleName(autoid));
			format(item, sizeof(item), "%sEspacios del baúl: {00CC00}%d{FFFFFF}\n",item,				ObtenerMaletero(VehiculosInfo[autoid][vmodelo]));
			format(item, sizeof(item), "%sVelocidad máxima: {FF3300}%d km/h{FFFFFF}\n",item,			VelocidadMaxima(VehiculosInfo[autoid][vmodelo]));
			format(item, sizeof(item), "%sCapacidad de combustible: {00CCFF}%d Litros{FFFFFF}\n",item,	GetVehicleMaxCombustible(autoid));
			format(item, sizeof(item), "%sCosto del seguro: {FF3300}$%s{FFFFFF}\n",item,				ConvertirNumero(VehiculosInfo[autoid][vprecio]/100));
			format(item, sizeof(item), "%sCosto de la grua: {FF3300}$%s{FFFFFF}\n",item,				ConvertirNumero(VehiculosInfo[autoid][vprecio]/100));
			format(item, sizeof(item), "%sCosto de la traba: {FF3300}$%s{FFFFFF}\n",item,				ConvertirNumero(((VehiculosInfo[autoid][vprecio] / 2) / 10) + 2000));
			format(item, sizeof(item), "%sCosto de la alarma: {FF3300}$%s{FFFFFF}\n",item,				ConvertirNumero(((VehiculosInfo[autoid][vprecio] / 2) / 10) + 5000));
			format(item, sizeof(item), "%sRepuestos: {00CCFF}%s{FFFFFF}\n",item,						ConvertirNumero(VehiculosInfo[autoid][vprecio]/5000));
			if(VehiculosInfo[autoid][vmoneda] == 0) format(item, sizeof(item), "%sPrecio: {00CC00}$%s{FFFFFF}",item,			ConvertirNumero(VehiculosInfo[autoid][vprecio]));
			else if(VehiculosInfo[autoid][vmoneda] == 1) format(item, sizeof(item), "%sPrecio: {00CC00}%s Oz{FFFFFF}",item,		ConvertirNumero(VehiculosInfo[autoid][vprecio]));
			ShowPlayerDialog(playerid, COMPRARAUTO, DIALOG_STYLE_MSGBOX, "Detalles del vehículo", item, "Comprar", "Salir");
		}
		new veh = GetPlayerVehicleID(playerid);
		if(JugadorLogeado[playerid] == 1)
		{
			if(BajandoV[playerid] == 1)
			{
				new string[128];
				if(PlayerInfo[playerid][jAdminNvl] >= ADMIN_LVL_JUEGO2) return 1;
				if(EnCarreras[playerid] > 0) return 1;
				if(MOTO(veh) && GetPlayerState(playerid) == PLAYER_STATE_PASSENGER) return 1;
				format(string,sizeof(string),"%s(ID:%d) posible cheater, sube sin animación a los vehículos (2) (Nivel:%d - %dhs)",NombreJugador(playerid),playerid,PlayerInfo[playerid][jNivel],PlayerInfo[playerid][horasjugadas]);
				for(new i, j = GetPlayerPoolSize() ;i <= j;i++)
				{
					if(PlayerInfo[i][jAdminNvl] >= ADMIN_LVL_JUEGO1 && VerAdmin[i] == 1) Mensaje(i,COLOR_YELLOW,string);
				}
				ExpulsarJugador(playerid, "Cheat de vehiculos (1)");
			}
		}
		iVehicle[playerid] = GetPlayerVehicleID(playerid);
		if(TAXI(autoid))
		{
			EnTaxi[playerid] = 1;
			TextDrawShowForPlayer(playerid,TAXIMET);
			TextDrawShowForPlayer(playerid,TAXIMET2[playerid]);
		}
		if(BICI(iVehicle[playerid]))
		{
			Manejando[playerid] = 1;
			//ResetPlayerWeapons(playerid);
			KillTimer(Bajando[playerid]);
			return 1;
		}
		MostrarVelocimetro(playerid);
	}
	if(oldstate == PLAYER_STATE_DRIVER)
	{
		EnAuto[playerid] = INVALID_VEHICLE_ID;
		if(EnTaxi[playerid] == 1)
		{
			EnTaxi[playerid] = 0;
			TextDrawHideForPlayer(playerid,TAXIMET);
			TextDrawHideForPlayer(playerid,TAXIMET2[playerid]);
		}
		QuitarVelocimetro(playerid);
		//DarArmas(playerid);
		Bajando[playerid] = SetTimerEx("Maneja",5000,false,"d",playerid);
	}
	return 1;
}

