function OnPlayerHealthChange(playerid,Float:newhealth,Float:oldheatlh)
{
	if(IsPlayerNPC(playerid)) return true;
	if(GetPlayerState(playerid) != PLAYER_STATE_WASTED && GetPlayerState(playerid) != PLAYER_STATE_SPAWNED)
	{
		if(VidaJugador[playerid] < newhealth && TiempoVida[playerid] < gettime())
		{
			new string[250];
			format(string,sizeof(string),"Posible cheat de vida {FF3300}%s(ID:%d - N: %d){a9c4e4} - Diferencia: %0.f.",NombreJugador(playerid),playerid,GetPlayerScore(playerid),(VidaJugador[playerid] - newhealth)*-1);
			MensajeAdmin(COLOR_SERVER, string, 3);
		}
	}
	VidaJugador[playerid] = newhealth;
	return true;
}

