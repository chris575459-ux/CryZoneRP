stock IsReductor(weaponid,Float: damage)
{
	switch(weaponid)
	{
		case 22, 29:
		{
			if(damage < 8.0) return 1;
		}
		case 23:
		{
			if(damage < 13.0) return 1;
		}
		case 24:
		{
			if(damage < 40.0) return 1;
		}
		case 30, 31:
		{
			if(damage < 9.0) return 1;
		}
		case 33:
		{
			if(damage < 20.0) return 1;
		}
		case 34:
		{
			if(damage < 41.0) return 1;
		}
	}
	return 0;
}

stock procesarCheckpoint(const playerid)
{
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER && BASU(GetPlayerVehicleID(playerid)))
	{
		if(PlayerTemp[playerid][pt_RolBasurero] == 1)
		{
			Mensaje(playerid, -1, "Tu trabajo es recolectar basura.");
			return 0;
		}
		SetTimerEx("PuntoB", 500, false, "d", Companero[playerid]);
		Mensaje(playerid, -1, "Espera a que tu compañero recolecte la basura.");
		DisablePlayerCheckpoint(playerid);
		PuntoBaX[playerid] = 0;
		return 1;
	}
	if(!IsPlayerInAnyVehicle(playerid) && RecorridoBasu[playerid] != 0)
	{
		if(PlayerTemp[playerid][pt_RolBasurero] == 2)
		{
			Mensaje(playerid, -1, "Tu trabajo es manejar.");
			return 0;
		}
		recogiendo[playerid]++;
		DisablePlayerCheckpoint(playerid);
		PuntoBaX[playerid] = 0;
		switch(PlayerInfo[playerid][pNivelBasurero])
		{
			case 0..1:
			{
				LoopingAnim(playerid, "BOMBER", "BOM_Plant", 4.0, 1, 1, 1, 1, 13000);
				SetTimerEx("Recolectar", 13000, false, "d", playerid);
				GameTextForPlayer(playerid, "~g~RECOLECTANDO BASURA", 13000, 3);
			}
			case 2:
			{
				LoopingAnim(playerid, "BOMBER", "BOM_Plant", 4.0, 1, 1, 1, 1, 11000);
				SetTimerEx("Recolectar", 11000, false, "d", playerid);
				GameTextForPlayer(playerid, "~g~RECOLECTANDO BASURA", 11000, 3);
			}
			case 3:
			{
				LoopingAnim(playerid, "BOMBER", "BOM_Plant", 4.0, 1, 1, 1, 1, 9000);
				SetTimerEx("Recolectar", 9000, false, "d", playerid);
				GameTextForPlayer(playerid, "~g~RECOLECTANDO BASURA", 9000, 3);
			}
			case 4:
			{
