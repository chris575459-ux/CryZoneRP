			PlayerTextDrawFont(playerid, jugador[playerid], 4);
			PlayerTextDrawShow(playerid, jugador[playerid]);
			asdx[playerid] = SetTimerEx("modificarpunto", 100, true, "i", playerid);
			if(PlayerInfo[playerid][VIP] == 0) Mensaje(playerid, -1, "Usa {DBED15}/ocultar gps{FFFFFF} para quitar el mapa.");
			else Mensaje(playerid, -1, "Usa {DBED15}/ocultar gps{FFFFFF} o el botón {DBED15}'N'{FFFFFF} para quitar el mapa.");
