				SetPlayerCheckpoint(playerid, 2786.7866,-2594.8406,-1.0248, 7.0);
			}
			else return Mensaje(playerid, -1,"No tienes un recorrido de pescador activo.");
		}
		if(strcmp(item, "borrar", true) == 0)
		{
			DisablePlayerCheckpoint(playerid);
		}
	}
	return 1;
}

