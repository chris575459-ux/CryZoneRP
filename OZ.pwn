CMD:luces(playerid,params[])
{
	new string[256],autoid;
	autoid = GetPlayerVehicleID(playerid);
	if(Luces[autoid] == 0)
	{
		new vehicleid = GetPlayerVehicleID(playerid);
		if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return Mensaje(playerid, COLORCARGO0, "¡No estás dentro del vehículo!");
		GetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);
		SetVehicleParamsEx(vehicleid, engine, 1, alarm, doors, bonnet, boot, objective);
		Luces[autoid] = 1;
		Mensaje(playerid, COLORCARGO0, "Encendiendo luces...");
		format(string, 256,"%s enciende las luces de su vehículo.",NombreJugador(playerid));
		ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
	}
	else if(Luces[autoid] == 1)
	{
		new vehicleid = GetPlayerVehicleID(playerid);
		if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return Mensaje(playerid, COLORCARGO0, "¡No estás dentro del vehículo!");
		GetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);
		SetVehicleParamsEx(vehicleid, engine, 0, alarm, doors, bonnet, boot, objective);
		Mensaje(playerid, COLORCARGO0, "Apagando luces...");
		format(string, 256,"%s apaga las luces de su vehículo.",NombreJugador(playerid));
		ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
		Luces[autoid] = 0;
	}
	return 1;
}

CMD:cerrar(playerid,params[])
{
	new vehch = GetSAPDVehicle(playerid);
	new item[10];
	new Float:x,Float:y,Float:z, Float:x2,Float:y2,Float:z2, Float:x3,Float:y3,Float:z3, Float:x4,Float:y4,Float:z4, Float:x5,Float:y5,Float:z5;
	GetVehiclePos(autos1[playerid],  x,y,z);
	GetVehiclePos(autos2[playerid], x2,y2,z2);
	GetVehiclePos(autos3[playerid], x3,y3,z3);
	GetVehiclePos(autos4[playerid], x4,y4,z4);
	GetVehiclePos(vehch, x5,y5,z5);
	if(!sscanf(params,"s[10]",item))
	{
		if(strcmp(item,"patrulla",true)==0)
		{
			if(PlayerInfo[playerid][pDuty] != 1) return 1;
			new Float:VPos[3];
			for(new i, j = GetVehiclePoolSize(); i <= j; i++)
			{
				if(SAPD(i))
				{
					new data[128];
					GetVehiclePos(i,VPos[0],VPos[1],VPos[2]);
					if(EstaEn(playerid,2.0,VPos[0],VPos[1],VPos[2]))
					{
						if(bloqueado[i] == 1) return Mensaje(playerid,-1,"Esta patrulla ya esta cerrada.");
						format(data, sizeof(data),"%s trabó las puertas de su patrullero.", NombreJugador(playerid));
						ProxDetector(10.0, playerid, data, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
						GetVehicleParamsEx(i, engine, lights, alarm, doors, bonnet, boot, objective);
						SetVehicleParamsEx(i, engine, lights, alarm, 1, bonnet, boot, objective);
						bloqueado[i] = 1;
					}
				}
			}
		}
		if(strcmp(item,"baul",true)==0 || strcmp(item,"maletero",true)==0)
		{
			new string[256];
			if(EstaEn(playerid, 4.0, x,y,z))
			{

				if(Baul[autos1[playerid]] == 0) return Mensaje(playerid,-1,"El baul ya esta cerrado.");
				GetVehicleParamsEx(autos1[playerid], engine, lights, alarm, doors, bonnet, boot, objective);
				SetVehicleParamsEx(autos1[playerid], engine, lights, alarm, doors, bonnet, 0, objective);
				Baul[autos1[playerid]] = 0;
				format(string, 256,"%s cierra el maletero de su vehículo.", NombreJugador(playerid));
				ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
				return 1;
			}
			else if(EstaEn(playerid, 4.0, x2,y2,z2))
			{
				if(Baul[autos2[playerid]] == 0) return Mensaje(playerid,-1,"El baul ya esta cerrado.");
				GetVehicleParamsEx(autos2[playerid], engine, lights, alarm, doors, bonnet, boot, objective);
				SetVehicleParamsEx(autos2[playerid], engine, lights, alarm, doors, bonnet, 0, objective);
				Baul[autos2[playerid]] = 0;
				format(string, 256,"%s cierra el maletero de su vehículo.", NombreJugador(playerid));
				ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
				return 1;
			}
			else if(EstaEn(playerid, 4.0, x3,y3,z3))
			{
				if(Baul[autos3[playerid]] == 0) return Mensaje(playerid,-1,"El baul ya esta cerrado.");
				GetVehicleParamsEx(autos3[playerid], engine, lights, alarm, doors, bonnet, boot, objective);
				SetVehicleParamsEx(autos3[playerid], engine, lights, alarm, doors, bonnet, 0, objective);
				Baul[autos3[playerid]] = 0;
				format(string, 256,"%s cierra el maletero de su vehículo.", NombreJugador(playerid));
				ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
				return 1;
			}
			else if(EstaEn(playerid, 4.0, x4,y4,z4))
			{
				if(Baul[autos4[playerid]] == 0) return Mensaje(playerid,-1,"El baul ya esta cerrado.");
				GetVehicleParamsEx(autos4[playerid], engine, lights, alarm, doors, bonnet, boot, objective);
				SetVehicleParamsEx(autos4[playerid], engine, lights, alarm, doors, bonnet, 0, objective);
				Baul[autos4[playerid]] = 0;
				format(string, 256,"%s cierra el maletero de su vehículo.", NombreJugador(playerid));
				ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
				return 1;
			}
			else if(EstaEn(playerid, 4.0, x5,y5,z5))
			{
		 		if(PlayerInfo[playerid][pDuty] == 1)
				{
					if(Baul[vehch] == 0) return Mensaje(playerid,-1,"El baul ya esta cerrado.");
					GetVehicleParamsEx(vehch, engine, lights, alarm, doors, bonnet, boot, objective);
					SetVehicleParamsEx(vehch, engine, lights, alarm, doors, bonnet, 0, objective);
					Baul[vehch] = 0;
					format(string, 256,"%s cierra el maletero del vehículo policial.", NombreJugador(playerid));
					ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
				}
				return 1;
			}
			else return Mensaje(playerid, -1,"Debes estar cerca de tu vehículo para cerrar su baul.");
		}
		if(strcmp(item,"capo",true)==0)
		{
			if(!IsPlayerInAnyVehicle(playerid)) return Mensaje(playerid,-1,"Debes estar conduciendo un vehículo para abrir su capó.");
			if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
			{
				new string[256],autoid = GetPlayerVehicleID(playerid);
				GetVehicleParamsEx(autoid, engine, lights, alarm, doors, bonnet, boot, objective);
				SetVehicleParamsEx(autoid, engine, lights, alarm, doors, 0, boot, objective);
				Capo[autoid] = 0;
				format(string, 256,"%s cierra el capó del vehículo.", NombreJugador(playerid));
				ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
				return 1;
			}
			else return Mensaje(playerid,-1,"Debes estar conduciendo un vehículo para abrir su capo.");
		}
		if(strcmp(item,"ropero",true) == 0)
		{
			for(new p = 1; p < MAX_PROPIEDADES; p++)
			{
				if(PropInfo[p][pEnUso] == 1)
				{
					if(GetPlayerVirtualWorld(playerid) == PropInfo[p][pVW])
					{
						if(PlayerInfo[playerid][p_DBID] == PropInfo[p][pIDPropietario])
						{
							if(EstaEn(playerid,1.5,254.2685,-41.5420,1002.0308) || EstaEn(playerid,5,PropInfo[p][pRoperoX],PropInfo[p][pRoperoY],PropInfo[p][pRoperoZ]))
							{
								if(PropInfo[p][pRoperoAbierto] == 1)
								{
									new string[250];
									PropInfo[p][pRoperoAbierto] = 0;
									format(string,sizeof(string),"%s cierra el ropero.",NombreJugador(playerid));
									ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
									GuardarPropiedad(p);
								}
								else Mensaje(playerid,-1,"El ropero ya está cerrado, usa {DBED15}/abrir ropero{FFFFFF} para abrirlo.");
							}
						}
					}
				}
			}
		}
		if(strcmp(item,"casa",true) == 0 || strcmp(item,"puerta",true) == 0)
		{
			for(new p = 1; p < MAX_PROPIEDADES; p++)
			{
				if(PropInfo[p][pEnUso] == 1)
				{
					if(PlayerInfo[playerid][p_DBID] == PropInfo[p][pIDPropietario])
					{
						if(GetPlayerVirtualWorld(playerid) == PropInfo[p][pVW] && GetPlayerInterior(playerid) == PropInfo[p][pInteriorID])
						{
							if(PropInfo[p][pAbierta] == 1)
							{
								new string[250];
								PropInfo[p][pAbierta] = 0;
								format(string,sizeof(string),"%s cierra la puerta de su propiedad.",NombreJugador(playerid));
								ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
								BorrarLabelCasa(p);
								GuardarPropiedad(p);
								CargarPropiedad(p);
							}
							else Mensaje(playerid,-1,"Esta propiedad ya está cerrada, usa {DBED15}/abrir puerta{FFFFFF} para abrirla.");
						}
					}
				}
			}
		}
		return true;
	}
	if(sscanf(params, "s[10]", item))
	{
		if(EstaEn(playerid, 10.0, x,y,z))
		{
			if(PlayerInfo[playerid][vTraba] == 0) return Mensaje(playerid, COLOR_BLANCO, "Para poder cerrar el vehículo, necesitas instalar un sistema de traba. Puedes comprar uno en cualquier {DBED15}24/7");
			if(bloqueado[autos1[playerid]] == 0) CerrarAuto1(playerid);
			else Mensaje(playerid, COLOR_BLANCO, "Este vehículo ya está cerrado.");
		}
		if(EstaEn(playerid, 10.0, x2,y2,z2))
		{
			if(PlayerInfo[playerid][v2Traba] == 0) return Mensaje(playerid, COLOR_BLANCO, "Para poder cerrar el vehículo, necesitas instalar un sistema de traba. Puedes comprar uno en cualquier {DBED15}24/7");
			if(bloqueado[autos2[playerid]] == 0) CerrarAuto2(playerid);
			else Mensaje(playerid, COLOR_BLANCO, "Este vehículo ya está cerrado.");
		}
		if(EstaEn(playerid, 10.0, x3,y3,z3))
		{
			if(PlayerInfo[playerid][v3Traba] == 0) return Mensaje(playerid, COLOR_BLANCO, "Para poder cerrar el vehículo, necesitas instalar un sistema de traba. Puedes comprar uno en cualquier {DBED15}24/7");
			if(bloqueado[autos3[playerid]] == 0) CerrarAuto3(playerid);
			else Mensaje(playerid, COLOR_BLANCO, "Este vehículo ya está cerrado.");
		}
		if(EstaEn(playerid, 10.0, x4,y4,z4))
		{
			if(PlayerInfo[playerid][v4Traba] == 0) return Mensaje(playerid, COLOR_BLANCO, "Para poder cerrar el vehículo, necesitas instalar un sistema de traba. Puedes comprar uno en cualquier {DBED15}24/7");
			if(bloqueado[autos4[playerid]] == 0) CerrarAuto4(playerid);
			else Mensaje(playerid, COLOR_BLANCO, "Este vehículo ya está cerrado.");
		}
		return 1;
		/*else
		{
			Mensaje(playerid, COLOR_BLANCO, "Tienes que estar cerca o dentro de tu vehículo para {DBED15}/cerrar");
		}*/
	}
	return 1;
}

CMD:alarma(playerid,params[])
{
	new Float:x, Float:y, Float:z, Float:x2, Float:y2, Float:z2, Float:x3, Float:y3, Float:z3, Float:x4, Float:y4, Float:z4;
	GetVehiclePos(autos1[playerid],  x,y,z);
	GetVehiclePos(autos2[playerid], x2,y2,z2);
	GetVehiclePos(autos3[playerid], x3,y3,z3);
	GetVehiclePos(autos4[playerid], x4,y4,z4);
	new data[80];
	if(EstaEn(playerid, 10.0, x,y,z))
	{
		if(PlayerInfo[playerid][vAlarm] == 0) return Mensaje(playerid, COLOR_BLANCO, "Tu vehículo no tiene instalado el sistema de alarma. Puedes comprar uno en cualquier {DBED15}24/7");
		if(PlayerInfo[playerid][vAlarma] == 0)
		{
			format(data, sizeof(data),"%s activa la alarma de su vehículo.", NombreJugador(playerid));
			ProxDetector(10.0, playerid, data, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
			PlayerInfo[playerid][vAlarma] = 1;
			alarma[autos1[playerid]] = 1;
		}
		else
		{
			format(data, sizeof(data),"%s desactiva la alarma de su vehículo.", NombreJugador(playerid));
