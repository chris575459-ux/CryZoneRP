		if(YaEstaAbierta[29] == 0)
		{
			MoveDynamicObject(PuertaAcademia,634.01001,-287.26999,1275.82996,2.5,0.00000,0.00000,207.00000);
			YaEstaAbierta[29] = 1;
		}
		else
		{
			MoveDynamicObject(PuertaAcademia,634.01001,-287.26999,1275.82996,2.5,0.00000,0.00000,90.00000);
			YaEstaAbierta[29] = 0;
		}
		return 1;
	}
	else Mensaje(playerid, COLOR_GREY, "No eres SAPD.");
	return 1;
