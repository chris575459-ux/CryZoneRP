	if(VehiculosInfo[carid][vcomprable] != 1) return 1;
	if(PlayerInfo[playerid][vModelo] != 0 && PlayerInfo[playerid][vModelo2] != 0 && PlayerInfo[playerid][VIP] != 1) return Mensaje(playerid, -1, "Tu ya tienes 2 vehículos, compra una membresía VIP para poder comprar hasta 4 vehículos.");
