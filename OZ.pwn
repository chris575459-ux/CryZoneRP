stock EnviarSMS(playerid, numero, text[256])
{
	if(PlayerInfo[playerid][pNumero] == numero) return Mensaje(playerid, COLORCARGO0, "No puedes enviarte un SMS a ti mismo.");
	foreach(new i : Player)
	{
		if(IsPlayerConnected(i) && PlayerInfo[i][pNumero] == numero && numero != 0)
		{
			if(TelefonoEstado[i] == 1)
			{
				Mensaje(playerid, COLORCARGO0, "El mensaje {FF4202}no se pudo entregar{FFFFFF}, comprueba que el número sea el correcto.");
				Mensaje(playerid, COLORCARGO0, "Si el número está bien, probablemente el teléfono esté apagado.");
				return 1;
			}
			if(PlayerInfo[i][pTiempoCarcel] > 0 || PlayerInfo[i][pTiempoJail] > 0)
			{
				Mensaje(playerid, COLORCARGO0, "El mensaje {FF4202}no se pudo entregar{FFFFFF}, comprueba que el número sea el correcto.");
				Mensaje(playerid, COLORCARGO0, "Si el número está bien, probablemente el teléfono esté apagado.");
				return 1;
			}
			MensajeEx(i, COLOR_SMS, "((SMS de %s [%d])): %s", NombreJugador(playerid),PlayerInfo[playerid][pNumero], text);
			Mensaje(playerid, COLOR_DORADO, "Mensaje enviado con éxito.");
			GameTextForPlayer(playerid, "~r~-15$", 5000, 1);
			if(PlayerTemp[playerid][pt_NumeroSMS] == 0 || PlayerTemp[playerid][pt_NumeroSMS] != numero)
			{
				PlayerTemp[playerid][pt_NumeroSMS] = numero;
				MensajeEx(playerid, COLORCARGO0, "Para volver a enviar otro mensaje al número {00CCFF}%d {FFFFFF}simplemente usa {DBED15}/sms mensaje{FFFFFF}.",numero);
			}
			PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
			QuitarDinero(playerid, 15);
			return 1;
		}
	}
	Mensaje(playerid, COLORCARGO0, "El mensaje {FF4202}no se pudo entregar{FFFFFF}, comprueba que el número sea el correcto.");
	Mensaje(playerid, COLORCARGO0, "Si el número está bien, probablemente el teléfono esté apagado.");
	return 1;
}

