public OnPlayerLeaveDynamicArea(playerid, areaid)
{
	if((IsPlayerInVehicle(playerid, NPC_Tren[0]) || IsPlayerInVehicle(playerid, NPC_Tren[1]) || IsPlayerInVehicle(playerid, NPC_Tren[2]) || IsPlayerInVehicle(playerid, NPC_Tren[3])) && GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
	{
	    for(new i; i < sizeof(BarrerasTren); i++)
	    {
			if(areaid == BarrerasZona[i])
			{
			    new Float:pos[2];
			    if(BarrerasTren[i][4] < 0) pos[0] = -30;
			    else pos[0] = 30;
			    if(BarrerasTren[i][10] < 0) pos[1] = -30;
			    else pos[1] = 30;
			    MoveDynamicObject(ObjetoBarreras[i][0], BarrerasTren[i][0], BarrerasTren[i][1], BarrerasTren[i][2]+0.0001, 0.000100, BarrerasTren[i][3], pos[0], BarrerasTren[i][5]);
                MoveDynamicObject(ObjetoBarreras[i][1], BarrerasTren[i][6], BarrerasTren[i][7], BarrerasTren[i][8]+0.0001, 0.000100, BarrerasTren[i][9], pos[1], BarrerasTren[i][11]);
				foreach(new p : Player)
				{
				    if(!IsPlayerInRangeOfPoint(p, 50, BarrerasTren[i][0], BarrerasTren[i][1], BarrerasTren[i][2])) continue;
					StopAudioStreamForPlayer(p);
				}
			    break;
			}
		}
	}
	if(IsPlayerNPC(playerid)) return 1;
	//printf("OnPlayerLeaveDynamicArea | ID: %d | A_ID: %d", playerid, areaid);
	new bool:adentro;
	if(Streamer_GetIntData(STREAMER_TYPE_AREA, areaid, E_STREAMER_EXTRA_ID) == AREA_CHEAT)
	{
		for(new i, j = GetPlayerPoolSize(); i <= j; i++)
		{
			if(IsPlayerInDynamicArea(playerid, AreaCheat[i]))
			{
				adentro = true;
				break;
			}
		}
		if(adentro) DisableRemoteVehicleCollisions(playerid, true);
		else DisableRemoteVehicleCollisions(playerid, false);
	}
