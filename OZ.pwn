CMD:vc(playerid,params[])
{
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_GLOBAL2) return 0;
	new id,comprable,string[70];
	if(!sscanf(params,"dd",id,comprable))
	{
		if(PropInfo[id][pEnUso] == 1)
		{
			if(comprable < 0 || comprable > 1) return Mensaje(playerid,-1,"Ese comprable es inválido.");
			PropInfo[id][pEnVenta] = comprable;
			if(comprable == 0)
			{
				PropInfo[id][pAbierta] = 0;
			}
			if(comprable == 1)
			{
				PropInfo[id][pAbierta] = 1;
				PropInfo[id][pRoperoAbierto] = 0;
				PropInfo[id][pIDPropietario] = 0;

				for(new i, j = GetPlayerPoolSize() ;i <= j; i++)
				{
					if(id == PlayerInfo[i][CasaID])
					{
						PlayerInfo[i][CasaID] = 0;
					}
					if(id == PlayerInfo[i][CasaID2])
					{
						PlayerInfo[i][CasaID2] = 0;
					}
				}

				new Query[100];

				mysql_format(Conecction, Query, sizeof(Query),"UPDATE `usuarios` SET `CasaID`='0' WHERE `CasaID`='%d'", id);
				mysql_pquery(Conecction, Query);

				mysql_format(Conecction, Query, sizeof(Query),"UPDATE `usuarios` SET `CasaID2`='0' WHERE `CasaID2`='%d'", id);
				mysql_pquery(Conecction, Query);

				mysql_format(Conecction, Query, sizeof(Query),"UPDATE `propiedades` SET `Propietario`='Nadie',`IDPropietario`='0',`EnVenta`='1' WHERE `ID`='%d'", id);
				mysql_pquery(Conecction, Query);
			}
			BorrarLabelCasa(id);
			GuardarPropiedad(id);
			CargarPropiedad(id);
			format(string,sizeof(string),"Pusiste en venta la casa con id: {DBED15}%d", id);
			Mensaje(playerid,-1,string);
		}
		else Mensaje(playerid,-1,"Esa propiedad no existe.");
	}
	else Mensaje(playerid,-1,"Por favor, usa {DBED15}/vc [ID] [comprable(0-1)]");
	return true;
}

