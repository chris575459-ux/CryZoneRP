public OnVehicleRespray(playerid, vehicleid, color1, color2)
{
	#if DEBUG_GM == 1
	printf("(OnVehicleRespray) PlayerID: %d | VehicleID: %d", playerid, vehicleid);
	#endif
	if(IsPlayerInVehicle(playerid, autos1[playerid]))
	{
		new query[250];
		PlayerInfo[playerid][vColor1] = color1;
		PlayerInfo[playerid][vColor2] = color2;
		mysql_format(Conecction, query, sizeof(query), "UPDATE `usuarios` SET `Color1`='%d',`Color2`='%d' WHERE `ID`='%d'",color1,color2, PlayerInfo[playerid][p_DBID]);
		mysql_pquery(Conecction, query);
