CMD:quitarprenda(playerid, params[])
{
  	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_ADM) return 0;
	new giveplayerid, prendaid, Query[200];
	if(sscanf(params, "dd", giveplayerid, prendaid)) return Mensaje(playerid, -1, "USA: /quitarprenda [playerid] [prendaid(1-4)]");
	switch (prendaid)
	{
		case 1:
		{
			if(IsPlayerAttachedObjectSlotUsed(giveplayerid,SlotP2[0][giveplayerid]))
			{
				RemovePlayerAttachedObject(giveplayerid, SlotP2[0][giveplayerid]);
				UsandoP[giveplayerid]--;
			}
			mysql_format(Conecction, Query, sizeof(Query),"DELETE FROM `prendas` WHERE `UserID`='%d' AND `Slot`='1'", PlayerInfo[giveplayerid][p_DBID]);
			mysql_pquery(Conecction, Query);
			PlayerInfo[giveplayerid][pObj] = 11392;
			NPrendas[giveplayerid]--;
			UsandoObj[giveplayerid] = 0;
			Mensaje(playerid, -1, "Le quitaste la prenda ID 1 a ese jugador.");
			return 1;
		}
		case 2:
		{
			if(IsPlayerAttachedObjectSlotUsed(giveplayerid,SlotP2[1][giveplayerid]))
			{
				RemovePlayerAttachedObject(giveplayerid, SlotP2[1][giveplayerid]);
				UsandoP[giveplayerid]--;
			}
			mysql_format(Conecction, Query, sizeof(Query),"DELETE FROM `prendas` WHERE `UserID`='%d' AND `Slot`='2'", PlayerInfo[giveplayerid][p_DBID]);
			mysql_pquery(Conecction, Query);
			PlayerInfo[giveplayerid][pObj2] = 11392;
			NPrendas[giveplayerid]--;
			UsandoObj2[giveplayerid] = 0;
			Mensaje(playerid, -1, "Le quitaste la prenda ID 2 a ese jugador.");
			return 1;
		}
		case 3:
		{
			if(IsPlayerAttachedObjectSlotUsed(giveplayerid, SlotP2[2][giveplayerid]))
			{
				RemovePlayerAttachedObject(giveplayerid,  SlotP2[2][giveplayerid]);
				UsandoP[giveplayerid]--;
			}
			mysql_format(Conecction, Query, sizeof(Query),"DELETE FROM `prendas` WHERE `UserID`='%d' AND `Slot`='3'", PlayerInfo[giveplayerid][p_DBID]);
			mysql_pquery(Conecction, Query);
			PlayerInfo[giveplayerid][pObj3] = 11392;
			NPrendas[giveplayerid]--;
			UsandoObj3[giveplayerid] = 0;
			Mensaje(playerid, -1, "Le quitaste la prenda ID 3 a ese jugador.");
			return 1;
		}
		case 4:
