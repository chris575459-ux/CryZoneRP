		if(IsPlayerConnected(i) && EnCarreras[i] > 0) SacarCarrera(i, 1);
	}
	ConteoCarrera = 0;
	ParticipantesCarrera = 0;
	ParticipantesCarrera2 = 0;
	lugarcarrera = 0;
	EmpezoCarrera = 0;
	HabilitarCarreras = 0;
	TipoConteo = 0;
	SlotGanador = 0;
	TiempoCarrera[0] = 0;
	TiempoCarrera[1] = 0;
	for(new a; a < 4; a++)
	{
		for(new b; b < 6; b++)
		{
			InfoCP[a][b] = 0;
		}
	}
}

