CMD:ex(playerid, params[])
{
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_ADM) return 0;
	if(!sscanf(params, "d", params[0]))
	{
		if(IsPlayerConnected(params[0]))
		{
			if(PlayerInfo[params[0]][jAdminNvl] > PlayerInfo[playerid][jAdminNvl]) return Mensaje(playerid, COLORCARGO0, "Error.");
			new Float:p_x,Float:p_y,Float:p_z;
			GetPlayerPos(params[0], p_x,p_y,p_z);
			CreateExplosion(p_x, p_y, p_z, 0, 10.0);
			return Mensaje(playerid, COLORCARGO0, "Explotado!");
		}
		else Mensaje(playerid, COLORCARGO0, "Esa id es inválida.");
	}
	else Mensaje(playerid, COLORCARGO0, "Usa {F4FA58}/ex [id]");
	return 1;
}

