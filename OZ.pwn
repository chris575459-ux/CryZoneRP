function Curar()
{
	foreach(new i : Player)
	{
		if(JugadorLogeado[i] == 1)
		{
			if(Curando[i] == 1)
			{
				new Float:health;
				GetPlayerHealth(i,health);
				if(health >= 60)
				{
					Curando[i] = 0;
					EstablecerVida(i, 60);
					SetTimerEx("ApareceHospital2", 1000, false, "d", i);
				}
				else
				{
					EstablecerVida(i, health+2.0);
				}
			}
			// ANTI TELEPORT
			new Float:Pos[3];
			GetPlayerPos(i,Pos[0],Pos[1],Pos[2]);
			new Float:Distancia = GetPlayerDistanceFromPoint(i,PosJugador[i][0],PosJugador[i][1],PosJugador[i][2]);
			if(!IsPlayerInAnyVehicle(i))
			{
				if(Distancia > 100 && TimerTP[i] < gettime() && GetPlayerState(i) != PLAYER_STATE_SPECTATING && GetPlayerSurfingVehicleID(i) == INVALID_VEHICLE_ID)
				{
					new string[250];
					format(string,sizeof(string),"[PRUEBA] TELEPORT - Jugador: %s (ID:%d) - Distancia: %f - Nivel: %d",NombreJugador(i),i,Distancia,PlayerInfo[i][jNivel]);
					for(new i2, j2 = GetPlayerPoolSize(); i2 <= j2; i2++)
					{
						if(PlayerInfo[i2][jAdminNvl] > ADMIN_LVL_JUEGO1 && VerAdmin[i2] == 1) Mensaje(i2,COLOR_SERVER,string);
					}
				}
			}
			else if(GetPlayerState(i) == PLAYER_STATE_DRIVER)
			{
   				if((Distancia > 100 && GetVehicleModel(GetPlayerVehicleID(i)) == 411 || Distancia > 100 && GetVehicleModel(GetPlayerVehicleID(i)) != 411) && TimerTP[i] < gettime() && !IsPlayerInTransfender(i) && GetPlayerInterior(i) == 0)
	   			{
					new string[250];
					format(string,sizeof(string),"[PRUEBA] TP VEHICLE - Jugador: %s (ID:%d) - Distancia: %f - Nivel: %d",NombreJugador(i),i,Distancia,PlayerInfo[i][jNivel]);
					for(new i2, j2 = GetPlayerPoolSize(); i2 <= j2; i2++)
					{
						if(PlayerInfo[i2][jAdminNvl] > ADMIN_LVL_JUEGO1 && VerAdmin[i2] == 1) Mensaje(i2,COLOR_SERVER,string);
					}
	 			}
			}

			PosJugador[i][0] = Pos[0];
			PosJugador[i][1] = Pos[1];
			PosJugador[i][2] = Pos[2];
			// ANTI TELEPORT
		}
	}
	for(new v, f = GetVehiclePoolSize(); v <= f; v++)
	{
		if(GetVehicleModel(v) > 0/* && ConductorVehiculo(v) != INVALID_PLAYER_ID*/)
		{
			new Float:Vida;
			GetVehicleHealth(v,Vida);
			if(Vida != VehicleVida[v])
			{
				CallRemoteFunction("OnVehicleHealthChange","iff",v,Vida,VehicleVida[v]);
				VehicleVida[v] = Vida;
			}
		}
	}
}

