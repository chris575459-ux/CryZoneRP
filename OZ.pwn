CMD:expulsar(playerid,params[])
{
	new id,string[250];
	if(!sscanf(params,"r",id))
	{
		if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT)
		{
			if(Alquilado == false) return Mensaje(playerid,-1,"El salón debe estar alquilado.");
			if(strcmp(NombreJugador(playerid),Alquilador,true) == 0 || PermisoSalon[playerid] == true)
			{
				if(IsPlayerConnected(id))
				{
					if(EnSalonEvento(id) || EnSalonEvento2(id))
					{
						if(strcmp(NombreJugador(id),Alquilador,true) == 0) return Mensaje(playerid,-1,"No puedes expulsar a esa persona del salón.");
						SetPlayerPos(id,1423.4301,-1180.1801,25.9922);
						SetPlayerVirtualWorld(id,0);
						SetPlayerInterior(id,0);
						format(string,sizeof(string),"Los guardias agarran a %s del brazo y lo expulsan del salón.",NombreJugador(id));
						ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
						Mensaje(id,-1,"Los guardias de seguridad te sacaron del salón de eventos.");
						format(string,sizeof(string),"El jugador {DBED15}%s{FFFFFF} expulso a {DBED15}%s{FFFFFF} del salón de eventos.", NombreJugador(playerid), NombreJugador(id));
						MandarMensajeSalon(string, playerid);
						Boleto[id] = false;
						if(strcmp(NombreJugador(playerid),Alquilador,true) == 0)
						{
							PermisoSalon[id] = false;
						}
					}
					else Mensaje(playerid,-1,"El jugador debe estar dentro del salón.");
				}
				else Mensaje(playerid,-1,"Ese jugador está desconectado.");
			}
			else Mensaje(playerid,-1,"No tienes permiso para expulsar a esa persona.");
		}
		else if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
		{
			if(IsPlayerConnected(id))
			{
				if(id == playerid) return Mensaje(playerid, -1, "¡No te puedes echar ati mismo!");
				if(IsPlayerInAnyVehicle(id))
				{
					if(GetPlayerVehicleID(playerid) == GetPlayerVehicleID(id))
					{
						format(string, sizeof(string), "%s agarra fuertemente a %s y lo expulsa del vehículo.", NombreJugador(playerid), NombreJugador(id));
						ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
						RemovePlayerFromVehicle(id);
					}
					else Mensaje(playerid, -1, "El jugador no está en tu vehículo.");
				}
				else Mensaje(playerid, -1, "El jugador no está en un vehículo.");
			}
			else Mensaje(playerid, -1, "Jugador desconectado.");
		}
	}
	return true;
}

