CMD:re(playerid, params[])
{
	new id,razon[250];
	if(ReporteEstado == 0) return Mensaje(playerid,-1,"No puedes reportar en este momento.");
	if(PlayerInfo[playerid][pTiempoJail] > 0) return Mensaje(playerid,-1,"No puedes usar este comando en este momento, en su lugar usa {DBED15}/reglas");
	if(!sscanf(params,"ds[250]",id,razon))
	{
		if(gettime() > JustReported[playerid])
		{
			if(!IsPlayerConnected(id)) return Mensaje(playerid,COLORCARGO0,"Jugador reportado no conectado.");
			new length = strlen(razon);
			if(length > 100) return Mensaje(playerid,-1,"No puedes enviar un reporte muy extenso, abrevia tus palabras por favor.");
			SendReportToQue(playerid,id, razon);
			Mensaje(playerid,-1,"{00CC00}El reporte fue enviado correctamente.");
			JustReported[playerid] = gettime() + 60;
			return 1;
		}
		else
		{
			new numero = JustReported[playerid] - gettime();
			MensajeEx(playerid, -1, "Espera {00CC00}%ds{FFFFFF} para enviar otro reporte.", numero);
			return 1;
		}
	}
	else return Mensaje(playerid,-1,"Por favor, para reportar usa {DBED15}/reportar [ID] [Razón]");
}

CMD:telefono(playerid,params[])
{
	new string[128];
	if(!isnull(params))
	{
		if(strcmp(params, "on", true)==0)
		{
			if(PlayerInfo[playerid][pNumero] <= 0) return Mensaje(playerid,-1,"No tienes un teléfono celular.");
			if(TelefonoEstado[playerid] == 0) return Mensaje(playerid,-1,"El teléfono ya esta encendido.");
			{
				TelefonoEstado[playerid] = 0;
				if(Specteando[playerid] == 0)
				{
					format(string,128,"%s encendió su teléfono móvil.", NombreJugador(playerid));
					ProxDetector(10.0,playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
				}
			}
		}
		if(strcmp(params, "off", true)==0)
		{
			if(PlayerInfo[playerid][pNumero] <= 0) return Mensaje(playerid,-1,"No tienes un teléfono celular.");
			if(TelefonoEstado[playerid] == 1) return Mensaje(playerid,-1,"El teléfono ya esta apagado.");
			{
				//
				if(PlayerInfo[playerid][MobileSub] == 2540 || PlayerInfo[playerid][MobileSub] == 911)
				{
					ColgarLaLlamada(playerid);
				}
				new caller = PlayerInfo[playerid][MobileSub];
				if(IsPlayerConnected(caller) && caller != -1)
				{
					Mensaje(caller, -1, "Colgaron la llamada.");
					SetPlayerSpecialAction(caller, SPECIAL_ACTION_STOPUSECELLPHONE);
					RemovePlayerAttachedObject(caller, 2);
					PlayerInfo[caller][MobileSub] = -1;
					EnLlamada[caller] = 0;
					finllamada(caller);
					ColgarLaLlamada(playerid);
				}
				//
				TelefonoEstado[playerid] = 1;
				if(Specteando[playerid] == 0)
				{
					format(string,128,"%s apagó su teléfono móvil.", NombreJugador(playerid));
					ProxDetector(10.0,playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
				}
			}
		}
	}
	else return Mensaje(playerid,-1,"Modo de uso: {DBED15}/telefono on/off");
	return 1;
}

