public OnPlayerDeath(playerid, killerid, reason)
{
	#if DEBUG_GM == 1
	printf("(OnPlayerDeath) PlayerID: %d | KillerID: %d", playerid, killerid);
	#endif
	SacarCarreraCross(playerid);
	new mensajemuerto[150];
	antifakekill[playerid] ++;
	PlayerAnticheat[playerid][paSpawneado] = false;
	SetTimerEx("antifakekill2", 1000,false,"i",playerid);
	for(new i, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if(PlayerInfo[i][jAdminNvl] >= ADMIN_LVL_JUEGO1 && killerid != 65535 && VerAdmin[i] == 1)
		{
			format(mensajemuerto, 150, "%s (ID: %d) mato a %s (ID: %d) con %s", NombreJugador(killerid), killerid, NombreJugador(playerid), playerid, GetWeaponNameEx(reason));
			Mensaje(i, COLOR_VERDE, mensajemuerto);
