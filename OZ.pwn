CMD:traertodos(playerid, params[])
{
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_ADM) return 0;
	new Float:pos[3];
	GetPlayerPos(playerid, pos[0], pos[1], pos[2]);
	for(new id, j = GetPlayerPoolSize(); id <= j; id++)
	{
		if(IsPlayerConnected(id))
		{
			SetPlayerPos(id, pos[0], pos[1], pos[2]);
			SetPlayerInterior(id, GetPlayerInterior(playerid));
			SetPlayerVirtualWorld(id, GetPlayerVirtualWorld(playerid));
		}
	}
	Mensaje(playerid,-1, "Has traido a todos a tu posición.");
	return 1;
}

CMD:weatherall(playerid, params[])
{
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_ADM) return 0;
	new weather, string[84];
	if(sscanf(params, "d", weather)) return Mensaje(playerid, COLORCARGO0, "USA: /weatherall [weatherid]");
	if(weather < 0||weather > 2009) return Mensaje(playerid, COLOR_GREY, "Nota: 0 a 2009!");
	SetWeather(weather);
	format(string, 256, "%s cambió el clima al ID %d", NombreJugador(playerid), weather);
	Mensaje(playerid, -1, string);
	return 1;
}

CMD:voz(playerid, params[])
{
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_ADM) return 0;
	if(isnull(params)) return Mensaje(playerid, COLOR_BLANCO, "Usa /voz [Texto]");
	new string[300];
	format(string, sizeof(string), "http://audio1.spanishdict.com/audio?lang=es&voice=Ximena&speed=25&text=%s", params);
	for(new i, j = GetPlayerPoolSize(); i <= j; i++)
	{
		PlayAudioStreamForPlayer(i, string, 0, 0, 0, 0, 0);
	}
	return 1;
}

CMD:damemod(playerid,params[])
{
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_ADM) return 0;
	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return 1;
	new mod, vehicleid = GetPlayerVehicleID(playerid);
	if(!sscanf(params,"d",mod))
	{
		AddVehicleComponent(vehicleid, mod);
	}
	return 1;
}
