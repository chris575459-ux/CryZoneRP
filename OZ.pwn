function Ingresar(playerid, tipo)
{
	if(tipo >= 1 && tipo != 2)
	{
		for(new c = 1; c < MAX_PROPIEDADES; c++)
		{
			if(PropInfo[c][pEnUso] == 1)
			{
				if(EstaEn(playerid,1.0,PropInfo[c][pPosX],PropInfo[c][pPosY],PropInfo[c][pPosZ])) // Puerta
				{
					if(GetPlayerVirtualWorld(playerid) == 0)
					{
						if(PlayerInfo[playerid][pCargos] > 0 && PropInfo[c][pEnVenta] == 1) return Mensaje(playerid,-1,"No puedes entrar a esta propiedad si la policía te está buscando.");
						if(PropInfo[c][pAbierta] == 1)
						{
							if(PlayerInfo[playerid][pCargos] >= 2)
							{
								for(new i, j = GetPlayerPoolSize(); i <= j; i++)
								{
									if(PlayerInfo[i][jMiembro] == 1 && PlayerInfo[i][pDuty] == 1 && EstaEnP(playerid,i,25)) return Mensaje(playerid,-1,"No puedes entrar si la policía te está siguiendo.");
								}
							}
							SetPosEx(playerid,PropInfo[c][pPosXS],PropInfo[c][pPosYS],PropInfo[c][pPosZS],PropInfo[c][pPosAS],PropInfo[c][pInteriorID],PropInfo[c][pVW]);
							if(PropInfo[c][pEmisora] != 0)
							{
								for(new id, j = GetPlayerPoolSize(); id <= j; id++)
								{
									if(Emisoras[id][CabinaOcupada] == PropInfo[c][pEmisora])
									{
