		PlayerInfo[playerid][pCama] = 0;
		PlayerTemp[playerid][pt_Enfermedad] = 0;
	}
	return 1;
}

function Float:DistanciaJugadorCamara(playerid)
{
	if(GetPlayerState(playerid) != PLAYER_STATE_SPECTATING)
	{
		new Float:pP[3],Float:tP[3],Float:Float_NaN;
		if(!GetPlayerPos(playerid,pP[0],pP[1],pP[2]) || !GetPlayerCameraPos(playerid,tP[0],tP[1],tP[2])) return Float_NaN;
		pP[0] -= tP[0], pP[1] -= tP[1],	pP[2] -= tP[2];
		return floatsqroot(floatpower(pP[0],2.0) + floatpower(pP[1],2.0) + floatpower(pP[2],2.0));
	}
	else return floatsqroot(0);
}

function resetearcoches()
{
	for(new i, j = GetVehiclePoolSize(); i <= j; i++)
	{
		for(new u, h = GetPlayerPoolSize(); u <= h; u++)
		{
			if(i == autos1[u]) goto _no_borrar;
			if(i == autos2[u]) goto _no_borrar;
			if(i == autos3[u]) goto _no_borrar;
			if(i == autos4[u]) goto _no_borrar;
			if(IsPlayerInVehicle(u,i)) goto _no_borrar;
		}
		Respawnear(i);
		LinkVehicleToInterior(i, 0);
		SetVehicleVirtualWorld(i, 0);
		_no_borrar:
	}
	return 1;
}

function resetearcochestrabajo()
{
	for(new i, j = GetVehiclePoolSize(); i <= j; i++)
	{
		for(new u, h = GetPlayerPoolSize(); u <= h; u++)
		{
			new Float:health;
			GetVehicleHealth(i, health);
			if(i == autos1[u]) goto _no_borrar;
			if(i == autos2[u]) goto _no_borrar;
			if(i == autos3[u]) goto _no_borrar;
			if(i == autos4[u]) goto _no_borrar;
			if(IsPlayerInVehicle(u,i)) goto _no_borrar;
			if(health > 375) goto _no_borrar;
		}
		Respawnear(i);
		LinkVehicleToInterior(i, 0);
		SetVehicleVirtualWorld(i, 0);
		_no_borrar:
	}
	return 1;
}

function AutoCheats()
{
	for(new i, j = GetPlayerPoolSize(); i <= j; i++)
	{
		new autoid = GetPlayerVehicleID(i);
		if(IsPlayerInAnyVehicle(i))
		{
			if(autoid != EnAuto[i] && PlayerInfo[i][jAdminNvl] < ADMIN_LVL_GLOBAL1)
			{
				if(AvisosAuto[i] < 5)
				{
					AvisosAuto[i]++;
				}
				else if(AvisosAuto[i] >= 5 && AceptacionBlindado[i] != 2)
				{
					ResetPlayerWeapons(i);
					DarArmas(i);
				}
			}
		}
	}
}

function Gasolinass()
{
	for(new i, j = GetVehiclePoolSize(); i <= j; i++)
	{
		for(new u, h = GetPlayerPoolSize(); u <= h; u++)
		{
			if(JugadorLogeado[u] == 1)
			{
				if(MotorAuto[i] == 1)
				{
					if(VehicleFuel[i] >= 1)
					{
						if(IsPlayerInVehicle(u,i) && GetPlayerState(u) == PLAYER_STATE_DRIVER)
						{
							VehicleFuel[i]--;
							if(i == autos1[u]) { PlayerInfo[u][vGas]  = VehicleFuel[i]; }
							if(i == autos2[u]) { PlayerInfo[u][v2Gas] = VehicleFuel[i]; }
							if(i == autos3[u]) { PlayerInfo[u][v3Gas] = VehicleFuel[i]; }
							if(i == autos4[u]) { PlayerInfo[u][v4Gas] = VehicleFuel[i]; }
							SinGaso[u] = 0;

							new td_str[16];
							format(td_str, sizeof td_str, "%d/%d", VehicleFuel[i], GetVehicleMaxCombustible(i));
							PlayerTextDrawSetString(u, PlayerVelocimetro[u][ptdCombustible][0], td_str);

							new Float:barraGas = 547.000000 + floatmul(floatdiv(float(VehicleFuel[i]), float(GetVehicleMaxCombustible(i))), 66.349976);
							if(barraGas > 613.349976) barraGas = 613.349976;
							PlayerTextDrawTextSize(u, PlayerVelocimetro[u][ptdCombustible][1], barraGas, 0.000000);
							PlayerTextDrawUseBox(u, PlayerVelocimetro[u][ptdCombustible][1], 1);
							PlayerTextDrawShow(u, PlayerVelocimetro[u][ptdCombustible][1]);
						}
					}
					else if(VehicleFuel[i] < 1)
					{
						new td_str[16];

						format(td_str, sizeof td_str, "%d/%d", VehicleFuel[i], GetVehicleMaxCombustible(i));
						PlayerTextDrawSetString(u, PlayerVelocimetro[u][ptdCombustible][0], td_str);

						PlayerTextDrawUseBox(u, PlayerVelocimetro[u][ptdCombustible][1], 0);
						PlayerTextDrawShow(u, PlayerVelocimetro[u][ptdCombustible][1]);

						GetVehicleParamsEx(i, engine, lights, alarm, doors, bonnet, boot, objective);
						SetVehicleParamsEx(i, 0, lights, alarm, doors, bonnet, boot, objective);
						SinGaso[i] = 1;
						MotorAuto[i] = 0;
					}
				}
			}
		}
	}
}

function Codigos()
{
	for(new u, j = GetPlayerPoolSize(); u <= j; u++)
	{
		if(PlayerInfo[u][pCargos] == 0 && Refuerzos[u] == 0 && AceptacionBlindado[u] != 2) MarcarJugador(u,COLORCARGO0,1);
		else if((Esposando[u] == 2 || PlayerInfo[u][pCargos] > 0 && VariableCaido[u] == 1) && EnPatrulla[u] == 0) MarcarJugador(u,COLORCARGO7,1);
		else if(PlayerInfo[u][pCargos] == 1) MarcarJugador(u,COLORCARGO1,1);
		else if(PlayerInfo[u][pCargos] == 2) MarcarJugador(u,COLORCARGO2,1);
		else if(PlayerInfo[u][pCargos] == 3) MarcarJugador(u,COLORCARGO3,1);
		else if(PlayerInfo[u][pCargos] == 4) MarcarJugador(u,COLORCARGO4,1);
		else if(PlayerInfo[u][pCargos] == 5) MarcarJugador(u,COLORCARGO5,1);
		else if(PlayerInfo[u][pCargos] >= 6) MarcarJugador(u,COLORCARGO6,1);
		// Trabajo Medico
		if(MarcadoMedicos[u] == 1)
		{
			for(new i, h = GetPlayerPoolSize(); i <= h; i++)
			{
				if(RopaMedi[i] == 1 && PlayerInfo[i][jJob] == MEDICO || PlayerInfo[i][jJob2] == MEDICO)
				{
					GetPlayerPos(u, DeathXH[u],DeathYH[u],DeathZH[u]);
					SetPlayerMarkerForPlayer(i, u, COLORABATIDO);
				}
			}
		}
		// Trabajo Taxi
		if(LlamadaTaxi[u] == 1)
		{
			for(new i, h = GetPlayerPoolSize(); i <= h; i++)
			{
				if(PlayerInfo[i][jJob] == TAXISTA || PlayerInfo[i][jJob2] == TAXISTA && EnTaxi[i] == 1)
				{
					SetPlayerMarkerForPlayer(i, u, COLOR_AMARILLO);
				}
			}
		}
		// Trabajo Mecanico
		if(LlamadaMeca[u] == 1)
		{
			for(new i, h = GetPlayerPoolSize(); i <= h; i++)
			{
				if(RopaMeca[i] == 1 && PlayerInfo[i][jJob] == MECANICO || PlayerInfo[i][jJob2] == MECANICO)
				{
					SetPlayerMarkerForPlayer(i, u, COLOR_AZUL);
				}
			}
		}
		// Trabajo Polícia
		if(PlayerInfo[u][pDuty] == 1 && AceptacionBlindado[u] != 2)
		{
			SetPlayerColor(u,TEAM_BLUE_COLOR);
		}
	}
	return 1;
}

stock VehicleToPoint(Float:radi, vehicleid, Float:x, Float:y, Float:z)
{
	new Float:oldposx, Float:oldposy, Float:oldposz;
	new Float:tempposx, Float:tempposy, Float:tempposz;
	GetVehiclePos(vehicleid, oldposx, oldposy, oldposz);
	tempposx = (oldposx -x);
	tempposy = (oldposy -y);
	tempposz = (oldposz -z);
	if (((tempposx < radi) && (tempposx > -radi)) && ((tempposy < radi) && (tempposy > -radi)) && ((tempposz < radi) && (tempposz > -radi)))
	{
		return 1;
	}
	return 0;
}

encode_tires(tire1, tire2, tire3, tire4)
{
	return tire1 | (tire2 << 1) | (tire3 << 2) | (tire4 << 3);
}

