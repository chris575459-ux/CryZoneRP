public OnVehicleDeath(vehicleid, killerid)
{
	#if DEBUG_GM == 1
	printf("(OnVehicleDeath) VehicleID: %d | KillerID: %d", vehicleid, killerid);
	#endif
	if(EmpezoMision == 1)
	{
		if(vehicleid == VehiculoAsalto[4])
		{
			for(new u; u < sizeof(OrdenGrupo); u++)
			{
				if(OrdenGrupo[u] > -1)
				{
					new i = OrdenGrupo[u];
					if(GrupoRobo[i] == 1) Mensaje(i,-1,"El camión de los guardias fue destruido, la misión fue cancelada.");
					else Mensaje(i,-1,"El camión fue destruido, la misión fue cancelada.");
					SacarDeMision(i);
					SetPlayerVirtualWorld(i,0);
					SetPlayerInterior(i,0);
				}
			}
			ResetearMision();
		}
		if(vehicleid == VehiculoAsalto[0])
		{
			for(new u; u < sizeof(OrdenGrupo); u++)
			{
				if(OrdenGrupo[u] > -1)
				{
					new i = OrdenGrupo[u];
					if(GrupoRobo[i] != 1) Mensaje(i,-1,"El camión de los delincuentes fue destruido, la misión fue cancelada.");
					else Mensaje(i,-1,"El camión fue delincuentes, la misión fue cancelada.");
					SacarDeMision(i);
					SetPlayerVirtualWorld(i,0);
					SetPlayerInterior(i,0);
				}
			}
			ResetearMision();
		}
	}
	return true;
}

public OnVehicleSpawn(vehicleid)
{
	#if DEBUG_GM == 1
	printf("(OnVehicleSpawn) VehicleID: %d", vehicleid);
	#endif
	foreach(new i : Player)
	{
		if(vehicleid == autos1[i])		return Auto1(i, 3);
		else if(vehicleid == autos2[i])	return Auto2(i, 3);
		else if(vehicleid == autos3[i])	return Auto3(i, 3);
		else if(vehicleid == autos4[i])	return Auto4(i, 3);
	}
	if(SAPD(vehicleid)) bloqueado[vehicleid] = 0;
	SetVehicleHealth(vehicleid, 1000.0);
	EmisoraEscuchando[vehicleid] = -1;
	VehicleFuel[vehicleid] = GetVehicleMaxCombustible(vehicleid);
	MotorAuto[vehicleid] = 0;
	SinGaso[vehicleid] = 0;
	Balde[vehicleid] = 0;
	Luces[vehicleid] = 0;
	return 1;
}

function Auto1(playerid, tipo)
{
	if(!IsPlayerConnected(playerid)) return 1;
	if(JugadorLogeado[playerid] != 1) return 1;
	if(tipo == 3)
	{
		MensajeEx(playerid,-1, "Tu {DBED15}%s{FFFFFF} fue remolcado al último lugar donde lo estacionaste.", GetVehicleName(autos1[playerid]));
		if(autos1[playerid] != INVALID_VEHICLE_ID) DestroyVehicle(autos1[playerid]);
		autos1[playerid] = CreateVehicle(PlayerInfo[playerid][vModelo],PlayerInfo[playerid][pOX],PlayerInfo[playerid][pOY],PlayerInfo[playerid][pOZ],PlayerInfo[playerid][pOA],PlayerInfo[playerid][vColor1],PlayerInfo[playerid][vColor2],-1,0);
		LinkVehicleToInterior(autos1[playerid], PlayerInfo[playerid][vOinterior]);
		SetVehicleVirtualWorld(autos1[playerid], PlayerInfo[playerid][vOvw]);
		SetVehicleHealth(autos1[playerid], PlayerInfo[playerid][pVidaVE]);
	}
	else if(tipo == 5 || tipo == 7 || tipo == 8 || tipo == 9 || tipo == 10 || tipo == 11)
	{
		if(tipo == 5) MensajeEx(playerid,-1, "Tu {DBED15}%s{FFFFFF} fue trasladado al estacionamiento del Salón de Eventos.",				GetVehicleName(autos1[playerid]));
		if(tipo == 7) MensajeEx(playerid,-1, "Tu {DBED15}%s{FFFFFF} fue trasladado al estacionamiento del Estudio de Radio y Televisión.",	GetVehicleName(autos1[playerid]));
		if(tipo == 8) MensajeEx(playerid,-1, "Tu {DBED15}%s{FFFFFF} fue trasladado al estacionamiento de Pershing Square.",					GetVehicleName(autos1[playerid]));
		if(tipo == 9) MensajeEx(playerid,-1, "Tu {DBED15}%s{FFFFFF} fue trasladado al estacionamiento del Estadio de Las Venturas.",		GetVehicleName(autos1[playerid]));
		if(tipo == 10) MensajeEx(playerid,-1, "Tu {DBED15}%s{FFFFFF} fue trasladado al estacionamiento del Estadio de San Fierro.",			GetVehicleName(autos1[playerid]));
		if(tipo == 11) MensajeEx(playerid,-1, "Tu {DBED15}%s{FFFFFF} fue trasladado al estacionamiento del Estadio de Los Santos.",			GetVehicleName(autos1[playerid]));
		if(autos1[playerid] != INVALID_VEHICLE_ID) DestroyVehicle(autos1[playerid]);
		new rand = random(sizeof(EstacionamientoSpawn));
