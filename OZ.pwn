function Reinicio()
{
	new string[128];
	if(TiempoReinicio == 1)
	{
		format(string, 128, "El servidor se reiniciará automáticamente en 1 minuto.", TiempoReinicio);
		MensajeAll(0xB1F6F6F6, string);
		SetTimerEx("Conteo", 1000, false, "i", 59);
		TiempoReinicio--;
		return 1;
	}
	if(TiempoReinicio > 0)
	{
		format(string, 128, "El servidor se reiniciará automáticamente en %d minutos.", TiempoReinicio);
		MensajeAll(0xB1F6F6F6, string);
		TiempoReinicio--;
		return 1;
	}
	if(TiempoReinicio == 0)
	{
		for(new i, j = GetPlayerPoolSize(); i <= j; i++)
		{
   			Kick(i);
			NoQuitar[i] = 1;
			SaveAccount(i);
		}
		MensajeAll(0xB1F6F6F6, "El servidor se reiniciara ahora.");
		KillTimer(TimerReinicio);
		SendRconCommand("exit");
	}
	return 1;
}

function Conteo(Tiempo)
{
	new string[128];
	if(Tiempo >= 1)
	{
		format(string, 128, "El servidor se reiniciará automáticamente en %d segundos.", Tiempo);
		MensajeAll(0xB1F6F6F6, string);
		TimerReinicio2 = SetTimerEx("Conteo", 1000, false, "i", Tiempo-1);
	}
	return 1;
}

