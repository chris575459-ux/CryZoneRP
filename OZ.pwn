function CargarAutos()
{
	for(new i = 18; i < cache_num_rows(); i++)
	{
		new stringinfovehiculo[70];
		cache_get_value_name(i, "propietario", VehiculosInfo[i][vpropietario]);
		cache_get_value_name_int(i, "precio", VehiculosInfo[i][vprecio]);
		cache_get_value_name_int(i, "modelo", VehiculosInfo[i][vmodelo]);
		cache_get_value_name_int(i, "moneda", VehiculosInfo[i][vmoneda]);
		cache_get_value_name_float(i, "x", VehiculosInfo[i][vx]);
		cache_get_value_name_float(i, "y", VehiculosInfo[i][vy]);
		cache_get_value_name_float(i, "z", VehiculosInfo[i][vz]);
		cache_get_value_name_float(i, "a", VehiculosInfo[i][va]);
		cache_get_value_name_int(i, "color1", VehiculosInfo[i][vcolor1]);
		cache_get_value_name_int(i, "color2", VehiculosInfo[i][vcolor2]);
		cache_get_value_name_int(i, "comprable", VehiculosInfo[i][vcomprable]);
		cache_get_value_name_int(i, "faccion", VehiculosInfo[i][vfaccion]);
		cache_get_value_name_int(i, "trabajo", VehiculosInfo[i][vtrabajo]);
		cache_get_value_name_int(i, "Vida", VehiculosInfo[i][vVida]);
		cache_get_value_name_int(i, "Interior", VehiculosInfo[i][vInterior]);
		cache_get_value_name_int(i, "VirtualWorld", VehiculosInfo[i][vVirtualWorld]);
		cache_get_value_name_int(i, "Capo", VehiculosInfo[i][vCapo]);
		cache_get_value_name_int(i, "Puertas", VehiculosInfo[i][vPuertas]);

		VehiculosInfo[i][vID] = i;
		CreateVehicle(VehiculosInfo[i][vmodelo], VehiculosInfo[i][vx], VehiculosInfo[i][vy], VehiculosInfo[i][vz], VehiculosInfo[i][va], VehiculosInfo[i][vcolor1], VehiculosInfo[i][vcolor2], 900);
		if(VehiculosInfo[i][vcomprable] == 1)
		{
			switch(VehiculosInfo[i][vmoneda])
			{
				case 0:
				{
					format(stringinfovehiculo, sizeof(stringinfovehiculo), "{DBED15}%s\n{DBED15}Precio: $%s", NombreAuto[VehiculosInfo[i][vmodelo]-400],ConvertirNumero(VehiculosInfo[i][vprecio]));
					CreateDynamic3DTextLabel(stringinfovehiculo,0xDBED15FF,0.0,0.0,0.0,10.0,INVALID_PLAYER_ID,VehiculosInfo[i][vID], 1, 0,0,-1, 100.0);
				}
				case 1:
				{
					format(stringinfovehiculo, sizeof(stringinfovehiculo), "{DBED15}%s\n{DBED15}Precio: {DBED15}%s Oz", NombreAuto[VehiculosInfo[i][vmodelo]-400],ConvertirNumero(VehiculosInfo[i][vprecio]));
					CreateDynamic3DTextLabel(stringinfovehiculo,0xDBED15FF,0.0,0.0,0.0,10.0,INVALID_PLAYER_ID,VehiculosInfo[i][vID], 1, 0,0,-1, 100.0);
				}
			}
		}
		if(VehiculosInfo[i][vVida] != 0) SetVehicleHealth(i,VehiculosInfo[i][vVida]);
		if(VehiculosInfo[i][vInterior] != 0) LinkVehicleToInterior(i,VehiculosInfo[i][vInterior]);
		if(VehiculosInfo[i][vVirtualWorld] != 0) SetVehicleVirtualWorld(i,VehiculosInfo[i][vVirtualWorld]);
		if(VehiculosInfo[i][vCapo] != 0)
		{
			GetVehicleParamsEx(i, engine, lights, alarm, doors, bonnet, boot, objective);
			SetVehicleParamsEx(i, engine, lights, alarm, doors, 1, boot, objective);
		}
		if(VehiculosInfo[i][vPuertas] != 0)
		{
			GetVehicleParamsEx(i, engine, lights, alarm, doors, bonnet, boot, objective);
			SetVehicleParamsEx(i, engine, lights, alarm, 1, bonnet, boot, objective);
			bloqueado[i] = 1;
		}
	}
	return 1;
}

public OnPlayerInteriorChange(playerid, newinteriorid, oldinteriorid)
{
	new aa;
	for(new i, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if(IsPlayerConnected(i) && !IsPlayerNPC(i) && SpecJugador[i] == playerid)
		{
			SetPlayerInterior(i, newinteriorid);
			SetPlayerVirtualWorld(i, GetPlayerVirtualWorld(playerid));
			if(aa == 0) SetTimerEx("ActualizarSpec",1000,0,"d",playerid);
			aa = 1;
		}
	}
	return 1;
}

IsValidRPName(const string[])
{
	new Regex:reg_exp = Regex_New("[A-Z][a-z]+_[A-Z][a-z]{1,6}[A-Z]?[a-z]*");
	new result = Regex_Check(string, reg_exp);
	Regex_Delete(reg_exp);
	return result;
}

stock PlayerConectado(playerid)
{
	if(playerid == INVALID_PLAYER_ID) return 1;
	if(playerid < 0 || playerid > MAX_PLAYERS) return 1;
	if(!IsPlayerConnected(playerid)) return 0;
	if(JugadorLogeado[playerid] == 1) return 1;
	return 0;
}

public OnPlayerDisconnect(playerid, reason)
{

	//
	/*
	new url[70];
	format(url, sizeof(url), "10.10.10.1:8080/ClassRem.php?addres=%s", PlayerInfo[playerid][pi_IP]);
