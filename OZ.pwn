				mysql_format(Conecction, Query, sizeof(Query), "INSERT INTO `log_ventas` (Vendedor, item, cantidad, precio, comprador,fecha) VALUES ('%s','Medicamentos', '%d','%d','%s','%02d/%02d/%02d')",	NombreJugador(IDOfrecioConsumible[playerid]),CantidadConsumible[playerid],PrecioConsumible[playerid],NombreJugador(playerid),day,month,year);
				mysql_pquery(Conecction, Query);
			}
			else if(QueCosaConsumible[playerid] == 2)
			{
				PlayerInfo[IDOfrecioConsumible[playerid]][pCrack] -= CantidadConsumible[playerid];
				PlayerInfo[playerid][pCrack] += CantidadConsumible[playerid];
				new Query[200];
	   			mysql_format(Conecction, Query, sizeof(Query), "INSERT INTO `log_ventas` (Vendedor, item, cantidad, precio, comprador,fecha) VALUES ('%s','crack', '%d','%d','%s','%02d/%02d/%02d')",			NombreJugador(IDOfrecioConsumible[playerid]),CantidadConsumible[playerid],PrecioConsumible[playerid],NombreJugador(playerid),day,month,year);
