			BorrarLabelCasa(id);
			CargarPropiedad(id);
			GuardarPropiedad(id);
			format(query,sizeof(query),"Cambiaste el interior de la casa ID: {DBED15}%d (Interior: %d).",id,interior);
			Mensaje(playerid,-1,query);
		}
		else Mensaje(playerid,-1,"Esa propiedad no existe.");
	}
	return true;
}

