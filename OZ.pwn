CMD:pstats(playerid, params[])
{
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_ADM) return 0;
	new opcion[16];
	if(sscanf(params, "s[16]", opcion)) return Mensaje(playerid, COLOR_BLANCO, "El uso del comando es {DBED15}/pstats [activar - desactivar]");
	if(!strcmp(opcion, "activar", true))
	{
		if(StatsStatus == 1) return Mensaje(playerid, COLOR_BLANCO, "Los stats se encuentran ya habilitados.");
		StatsStatus = 1;
		MensajeAll(0xDBED15FF, "Los stats se encuentran ahora activos.");
		mysql_pquery(Conecction, "UPDATE `registros` SET `Stats`='1'");
		foreach(new i : Player)
		{
			if(PlayerInfo[i][pStats] == 0)
			{
				Mensaje(i, COLOR_BLANCO, "Aún no has recibido tus stats. Puedes usar {DBED15}/recibir stats");
			}
		}
	}
	else if(!strcmp(opcion, "desactivar", true))
	{
		if(StatsStatus == 0) return Mensaje(playerid, COLOR_BLANCO, "Los stats se encuentran ya deshabilitados.");
		StatsStatus = 0;
		MensajeAll(0xDBED15FF, "Los stats se encuentran ahora desactivados.");
		mysql_pquery(Conecction, "UPDATE `registros` SET `Stats`='0'");
	}
	return 1;
}

CMD:disable(playerid, params[])
{
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_ADM) return 0;
	new opcion[16];
	if(sscanf(params, "s[16]", opcion)) return Mensaje(playerid, -1, "El uso del comando es {DBED15}/disable [activar - desactivar]");
	if(!strcmp(opcion, "activar", true))
	{
		if(DisableStatus == 1) return Mensaje(playerid, -1, "Disable ya se encuentra habilitado.");
		DisableStatus = 1;
		Mensaje(playerid, -1, "Disable fue activado correctamente.");
