function EncenderMotor(playerid)
{
	new string[256], autoid, Float:health;
	autoid = GetPlayerVehicleID(playerid);
	GetVehicleHealth(autoid, health);
	if(IsPlayerInAnyVehicle(playerid))
	{
		if(VehicleFuel[autoid] < 1) return SinGasolina(playerid);
		if(health <= 375.0)
		{
			new rand = random(2);
			switch (rand)
			{
				case 0:
				{
					if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
					Mensaje(playerid, -1, "Vehículo en marcha.");
					format(string, 256, "%s encendió el motor de su vehículo.", NombreJugador(playerid));
					ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
					GetVehicleParamsEx(autoid, engine, lights, alarm, doors, bonnet, boot, objective);
					SetVehicleParamsEx(autoid, 1, lights, alarm, doors, bonnet, boot, objective);
					YaEncendiendo[playerid] = 0;
					MotorAuto[autoid] = 1;
					SetTimerEx("ComprobarDanoMotor", 1000, 0, "dd", playerid, autoid);
					return 1;
				}
				case 1:
				{
					format(string, sizeof(string), "%s intenta encender el motor de su vehículo pero no puede.", NombreJugador(playerid));
					ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
					Mensaje(playerid, -1, "El motor no arranca, reintenta o llama al {DBED15}/servicio mecanico.");
					MotorAuto[autoid] = 0;
					return 1;
				}
			}
		}
		else
		{
			if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
			Mensaje(playerid, -1, "Vehículo en marcha.");
			format(string, 256, "%s encendió el motor de su vehículo.", NombreJugador(playerid));
			ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
			GetVehicleParamsEx(autoid, engine, lights, alarm, doors, bonnet, boot, objective);
			SetVehicleParamsEx(autoid, 1, lights, alarm, doors, bonnet, boot, objective);
			YaEncendiendo[playerid] = 0;
			MotorAuto[autoid] = 1;
			SetTimerEx("ComprobarDanoMotor", 1000, 0, "dd", playerid, autoid);
			return 1;
		}
	}
	return 1;
}

function ComprobarDanoMotor(playerid, autoid)
{
	new Float:health;
	GetVehicleHealth(autoid, health);
	if(health <= 375.0)
	{
		SetVehicleHealth(autoid, 375.0);
		GetVehicleParamsEx(autoid, engine, lights, alarm, doors, bonnet, boot, objective);
		SetVehicleParamsEx(autoid, 0, lights, alarm, doors, bonnet, boot, objective);
		averiado[autoid] = 1;
		MotorAuto[autoid] = 0;
		GameTextForPlayer(playerid, "~r~Motor quemado", 2500, 3);
		Mensaje(playerid,-1,"El motor dejó de funcionar, llama a un mecánico. {DBED15}/servicio mecanico{FFFFFF}.");
		return 1;
	}
	return 1;
}

