		{
			if(PlayerInfo[playerid][jJob] != TAXISTA && PlayerInfo[playerid][jJob2] != TAXISTA) return Mensaje(playerid,-1,"No eres taxista.");
			if(ResponderTaxi[playerid] == 1)
			{
				for(new i, j = GetPlayerPoolSize(); i <= j; i++)
				{
					if(LlamadaTaxi[i] == 1)
					{
						Mensaje(i,-1,"{22BF35}Un taxista respondió a tu llamada y va en camino.");
					}
				}
				Mensaje(playerid, -1,"{22BF35}Le acabas de avisar al cliente que vas a a buscarlo.");
				ResponderTaxi[playerid] = 0;
			}
			else return Mensaje(playerid,-1,"Ya fueron atendidos todos los llamados.");
		}
		if(strcmp(item,	"mecanico",true) == 0)
		{
			if(PlayerInfo[playerid][jJob] != MECANICO && PlayerInfo[playerid][jJob2] != MECANICO) return Mensaje(playerid,-1,"No eres mecánico.");
			if(ResponderMeca[playerid] == 1)
			{
				for(new i, j = GetPlayerPoolSize(); i <= j; i++)
				{
					if(LlamadaMeca[i] == 1)
					{
						Mensaje(i,-1,"{22BF35}Un mecánico respondió a tu llamada y va en camino.");
					}
				}
				Mensaje(playerid, -1,"{22BF35}Le acabas de avisar al cliente que vas a reparar su vehículo.");
