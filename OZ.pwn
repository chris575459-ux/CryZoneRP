function Armas()
{
	for(new i, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if(IsPlayerConnected(i))
		{
			// Anticheat
			if(PlayerInfo[i][jAdminNvl] < ADMIN_LVL_JUEGO1 && (GetPlayerCameraMode(i) == 7 || GetPlayerCameraMode(i) == 8 || GetPlayerCameraMode(i) == 51))
			{
				new string[200];
