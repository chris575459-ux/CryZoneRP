			mysql_format(Conecction, Query, sizeof(Query), "INSERT INTO `log_ventas` (Vendedor, item, cantidad, precio, comprador,fecha) VALUES ('%s','Oz', '%d','%d','%s','%02d/%02d/%02d')",NombreJugador(CompradorFZ[playerid]),FZAVender[playerid],PrecioFZ[playerid],NombreJugador(playerid),day,month,year);
			mysql_pquery(Conecction, Query);
			CompradorFZ[playerid] = -1;
			FZAVender[playerid] = 0;
			PrecioFZ[playerid] = 0;
			TiempoVentaFZ[playerid] = 0;
		}
		if(strcmp(item,"Muerte", true) == 0)
		{
			if(PlayerInfo[playerid][Agonizando] == 1 && Muriendo[playerid] == 1)
			{
				if(GetPlayerState(playerid) == 2 || GetPlayerState(playerid) == 3) return Mensaje(playerid, -1, "No puedes hacer esto dentro de un vehículo.");
   				SetPlayerColor(playerid, COLORCARGO0);
				ApareceHospital(playerid);
				Muriendo[playerid] = 0;
				suicidio[playerid] = 0;
				MarcadoMedicos[playerid] = 0;
				LlamadaMedicos[playerid] = 0;
			}
		}
		if(strcmp(item,"reparacion", true) == 0)
		{
			if(!PuedeHora(playerid)) return 1;
			if(AceptoRepara[playerid] == 1) return Mensaje(playerid,-1,"Ya aceptaste una reparación.");
			if(Mecanico[playerid] == INVALID_PLAYER_ID) return Mensaje(playerid,-1,"Nadie te ofreció una reparación.");
			if(!IsPlayerInAnyVehicle(playerid) && GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return Mensaje(playerid,-1,"Debes estar conduciendo tu vehículo para utilizar este comando.");
			if(PlayerInfo[playerid][jDinero] < MConductor[playerid]) return Mensaje(playerid,-1,"No tienes suficiente dinero.");
			GetPlayerPos(Mecanico[playerid], X,Y,Z);
			if(EstaEn(playerid, 4.0, X,Y,Z))
			{
				new string[200],Float:angle,autoid = GetPlayerVehicleID(playerid);
				GetPlayerFacingAngle(Mecanico[playerid],angle);
				SetPlayerFacingAngle(Mecanico[playerid],angle+180);
				LoopingAnim(Mecanico[playerid],"CAR","Fixn_Car_Loop",4.1,0,0,0,1,15000);
				switch(PlayerInfo[Mecanico[playerid]][pNivelPiloto])
				{
					case 1:
					{
						SetTimerEx("Reparando",15000,false,"dddd",Mecanico[playerid],playerid,autoid,1);
						GameTextForPlayer(Mecanico[playerid], "REPARANDO VEHICULO...", 15000, 5);
					}
					case 2:
					{
						SetTimerEx("Reparando",13000,false,"dddd",Mecanico[playerid],playerid,autoid,1);
						GameTextForPlayer(Mecanico[playerid], "REPARANDO VEHICULO...", 13000, 5);
					}
					case 3:
					{
						SetTimerEx("Reparando",9000,false,"dddd",Mecanico[playerid],playerid,autoid,1);
						GameTextForPlayer(Mecanico[playerid], "REPARANDO VEHICULO...", 9000, 5);
					}
					case 4:
					{
						SetTimerEx("Reparando",6000,false,"dddd",Mecanico[playerid],playerid,autoid,1);
						GameTextForPlayer(Mecanico[playerid], "REPARANDO VEHICULO...", 6000, 5);
					}
					case 5..100:
					{
						SetTimerEx("Reparando",3000,false,"dddd",Mecanico[playerid],playerid,autoid,1);
						GameTextForPlayer(Mecanico[playerid], "REPARANDO VEHICULO...", 3000, 5);
					}
				}
				new msjadm[135];
				format(msjadm, sizeof(msjadm), "%s (ID:%d) recibe $%s por reparar el vehículo de %s (ID: %d - Nivel: %d - %dhs).", NombreJugador(Mecanico[playerid]), Mecanico[playerid], ConvertirNumero(MConductor[playerid]), NombreJugador(playerid), playerid, PlayerInfo[playerid][jNivel], PlayerInfo[playerid][horasjugadas]);
				MensajeAdmin(COLOR_YELLOW, msjadm, 3);
				format(string, sizeof(string), "%s comienza a reparar el vehículo.", NombreJugador(Mecanico[playerid]));
				ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
				LlamadaMeca[playerid] = 0;
				SetPlayerColor(playerid, COLORCARGO0);
				QuitarDinero(playerid, MConductor[playerid]);
				DarDinero(Mecanico[playerid], MConductor[playerid]);
				PlayerInfo[Mecanico[playerid]][Repuestos] -= MRepuestos[Mecanico[playerid]];
				Mecanico[playerid] = INVALID_PLAYER_ID;
				AceptoRepara[playerid] = 1;
			}
			else return Mensaje(playerid,-1,"Debes estar cerca del mecánico para aceptar la reparación.");
		}
		if(strcmp(item,"proteccion", true) == 0)
		{
			if(!PuedeHora(playerid)) return 1;
			if(EnCombate[playerid] == 1) return Mensaje(playerid,-1,"No puedes hacer esto en combate, espera unos segundos y reintenta.");
			if(GuardaEspalda[playerid] == INVALID_PLAYER_ID) return Mensaje(playerid,-1,"No te han ofrecido ninguna protección.");
			if(PlayerInfo[playerid][jDinero] < dineropedido[playerid]) return Mensaje(playerid,-1,"No tienes suficiente dinero.");
			GetPlayerPos(GuardaEspalda[playerid], X,Y,Z);
			if(!EstaEn(playerid, 4.0, X,Y,Z)) return Mensaje(playerid,-1,"No estas cerca del vendedor.");
			{
				new string[256],Query[200];
				mysql_format(Conecction, Query, sizeof(Query), "INSERT INTO `log_ventas` (Vendedor, item, cantidad, precio, comprador,fecha) VALUES ('%s','Proteccion', '50','%d','%s','%02d/%02d/%02d')",NombreJugador(GuardaEspalda[playerid]),dineropedido[playerid],NombreJugador(playerid),day,month,year);
				mysql_pquery(Conecction, Query);
				QuitarDinero(playerid,dineropedido[playerid]);
				DarDinero(GuardaEspalda[playerid],dineropedido[playerid]);
				if(strcmp(PlayerInfo[GuardaEspalda[playerid]][Totem], NombreJugador(playerid), true) == 0 || strcmp(PlayerInfo[playerid][Totem], NombreJugador(GuardaEspalda[playerid]), true) == 0)  EstablecerChaleco(playerid, 70);
				else EstablecerChaleco(playerid, 50);
				format(string, 256, "%s acepta la protección de %s.",NombreJugador(playerid),NombreJugador(GuardaEspalda[playerid]));
				ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
				GuardaEspalda[playerid] = INVALID_PLAYER_ID;
			}
		}
	}
	return 1;
}

