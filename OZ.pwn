									if(autos2[i] == autoid)
									{
										new repuestos = PlayerInfo[i][v2Precio]/5000;
										format(string,256,"Este vehículo necesita {FF3300}%d{FFFFFF} repuestos.",repuestos);
										Mensaje(playerid,-1,string);
										MRepuestos[playerid] = repuestos;
										return 1;
									}
									if(autos3[i] == autoid)
									{
										new repuestos = PlayerInfo[i][v3Precio]/5000;
										format(string,256,"Este vehículo necesita {FF3300}%d{FFFFFF} repuestos.",repuestos);
										Mensaje(playerid,-1,string);
										MRepuestos[playerid] = repuestos;
										return 1;
									}
									if(autos4[i] == autoid)
									{
										new repuestos = PlayerInfo[i][v4Precio]/5000;
										format(string,256,"Este vehículo necesita {FF3300}%d{FFFFFF} repuestos.",repuestos);
										Mensaje(playerid,-1,string);
										MRepuestos[playerid] = repuestos;
										return 1;
									}
