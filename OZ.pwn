			cache_get_value_name(0, "razon", ActionQueue[i][Razon]);

			printf("Query: type: %d | ID: %d | FaccJ: %d | Status: %d | TiempoJail: %d | UserSes: %s | JugName: %s | queue: %s | razon: %s", type,ActionQueue[i][qID],ActionQueue[i][qfaccj],ActionQueue[i][qstatus],ActionQueue[i][TiempoJailW],ActionQueue[i][quser_ses],ActionQueue[i][qjugname],ActionQueue[i][qparams],ActionQueue[i][Razon]);

			if(ActionQueue[i][qstatus] == 0)
			{
				new qString[128];
				switch(type)
				{
					case 1: // Aceptar invitacion
					{
						ActionQueue[i][qstatus] = 1;
						format(qString, 128, "jugadorinvitacion %d %s", ActionQueue[i][qfaccj], ActionQueue[i][quser_ses]);
						CustomProc(qString);
						printf("OQF: %s", qString);
					}
					case 2: // Cambiar de rango
					{
						if(!strcmp(ActionQueue[i][qparams], "nada", true))
						{
							ActionQueue[i][qstatus] = 2;
						}
						new gRank;
						sscanf(ActionQueue[i][qparams], "p<|>d", gRank);
						format(qString, 128, "gMemberRank %d %d %s %s", ActionQueue[i][qfaccj], gRank, ActionQueue[i][quser_ses], ActionQueue[i][qjugname]);
						CustomProc(qString);
						printf("OQF: %s", qString);
					}
					case 3: // Expulsar
					{
						ActionQueue[i][qstatus] = 1;
						format(qString, 128, "expulsarmiembro %d %s %s", ActionQueue[i][qfaccj], ActionQueue[i][quser_ses], ActionQueue[i][qjugname]);
						CustomProc(qString);
						printf("OQF: %s", qString);
					}
					case 4: // Abandonar banda
					{
						ActionQueue[i][qstatus] = 1;
						format(qString, 128, "abandonarbanda %d %s", ActionQueue[i][qfaccj], ActionQueue[i][quser_ses]);
						CustomProc(qString);
						printf("OQF: %s", qString);
					}
					case 5: // Cargar banda
					{
						ActionQueue[i][qstatus] = 1;
						format(qString, 128, "recargarbandas");
						CustomProc(qString);
						printf("OQF: %s", qString);
					}
					case 6: // Cambiar nombre rango
					{
						ActionQueue[i][qstatus] = 1;
						format(qString, 128, "gRankName %d %s %s %s", ActionQueue[i][qfaccj], ActionQueue[i][quser_ses], ActionQueue[i][qparams], ActionQueue[i][qjugname]);
						CustomProc(qString);
						printf("OQF: %s", qString);
					}
					case 7: // Sancion Web
					{
						ActionQueue[i][qstatus] = 1;
						format(qString, 128, "sancionar %s %d %s %s", ActionQueue[i][qjugname], ActionQueue[i][TiempoJailW], ActionQueue[i][quser_ses], ActionQueue[i][Razon]);
						CustomProc(qString);
						printf("OQF: %s", qString);
					}
					case 8: // Baneo Web
					{
						ActionQueue[i][qstatus] = 1;
						format(qString, 128, "ban %s %s %s", ActionQueue[i][qjugname], ActionQueue[i][quser_ses], ActionQueue[i][Razon]);
						CustomProc(qString);
						printf("OQF: %s", qString);
					}
				}
				mysql_format(Conecction, queryString, sizeof(queryString), "DELETE FROM `action_queue` WHERE `ID` = '%d'", ActionQueue[i][qID]);
				mysql_pquery(Conecction, queryString);
				printf("Delete: %s", queryString);
			}
		}
	}
	return 1;
}

