{
	if(JugadorLogeado[playerid] == 1)
	{
		new autoid = GetPlayerVehicleID(playerid);
		if(IsPlayerInAnyVehicle(playerid))
		{
			if(PlayerInfo[playerid][jAdminNvl] > ADMIN_LVL_JUEGO2) return 1;
			if(EnAuto[playerid] != autoid)
			{
				new string[128];
				if(BajandoV[playerid] == 1) return 1;
				if(EnCarreras[playerid] > 0) return 1;
				if(MotoCross[playerid] > 0) return 1;
				if(MOTO(autoid) && GetPlayerState(playerid) == PLAYER_STATE_PASSENGER) return 1;
				format(string,sizeof(string),"%s(ID:%d) posible cheater, sube sin animación a los vehículos (1) (Nivel:%d - %dhs)",NombreJugador(playerid),playerid,PlayerInfo[playerid][jNivel],PlayerInfo[playerid][horasjugadas]);
				MensajeAdmin(COLOR_YELLOW, string, 3);
				ExpulsarJugador(playerid, "Posible cheater de auto");
			}
		}
	}
	return 1;
}

function Conteos(Tiempo)
{
	if(Tiempo >= 1)
	{
		new Mensajes[6];
		format(Mensajes, sizeof(Mensajes), "~r~%d", Tiempo);
		foreach(new i : Player)
		{
			if(IsPlayerConnected(i)) GameTextForPlayer(i, Mensajes, 1000, 3);
		}
		SetTimerEx("Conteos", 1000, false, "i", Tiempo-1);
	}
	else
	{
		foreach(new i : Player)
		{
			if(IsPlayerConnected(i)) GameTextForPlayer(i, "~g~YA!", 1000, 3);
			EnConteo = false;
		}
	}
	return 1;
}

stock IsNumeric(const str[])
{
	for(new i, len = strlen(str); i < len; i++)
	{
		if(!('0' <= str[i] <= '9')) return false;
	}
	return true;
}

