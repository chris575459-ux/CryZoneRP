function Recolectar(playerid)
{
	new item = random(16);
	switch(item)
	{
		case 0:
		{
			Mensaje(playerid,-1,"Has encontrado {00FF00}50gr de crack{FFFFFF} y le diste la mitad a tu compañero.");
			Mensaje(Companero[playerid],-1,"Tu compañero encontró {F00000}50gr de crack{FFFFFF} y te dio la mitad.");
			PlayerInfo[playerid][pCrack] += 25;
			PlayerInfo[Companero[playerid]][pCrack] += 25;
		}
		case 3:
		{
			Mensaje(playerid,-1,"Has encontrado un {00CC00}par de manoplas{FFFFFF} y le entregas una a tu compañero.");
			Mensaje(Companero[playerid],-1,"Tu compañero ha encontrado un {00CC00}par de manoplas{FFFFFF} y te entregó una.");
			DarArma(playerid,1);
			DarArma(Companero[playerid],1);
		}
		case 6:
		{
			Mensaje(playerid,-1,"Has encontrado una billetera con {00CC00}$20.000{FFFFFF} y le diste la mitad a tu compañero.");
			Mensaje(Companero[playerid],-1,"Tu compañero encontró una billetera con {00CC00}$20.000{FFFFFF}, y te dio la mitad.");
			DarDinero(playerid,10000);
			DarDinero(Companero[playerid],10000);
		}
		case 9:
		{
			Mensaje(playerid,-1,"Has encontrado {00CC00}50 medicamentos{FFFFFF} y le diste la mitad a tu compañero.");
			Mensaje(Companero[playerid],-1,"Tu compañero encontró {00CC00}50 medicamentos{FFFFFF} y te dio la mitad.");
			PlayerInfo[playerid][pMedicamentos] += 25;
			PlayerInfo[Companero[playerid]][pMedicamentos] += 25;
		}
		case 12:
		{
			Mensaje(playerid,-1,"Has encontrado {00CC00}12 diamantes{FFFFFF} y le diste la mitad a tu compañero.");
			Mensaje(Companero[playerid],-1,"Tu compañero encontró {00CC00}12 diamantes{FFFFFF} y te dio la mitad.");
			PlayerInfo[playerid][Moneda] += 6;
			PlayerInfo[Companero[playerid]][Moneda] += 6;
		}
		case 15:
		{
			Mensaje(playerid,-1,"Has encontrado {00FF00}50gr de marihuana{FFFFFF} y le diste la mitad a tu compañero.");
