function LlamarHospital(playerid)
{
	if(EstaEn(playerid,1.0,1147.9966,-1322.4128,-10.7922) || EstaEn(playerid,1.0,-2713.9946,636.8887,1385.6547) || EstaEn(playerid,1.0,1242.1232,-809.9145,1164.2759))
	{
		if(PlayerInfo[playerid][pCama] > 0) return 1;
		new Float:Vida,Float:calculo,string[256];
		GetPlayerHealth(playerid,Vida);
		if(Vida > 99) return Mensaje(playerid,-1,"Tu no necesitas este servicio en este momento.");
		calculo = 100-Vida;
		HCalculo[playerid] = floatround(calculo, floatround_round);
		format(string,256,"Bienvenido al hospital Central de Los Santos\n\nNecesitas una revisión medica\n\nCosto de la revisión: {00CC00}$%s",ConvertirNumero(HCalculo[playerid]*10));
		ShowPlayerDialog(playerid,DHOSPITAL,DIALOG_STYLE_MSGBOX,"Servicio Médico",string,"Aceptar","Cancelar");
	}
	else if(PlayerInfo[playerid][Agonizando] == 1 && LlamadaMedicos[playerid] == 0)
	{
		for(new i, j = GetPlayerPoolSize(); i <= j; i++)
		{
			if(RopaMedi[i] == 1 && PlayerInfo[i][jJob] == MEDICO || PlayerInfo[i][jJob2] == MEDICO && RopaMedi[i] == 1)
			{
				SetPlayerMarkerForPlayer(i, playerid, COLOR_AMARILLO);
				Mensaje(i, 0x00C600FF, "{FFFFFF}Hay un nuevo paciente solicitando {FFFF00}/servicio medico{FFFFFF}, para avisarle que vas a atenderlo usa {FFFF00}/responder medico");
				SetTimerEx("CancelarMedi",minutos(5),false,"d",playerid);
				ResponderMedi[i] = 1;
			}
		}
		Mensaje(playerid, -1,"Llamaste al servicio medico, por favor espera un momento.");
		MarcadoMedicos[playerid] = 1;
		LlamadaMedicos[playerid] = 1;
	}
	else Mensaje(playerid, -1,"{FFFFFF}Para usar {DBED15}/servicio medico{FFFFFF}, tienes que estar herido o dentro de un hospital.");
	return 1;
}

function LlamarMecanicos(playerid)
{
	if(AceptoRepara[playerid] == 1) return Mensaje(playerid,-1,"Ya aceptaste una reparación.");
	new Float:vida, autoid = GetPlayerVehicleID(playerid);
	GetVehicleHealth(autoid, vida);
	if(IsPlayerInAnyVehicle(playerid) && GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
	{
		if(vida > 375.0) return Mensaje(playerid,-1,"El motor del vehículo no está averiado.");
		if(Capo[autoid] == 0) return Mensaje(playerid,-1,"Primero necesitas {FFFF00}/abrir capo{FFFFFF}.");
		if(LlamadaMeca[playerid] == 0)
		{
			for(new i, j = GetPlayerPoolSize(); i <= j; i++)
			{
				if(RopaMeca[i] == 1 && PlayerInfo[i][jJob] == MECANICO || PlayerInfo[i][jJob2] == MECANICO)
				{
					SetPlayerMarkerForPlayer(i, playerid, COLOR_AZUL);
					Mensaje(i, 0x33CCFFAA, "{FFFFFF}Hay un nuevo cliente llamando al {FFFF00}/servicio mecanico{FFFFFF}, para confimar usa {FFFF00}/responder mecanico");
					ResponderMeca[i] = 1;
					SetTimerEx("CancelarMeca",minutos(5),false,"d",playerid);
				}
			}
			Mensaje(playerid, -1,"Has solicitado un mecanico, espera que venga a tu posición.");
			LlamadaMeca[playerid] = 1;
		}
		else return Mensaje(playerid,-1,"Ya has solicitado un mecánico.");
	}
	else Mensaje(playerid,-1,"Debes estar en un vehículo para solicitar un mecanico.");
	return 1;
}

