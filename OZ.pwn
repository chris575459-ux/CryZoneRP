		if(isnull(params)) return Mensaje(playerid, -1, "Usa: /nombre [Nombre Temporal]");
		if(!strcmp(params, "Hahn", true)) return Mensaje(playerid, -1, "No puedes utilizar este nombre.");
		if(!strcmp(params, "Kevin_Mcfly", true)) return Mensaje(playerid, -1, "No puedes utilizar este nombre.");
		format(string, sizeof(string), "%s, estas usando temporalmente el nombre: %s.", NombreJugador(playerid), params);
		Mensaje(playerid, -1, string);
		Log("logs/nombres.log", string);
		PlayerTemp[playerid][pt_TempName] = true;

		strcpy(PlayerTemp[playerid][pt_NombreTemp], NombreJugador(playerid));
		SetPlayerName(playerid, params);
	}
	else
	{
		format(string, sizeof(string), "Tu nombre a sido cambiado al original: %s", PlayerTemp[playerid][pt_NombreTemp]);
		Mensaje(playerid, -1, string);
		PlayerTemp[playerid][pt_TempName] = false;
		SetPlayerName(playerid, PlayerTemp[playerid][pt_NombreTemp]);
	}
	return 1;
}

CMD:entraradm(playerid,params[])
{
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_ADM) return 0;
	for(new c = 1; c < MAX_PROPIEDADES; c++)
	{
		if(PropInfo[c][pEnUso] == 1)
		{
			if(EstaEn(playerid,1.0,PropInfo[c][pPosX],PropInfo[c][pPosY],PropInfo[c][pPosZ])) // Puerta
			{
				if(GetPlayerVirtualWorld(playerid) == 0)
				{
					SetPlayerPos(playerid,PropInfo[c][pPosXS],PropInfo[c][pPosYS],PropInfo[c][pPosZS]);
					SetPlayerFacingAngle(playerid,PropInfo[c][pPosAS]);
					SetPlayerInterior(playerid,PropInfo[c][pInteriorID]);
					SetPlayerVirtualWorld(playerid,PropInfo[c][pVW]);
					CargandoEx(playerid);
					//
					if(PropInfo[c][pEmisora] != 0)
					{
						for(new id, j = GetPlayerPoolSize(); id <= j; id++)
						{
							if(Emisoras[id][CabinaOcupada] == PropInfo[c][pEmisora])
							{
								if(Emisoras[id][TransEmisora] == 1)
								{
									new stringa[128];
									format(stringa, sizeof(stringa), "{FFFFFF}Escuchando radio {DBED15}%s{FFFFFF}.", Emisoras[id][NombreEmisora]);
									Mensaje(playerid, COLOR_BLANCO, stringa);
									format(stringa, sizeof(stringa),"http://radio.omegazone.net:8000/listen.pls?sid=%d", Emisoras[id][EmisoraID]);
									if(ShowSound[playerid] == 1) PlayAudioStreamForPlayer(playerid, stringa);
								}
							}
						}
					}
				}
			}
			if(EstaEn(playerid,2.5,PropInfo[c][pRoperoX],PropInfo[c][pRoperoY],PropInfo[c][pRoperoZ]) && GetPlayerVirtualWorld(playerid) == PropInfo[c][pVW]) // Ropero
			{
				if(PropInfo[c][pRoperoAbierto] == 0)
				{
					new query[80];
					format(query,sizeof(query),"%s abre el ropero, entra y lo cierra rápidamente.",NombreJugador(playerid));
					ProxDetector(10.0, playerid, query, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
				}
				SetPlayerPos(playerid, 254.2685, -41.5420, 1002.0308);
				SetPlayerFacingAngle(playerid, 267.0000);
				SetPlayerInterior(playerid, 14);
			}
			if(EstaEn(playerid,4.5,PropInfo[c][pGX],PropInfo[c][pGY],PropInfo[c][pGZ]) && PropInfo[c][pGX] != 0) // Garaje
			{
				if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
				{
					CargandoEx(playerid);
					if(PropInfo[c][pInterior] == 43) SetPosVeh(GetPlayerVehicleID(playerid), 1403.4918, 597.7753, 1001.0373, 177.8408, 99, PropInfo[c][pVW]);
					else SetPosVeh(GetPlayerVehicleID(playerid), 303.7281, 306.4251, 999.2913, 0, 4, PropInfo[c][pVW]);
				}
				if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT)
				{
				    if(PropInfo[c][pInterior] == 43) SetPosEx(playerid, 1403.4918, 597.7753, 1001.0373, 177.8408, 99, PropInfo[c][pVW]);
					else SetPosEx(playerid, 303.7281, 306.4251, 999.2913, 0, 4, PropInfo[c][pVW]);
				}
			}
		}
	}
	return true;
}

CMD:ubicar(playerid, params[])
{
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_ADM) return 0;
	if(!sscanf(params, "u", params[0]))
	{
		if(!IsPlayerConnected(params[0])) return Mensaje(playerid, -1, "La ID es inválida.");
		new Float:X,Float:Y,Float:Z, zone[MAX_ZONE_NAME],string[256];
		GetPlayer2DZone(params[0], zone, MAX_ZONE_NAME);
		GetPlayerPos(params[0], X,Y,Z);
		SetPlayerCheckpoint(playerid, X,Y,Z, 6);
		format(string, sizeof(string), "[Busqueda]{FFFFFF} {00C7FF}%s{FFFFFF}(%d) fue visto por ultimas ves en %s.", NombreJugador(params[0]), params[0], zone);
		Mensaje(playerid, COLOR_YELLOW, string);
	}
	else Mensaje(playerid, -1, "Utilize: /ubicar [Nombre_Jugador/ID]");
	return 1;
}

