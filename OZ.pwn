			CompradorPiezas[playerid] = -1;
			PiezasAVender[playerid] = 0;
			PrecioPiezas[playerid] = 0;
			TiempoVentaPiezas[playerid] = 0;
		}
		if(strcmp(item,"arma",true) == 0)
		{
			if(!PuedeHoras(playerid)) return 1;
			if(!PlayerConectado(OfrecioArma[playerid])) return Mensaje(playerid,-1,"El jugador que te ofrecio un arma está desconectado.");
			if(OfrecioArma[playerid] == -1) return Mensaje(playerid,-1,"Nadie te ofreció un arma.");
			if(IsPlayerInAnyVehicle(OfrecioArma[playerid])) return Mensaje(playerid,-1,"La persona que te ha ofrecido el arma no puede estar en un vehículo.");
			if(IsPlayerInAnyVehicle(playerid)) return Mensaje(playerid,-1,"No puedes aceptar un arma si estas en un vehículo.");
			if(!EstaEnP(playerid,OfrecioArma[playerid],7)) return Mensaje(playerid,-1,"Debes estar cerca del jugador que te ofreció el arma.");
			if(PlayerInfo[playerid][jDinero] < PrecioArma[playerid]) return Mensaje(playerid,-1,"No tienes suficiente dinero para aceptar el arma.");
			if(!SiTieneArma(OfrecioArma[playerid],ArmaOfrecida[playerid])) return Mensaje(playerid,-1,"El jugador que te ofreció el arma ya no la tiene.");
			new count;
			if(TiempoVentaArmas[OfrecioArma[playerid]] < gettime()) return Mensaje(playerid,-1,"El vendedor se olvidó que te iba a vender.");
			for(new i, j = GetPlayerPoolSize(); i <= j; i++)
			{
				if(PlayerConectado(i) && OfrecioArma[i] == OfrecioArma[playerid] && i != playerid) count ++;
			}
			if(count != 0) return Mensaje(playerid,-1,"* El vendedor se olvidó que te iba a vender.");
			new string[70];
			format(string,sizeof(string),"%s recibió el arma de %s.",NombreJugador(playerid),NombreJugador(OfrecioArma[playerid]));
			ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
