			DarObjeto(playerid, 1550);
		}
	}
	else if(strcmp(op,"armas",true) == 0)
	{
		if(GrupoRobo[playerid] != 3) return 1;
		if(AceptacionBlindado[playerid] != 2) return 1;
		if(!EstaEn(playerid,3,1424.2625,491.3984,1007.3831)) return 1;
