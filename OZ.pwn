			if(PlayerInfo[playerid][jJob2] == ID)
			{
				format(string,128,"Has abandonado el trabajo de {DBED15}%s", NombreTrabajo(PlayerInfo[playerid][jJob2]));
				Mensaje(playerid, -1, string);
				if(PlayerInfo[playerid][jJob2] == MEDICO)	{ RopaMedi[playerid] = 0; }
				if(PlayerInfo[playerid][jJob2] == BASURERO)	{ RopaBasu[playerid] = 0; }
				if(PlayerInfo[playerid][jJob2] == MECANICO)	{ RopaMeca[playerid] = 0; }
				if(PlayerInfo[playerid][jJob2] == MINERO)	{ RopaMine[playerid] = 0; }
				PlayerInfo[playerid][jJob2] = 0;
			}
		}
	}
	return 1;
}

