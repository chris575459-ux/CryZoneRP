CMD:reanimar(playerid,params[])
{
	new ID;
	if(IsPlayerInAnyVehicle(playerid)) return Mensaje(playerid, COLOR_BLANCO, "No puedes hacer esto en un vehículo.");
	if(PlayerInfo[playerid][jJob] != MEDICO && PlayerInfo[playerid][jJob2] != MEDICO) return 1;
	if(sscanf(params,"d",ID)) return Mensaje(playerid, -1,"Utiliza {DBED15}/reanimar [ID]");
	if(!IsPlayerConnected(ID)) return Mensaje(playerid, -1,"Jugador desconectado.");
	if(MarcadoMedicos[ID] != 1) return Mensaje(playerid, -1,"Este jugador no ha solicitado un médico.");
	if(!EstaEn(playerid, 5.0, DeathXH[ID],DeathYH[ID],DeathZH[ID])) return Mensaje(playerid, -1,"Debes estar cerca de un paciente para reanimarlo.");
	{
		Mensaje(playerid, COLOR_DORADO,"Reanimando...");
		LoopingAnim(playerid, "MEDIC", "CPR", 4.0, 0, 0, 0, 0, 0);
		SetTimerEx("OcultarAnim",8000,false,"d",playerid);
		EstablecerVida(ID, 100.0);
		Reanimado[ID] = 1;
		Paciente[playerid] = ID;
	}
	return 1;
}

