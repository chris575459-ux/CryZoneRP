public OnEnterExitModShop(playerid, enterexit, interiorid)
{
	#if DEBUG_GM == 1
	printf("(OnEnterExitModShop) PlayerID: %d | EnterExit: %d", playerid, enterexit);
	#endif
	if(!enterexit)
	{
		if(IsPlayerInVehicle(playerid, autos1[playerid])) GuardarAuto1(playerid);
		else if(IsPlayerInVehicle(playerid, autos2[playerid])) GuardarAuto2(playerid);
		else if(IsPlayerInVehicle(playerid, autos3[playerid])) GuardarAuto3(playerid);
		else if(IsPlayerInVehicle(playerid, autos4[playerid])) GuardarAuto4(playerid);
	}
	return 1;
}

