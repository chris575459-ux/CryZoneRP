CMD:cancelar(playerid, params[])
{
	new item[32];
	new vw = GetPlayerVirtualWorld(playerid);
	if(IsPlayerConnected(playerid))
	if(!sscanf(params, "s[32]", item))
	{
		if(strcmp(item,"mutual",true) ==0 || strcmp(item,"socio",true) ==0)
		{
			if(EstaEn(playerid, 5.0, 1147.9966,-1322.4128,-10.7922) || EstaEn(playerid, 5.0, -2713.9946,636.8887,1385.6547) || EstaEn(playerid, 5.0, 1242.1232,-809.9145,1164.2759))
			{
				switch(PlayerInfo[playerid][pSocioHP])
				{
					case 0:
					{
						Mensaje(playerid,-1,"{FFFFFF}No eres socio de este hospital.");
					}
					case 1: // Central
					{
						if(vw != 6) return Mensaje(playerid, -1, "{FFFFFF}No eres socio de este hospital.");
						PlayerInfo[playerid][pSocioHP] = 0;
						Mensaje(playerid,-1,"{c04306}Dejaste de ser socio de este hospital.");
					}
					case 2: // Grove
					{
						if(vw != 9) return Mensaje(playerid, -1, "{FFFFFF}No eres socio de este hospital.");
						PlayerInfo[playerid][pSocioHP] = 0;
						Mensaje(playerid,-1,"{c04306}Dejaste de ser socio de este hospital.");
					}
					case 3: // SF
					{
						if(vw != 7) return Mensaje(playerid, -1, "{FFFFFF}No eres socio de este hospital.");
						PlayerInfo[playerid][pSocioHP] = 0;
						Mensaje(playerid,-1,"{c04306}Dejaste de ser socio de este hospital.");
					}
					case 4: // LV
					{
						if(vw != 8) return Mensaje(playerid, -1, "{FFFFFF}No eres socio de este hospital.");
						PlayerInfo[playerid][pSocioHP] = 0;
						Mensaje(playerid,-1,"{c04306}Dejaste de ser socio de este hospital.");
					}
				}
			}
			else return Mensaje(playerid, -1, "{FFFFFF}Tienes que ir al hospital para cancelar la mutual.");
		}
	}
	return 1;
}

