CMD:estereo(playerid, params[])
{
	if(!IsPlayerInAnyVehicle(playerid)) return Mensaje(playerid, COLOR_BLANCO, "Necesitas estar en un vehículo.");
	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return Mensaje(playerid, COLOR_BLANCO, "No eres el conductor de este vehículo.");
	new vehicleid, string[128];
	vehicleid = GetPlayerVehicleID(playerid);
	if(strcmp(params,"off",true) == 0)
	{
		EmisoraEscuchando[GetPlayerVehicleID(playerid)] = -1;
		format(string, sizeof(string), "%s apaga el estéreo.", NombreJugador(playerid));
		ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
		for(new i, j = GetPlayerPoolSize(); i <= j; i++)
		{
			if(!IsPlayerConnected(i) || IsPlayerNPC(i))
			continue;
			if(vehicleid == GetPlayerVehicleID(i))
			{
				StopAudioStreamForPlayer(i);
			}
		}
	}
	return 1;
}

