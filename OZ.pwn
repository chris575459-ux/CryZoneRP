function Maneja(playerid)
{
	Manejando[playerid] = 0;
}

function DespuesSlap(playerid, to_player)
{
	new Float:health, Float:armour, szMessage[150];
	GetPlayerHealth(to_player, health);
	GetPlayerArmour(to_player, armour);
	format(szMessage, sizeof(szMessage), "(2) %s[%d] = (Vida: %f) - (Chaleco: %f) - (Ping: %d) - (Packets lost: %.2f)", NombreJugador(to_player), to_player, health, armour, GetPlayerPing(playerid), NetStats_PacketLossPercent(playerid));
	Mensaje(playerid, TEAM_BLUE_COLOR, szMessage);
	to_player = -1;
	return 1;
}

function DArmas(playerid)
{
	DarArmas(playerid);
	return 1;
}

function CheckAuto(playerid)
{
	if(!IsPlayerInAnyVehicle(playerid))
	{
		EnAuto[playerid] = 0;
	}
}

