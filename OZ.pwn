public OnPlayerKeyStateChange(playerid, newkeys, oldkeys)
{
	// Trabar y destrabar con ESPACIO + Y
	if((newkeys & KEY_SPRINT && newkeys & KEY_YES && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT) || (newkeys & KEY_HANDBRAKE && newkeys & KEY_YES && IsPlayerInAnyVehicle(playerid) == 1))
	{
		new Float:x,Float:y,Float:z,Float:x2,Float:y2,Float:z2,Float:x3,Float:y3,Float:z3,Float:x4,Float:y4,Float:z4;
		GetVehiclePos(autos1[playerid], x,y,z);
		GetVehiclePos(autos2[playerid], x2,y2,z2);
		GetVehiclePos(autos3[playerid], x3,y3,z3);
		GetVehiclePos(autos4[playerid], x4,y4,z4);
		if(EstaEn(playerid, 10.0, x,y,z))
		{
			if(PlayerInfo[playerid][vTraba] == 0) return Mensaje(playerid, COLOR_BLANCO, "Para poder abrir el vehículo, necesitas instalar un sistema de traba. Puedes comprar uno en cualquier {DBED15}24/7");
			if(bloqueado[autos1[playerid]] == 0) CerrarAuto1(playerid);
			else if(bloqueado[autos1[playerid]] == 1) AbrirAuto1(playerid);
		}
		if(EstaEn(playerid, 10.0, x2,y2,z2))
		{
			if(PlayerInfo[playerid][v2Traba] == 0) return Mensaje(playerid, COLOR_BLANCO, "Para poder abrir el vehículo, necesitas instalar un sistema de traba. Puedes comprar uno en cualquier {DBED15}24/7");
			if(bloqueado[autos2[playerid]] == 0) CerrarAuto2(playerid);
			else if(bloqueado[autos2[playerid]] == 1) AbrirAuto2(playerid);
		}
		if(EstaEn(playerid, 10.0, x3,y3,z3))
		{
			if(PlayerInfo[playerid][v3Traba] == 0) return Mensaje(playerid, COLOR_BLANCO, "Para poder abrir el vehículo, necesitas instalar un sistema de traba. Puedes comprar uno en cualquier {DBED15}24/7");
			if(bloqueado[autos3[playerid]] == 0) CerrarAuto3(playerid);
			else if(bloqueado[autos3[playerid]] == 1) AbrirAuto3(playerid);
		}
		if(EstaEn(playerid, 10.0, x4,y4,z4))
		{
			if(PlayerInfo[playerid][v4Traba] == 0) return Mensaje(playerid, COLOR_BLANCO, "Para poder abrir el vehículo, necesitas instalar un sistema de traba. Puedes comprar uno en cualquier {DBED15}24/7");
			if(bloqueado[autos4[playerid]] == 0) CerrarAuto4(playerid);
			else if(bloqueado[autos4[playerid]] == 1) AbrirAuto4(playerid);
		}
		return 1;
	}
	// Arrancar y detener con 2
	if((newkeys & KEY_LOOK_BEHIND) && (GetPlayerState(playerid) == PLAYER_STATE_DRIVER))
	{
	    new vehicleid = GetPlayerVehicleID(playerid);
	    if(VehiculosInfo[vehicleid][vcomprable] == 1) return 1;
		if(MotorAuto[vehicleid] == 0)
		{
		    MotorAuto[vehicleid] = 3;
			Mensaje(playerid, COLORCARGO0, "Arrancando vehículo...");
			SetTimerEx("EncenderMotor",1200, false, "d",playerid);
		}
		else if(MotorAuto[vehicleid] == 1)
		{
			new string[70];
			format(string, 70, "%s detuvo el motor de su vehículo.",NombreJugador(playerid));
			GetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);
			SetVehicleParamsEx(vehicleid, 0, 0, alarm, doors, bonnet, boot, objective);
			ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
			MotorAuto[vehicleid] = 0;
			Luces[vehicleid] = 0;
		}
		else return 1;
	}
	// Cargos SAPD con NUM2
	if(newkeys & KEY_ANALOG_DOWN && PlayerInfo[playerid][pDuty] == 1 && IsPlayerInAnyVehicle(playerid) && SAPD(GetPlayerVehicleID(playerid)))
	{
		if(gettime() > TiempoCompu[playerid])
		{
			TiempoCompu[playerid] = gettime() + 60;
			new count,string[250];
			format(string,sizeof(string),"%s inició la búsqueda de sospechosos en la computadora de la patrulla.",NombreJugador(playerid));
			ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
			ReportCrimen(playerid,4);
			for(new i, j = GetPlayerPoolSize(); i <= j; i++)
			{
				if(PlayerConectado(i) && EstaEnP(playerid,i,20.0) && PlayerInfo[i][pCargos] < 3 && PlayerInfo[i][jMiembro] != 1)
				{
					if((SiTieneArma(i,25) || SiTieneArma(i,27) || SiTieneArma(i,29) || SiTieneArma(i,30) || SiTieneArma(i,31) || SiTieneArma(i,33)) && (MOTO(GetPlayerVehicleID(i)) || !IsPlayerInAnyVehicle(i)))
					{
						format(string, sizeof(string), "{3399FF}[Patrulla de %s]{FFFFFF} Resultado: Se detectó a %s armado, proceder con precaución.",NombreJugador(playerid),NombreJugador(i));
						SendRadioMessage(1,-1,string,INVALID_PLAYER_ID, 1);
						PlayerInfo[i][pCargos] = 3;
						SetPlayerWantedLevel(i,3);
						count ++;
					}
				}
			}
			if(count == 0)
			{
				format(string, sizeof(string), "{3399FF}[Patrulla de %s]{FFFFFF} Resultado: No se detectaron sospechosos.", NombreJugador(playerid));
 				SendRadioMessage(1,-1,string,INVALID_PLAYER_ID, 1);
			}
		}
		else MensajeEx(playerid,-1,"Por favor espera {FF3300}%d{ffffff} segundos.", TiempoCompu[playerid] - gettime());
	}
	//
	if(newkeys & KEY_CTRL_BACK && Muriendo[playerid] == 1)
