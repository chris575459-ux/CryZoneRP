/* ANTICHEAT DE REPAIR CAR */

function OnVehicleHealthChange(vehicleid,Float:newhealth,Float:oldheatlh)
{
	if(ConductorVehiculo(vehicleid) != INVALID_PLAYER_ID && VidaVehiculo[vehicleid] != 0)
	{
		if(VidaVehiculo[vehicleid] < newhealth  && !IsPlayerNPC(ConductorVehiculo(vehicleid)))
		{
			new playerid = ConductorVehiculo(vehicleid);
			if(IsPlayerNPC(playerid)) return true;
			if(!IsPlayerInPaint(playerid) && !IsPlayerInTransfender(playerid))
			{
				if(newhealth > 1000)
				{
					new string[200];
					format(string,sizeof(string),"%s(ID:%d) posible cheater, mas de 1000 de vida en vehículos. (Nivel:%d - %dhs)",NombreJugador(playerid),playerid,PlayerInfo[playerid][jNivel],PlayerInfo[playerid][horasjugadas]);
					MensajeAdmin(COLOR_YELLOW, string, 3);
					ExpulsarJugador(playerid,"Mas de 1000 de vida en vehiculos.");
					return 1;
				}
				new string[250];
				format(string,sizeof(string),"[PRUEBA] REPAIR CAR - Jugador: %s (ID:%d) - Nivel: %d",NombreJugador(playerid),playerid,GetPlayerScore(playerid),VidaVehiculo[vehicleid],newhealth,VehicleVida[vehicleid]);
				MensajeAdmin(COLOR_SERVER, string, 3);
			}
		}
	}
	VidaVehiculo[vehicleid] = newhealth;
	return true;
}

