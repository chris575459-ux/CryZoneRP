CMD:hahnban(playerid, params[])
{
	new string[256], ID, RAZON[31], query[200], day, month, year;
	getdate(year,month,day);
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_JUEGO1) return 0;
	if(sscanf(params, "rs[31]", ID, RAZON) )return Mensaje(playerid,COLORCARGO0,"Uso: /ban [id] [razón]");
	if(GetPlayerScore(ID) == 0) return Mensaje(playerid, -1, "El jugador que quieres banear está conectado pero aún no ha ingresado al juego.");
	if(!IsPlayerConnected(ID)) return Mensaje(playerid,COLORCARGO0,"No se encontró ningún jugador.");
	if(!strcmp(NombreJugador(ID), "Hahn") || !strcmp(NombreJugador(ID), "Kevin_Mcfly")) return Mensaje(playerid,COLORCARGO0,"No puedes usar este comando con ese usuario.");
	{
		if(PlayerInfo[ID][jAdminNvl] >= PlayerInfo[playerid][jAdminNvl])
		{
			format(string, sizeof(string), "%s(%d) fue auto-expulsado. Razón: Intento de baneo a un ADM (%d).",NombreJugador(playerid), playerid, NombreJugador(ID));
			MensajeAdmin(COLOR_GREY, string, 3);
			PlayerInfo[playerid][jAdminNvl] = 0;
			ExpulsarJugador(playerid, "Intento banear a un adm");
			return 1;
		}
		Baneados[playerid] ++;
		if(Baneados[playerid] >= 5 && !strcmp(NombreJugador(playerid),"Hahn",true))
		{
			mysql_format(Conecction, query, sizeof(query),"UPDATE `usuarios` SET `Admin`='0' WHERE `Nombre`='%s'", NombreJugador(playerid));
			mysql_pquery(Conecction,query);
			PlayerInfo[playerid][jAdminNvl] = 0;
			ExpulsarJugador(playerid, "Administrador traidor");
			return true;
		}
		SetTimerEx("SacarBaneadoss",5*1000,0,"d",playerid);

		if(playerid == ID)
		{
			format(string, 128, "%s se autobaneo, razon: %s",NombreJugador(ID), RAZON);
			MensajeAdmin(COLOR_GREY, string, 3);
		}
		else
		{
			format(string, sizeof(string), "El jugador %s(%d) fue baneado por %s. Razón: %s.",NombreJugador(ID),  ID, NombreJugador(playerid), RAZON);
			MensajeAdmin(COLOR_GREY, string, 6);
			//
			//
			//
		}

		//
		//

		mysql_format(Conecction, query, sizeof(query), "INSERT INTO `log_baneos` (Administrador, Baneado, Fecha,Motivo) VALUES ('%d','%d', UNIX_TIMESTAMP(), '%e')", PlayerInfo[playerid][p_DBID], PlayerInfo[ID][p_DBID], RAZON);
		mysql_pquery(Conecction, query);
		mysql_format(Conecction, query, sizeof(query), "UPDATE `usuarios` SET `Baneado`='1',`razon`='%e',`QuienBan`='%s' WHERE `ID`='%d'", RAZON, NombreJugador(playerid), PlayerInfo[ID][p_DBID]);
		mysql_pquery(Conecction, query);
		BanEx(ID, RAZON);
		// Vender CASA
		if(PlayerInfo[ID][CasaID] > 0 || PlayerInfo[ID][CasaID2] > 0)
		{
			for(new p=1; p < MAX_PROPIEDADES; p++)
			{
				if(PropInfo[p][pEnUso] == 0) break;
				if(PropInfo[p][propiedad_ID] == PlayerInfo[ID][CasaID])
				{
					PlayerInfo[ID][CasaID] = 0;
					mysql_format(Conecction, query, sizeof(query),"UPDATE `usuarios` SET `CasaID`='0' WHERE `ID`='%d'", PlayerInfo[ID][p_DBID]);
					mysql_pquery(Conecction, query);
					format(PropInfo[p][pPropietario],40,"Nadie");
					PropInfo[p][pEnVenta] = 1;
					PropInfo[p][pAbierta] = 1;
					PropInfo[p][pRoperoAbierto] = 0;
					PropInfo[p][pIDPropietario] = 0;
					mysql_format(Conecction, query, sizeof(query),"UPDATE `propiedades` SET `Propietario`='Nadie',`IDPropietario`='0',`EnVenta`='1' WHERE `ID`='%d'", p);
					mysql_pquery(Conecction, query);
					BorrarLabelCasa(p);
					GuardarPropiedad(p);
					CargarPropiedad(p);
				}
				if(PropInfo[p][propiedad_ID] == PlayerInfo[ID][CasaID2])
				{
					PlayerInfo[ID][CasaID2] = 0;
					mysql_format(Conecction, query, sizeof(query),"UPDATE `usuarios` SET `CasaID2`='0' WHERE `ID`='%d'", PlayerInfo[ID][p_DBID]);
					mysql_pquery(Conecction, query);
					format(PropInfo[p][pPropietario],40,"Nadie");
					PropInfo[p][pEnVenta] = 1;
					PropInfo[p][pAbierta] = 1;
					PropInfo[p][pRoperoAbierto] = 0;
					PropInfo[p][pIDPropietario] = 0;
					mysql_format(Conecction, query, sizeof(query),"UPDATE `propiedades` SET `Propietario`='Nadie',`IDPropietario`='0',`EnVenta`='1' WHERE `ID`='%d'", p);
					mysql_pquery(Conecction, query);
					BorrarLabelCasa(p);
					GuardarPropiedad(p);
					CargarPropiedad(p);
				}
			}
		}
		// Vender CASA
		return 1;
	}
}

