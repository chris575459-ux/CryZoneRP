CMD:ocultar(playerid, params[])
{
	if(strcmp(params,"gps",true) == 0)
	{
		if(ViendoGPS[playerid] == 0) return Mensaje(playerid, -1, "Actualmente no estas viendo tu GPS.");
		ViendoGPS[playerid] = 0;
		foreach(new i : Player) PlayerTextDrawDestroy(playerid, jugador[playerid]);
		KillTimer(asdx[playerid]);
		TextDrawHideForPlayer(playerid, MapaGTA);
		TextDrawHideForPlayer(playerid, MapaGTA2);
		TextDrawHideForPlayer(playerid, MapaGTA3);
		TextDrawHideForPlayer(playerid, MapaGTA4);
		TextDrawHideForPlayer(playerid, MapaGTA5);
		TextDrawHideForPlayer(playerid, MapaGTA6);
		if(SedOculta[playerid] == 1)
		{
			BarraHambre(playerid, 1);
			BarraSed(playerid, 1);
		}
		return 1;
	}
	if(strcmp(params,"nombres",true) == 0)
	{
		for(new i, j = GetPlayerPoolSize(); i <= j; i++) ShowPlayerNameTagForPlayer(playerid, i, 0);
		GameTextForPlayer(playerid, "~r~ OCULTAR ~w~NOMBRES", 5000, 5);
		return 1;
	}
	if(strcmp(params,"pantalla",true) == 0)
	{
		if(ViendoPantalla[playerid] == 0) return Mensaje(playerid, -1, "Actualmente no estas viendo ninguna pantalla.");
		ViendoPantalla[playerid] = 0;
		foreach(new i : NPC) PlayerTextDrawDestroy(playerid, tren[playerid][i]);
		KillTimer(asdtren[playerid]);
		TextDrawHideForPlayer(playerid, MapaGTA);
		TextDrawHideForPlayer(playerid, MapaGTA2);
		TextDrawHideForPlayer(playerid, MapaGTA4);
		TextDrawHideForPlayer(playerid, MapaGTA7);
		TextDrawHideForPlayer(playerid, MapaGTA8);
		if(SedOculta[playerid] == 1)
		{
			BarraHambre(playerid, 1);
			BarraSed(playerid, 1);
		}
		DescongelarEx(playerid);
		return 1;
	}
	if(strcmp(params,"sed",true) == 0)
	{
		if(SedOculta[playerid] == 0) return Mensaje(playerid,-1,"La barra de sed ya esta oculta.");
		SedOculta[playerid] = 0;
		BarraHambre(playerid, 2);
		BarraSed(playerid, 2);
		Mensaje(playerid,-1,"Ocultaste la barra de sed, para volver a mostrarla usa {DBED15}/mostrar sed{FFFFFF}.");
		return 1;
	}
	return 1;
}

