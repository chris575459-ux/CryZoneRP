CMD:puertaacademia(playerid)
{
	if(PlayerInfo[playerid][pDuty] == 1 && PlayerInfo[playerid][jMiembro] == 1)
	{
		if(PlayerInfo[playerid][pRango] < 4) return Mensaje(playerid, COLOR_GREY, "Necesitas ser mayor de rango 4.");
