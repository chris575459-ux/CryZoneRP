CMD:cargos(playerid, params[])
{
	new string[148],texto[100],id,cargos;
	if(PlayerInfo[playerid][jMiembro] != 1) return 1;
	if(PlayerInfo[playerid][pDuty] != 1) return 1;
	if(sscanf(params,"rd", id, cargos)) return Mensaje(playerid, -1, "Por favor, usa {DBED15}/cargos [id] [0-6]{FFFFFF}.");
	if(!IsPlayerConnected(id)) return Mensaje(playerid,COLORCARGO0,"No se encontró ningún jugador.");
	if(IsPlayerNPC(id)) return Mensaje(playerid,COLORCARGO0,"No puedes ponerle cargos a un npc.");
	if(PlayerInfo[id][jMiembro] == 1 && cargos != 0) return Mensaje(playerid,-1,"{DBED15}Le estás intentado poner nivel de búsqueda a un policía.");
	if(PlayerInfo[id][pCargos] == cargos) return Mensaje(playerid,-1,"Ese jugador ya tiene ese cargo.");
	if(cargos < 0 || cargos > 6) return Mensaje(playerid,-1,"Por favor, usa {DBED15}/c [id] [0-6]{FFFFFF}.");
	if(!EstaEnP(playerid,id,400)) return Mensaje(playerid,-1,"No puedes ponerle cargos a una persona desde tan lejos.");
	PlayerInfo[id][pCargos] = cargos;
	DarEstrella(id,PlayerInfo[id][pCargos]);
	ReportCrimen(id,cargos);
	if(PlayerInfo[id][jMiembro] == 1) SetPlayerColor(id, TEAM_BLUE_COLOR);

	if(cargos == 0) strcpy(PlayerTemp[id][pt_QuienCargos], "Nadie"), Advertido[id] = 0;
	else strcpy(PlayerTemp[id][pt_QuienCargos], NombreJugador(playerid));

	switch(cargos)
	{
		case 0: MarcarJugador(id,COLORCARGO0,1),format(texto,sizeof(texto),"no es sospechoso, lo dejo libre");
		case 1:
		{
			MarcarJugador(id,COLORCARGO1,1),format(texto,sizeof(texto),"parece sospechoso, lo voy a interrogar");
			Mensaje(id, COLOR_YELLOW, "La policia quiere hablar con usted, si se da a la fuga, sera arrestado.");
		}
		case 2: MarcarJugador(id,COLORCARGO2,1),format(texto,sizeof(texto),"se da a la fuga, empiezo persecución");
		case 3: MarcarJugador(id,COLORCARGO3,1),format(texto,sizeof(texto),"se encuentra armado, prosigan con precaución");
		case 4: MarcarJugador(id,COLORCARGO4,1),format(texto,sizeof(texto),"abrió fuego, necesito refuerzos");
		case 5: MarcarJugador(id,COLORCARGO5,1),format(texto,sizeof(texto),"cometió un asesinato, procedan a disparar");
		case 6: MarcarJugador(id,COLORCARGO6,1),format(texto,sizeof(texto),"es extremadamente peligroso, capturenlo vivo o muerto");
	}
	format(string, sizeof(string), "[Radio] (%s %s) dice: %s %s", RangoPolicia(playerid), NombreJugador(playerid), NombreJugador(id),texto);
	ProxDetector(10.0, playerid, string,-1,ColorChatNormal1,ColorChatNormal2,ColorChatNormal3,ColorChatNormal4);
	format(string, sizeof(string), "[Radio de policía]{FFFFFF} %s reportando: {00CCFF}%s{FFFFFF} [%d] %s. {DBED15}(%d)*", NombreJugador(playerid), NombreJugador(id),id,texto,cargos);
 	SendRadioMessage(1, TEAM_BLUE_COLOR, string,playerid, 1);
   	return 1;
}

CMD:radio(playerid, params[])
{
	new item[4];
	if(PlayerInfo[playerid][pRadio] != 1) return Mensaje(playerid, -1,"Tu no tienes un radio comunicador, puedes comprar uno en un 24/7.");
	if(sscanf(params, "s[4]", item)) return Mensaje(playerid, -1,"Utiliza {DBED15}/radio [on/off]");
	{
		if(strcmp(item,	"on",true) == 0)
		{
			if(EstadoRadio[playerid] == 1) return Mensaje(playerid,-1,"Tu radio ya esta encendida.");
			new string[60];
			format(string, 60, "%s enciende su radio comunicador.", NombreJugador(playerid));
			EstadoRadio[playerid] = 1;
			if(Specteando[playerid] == 0) ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
		}
		if(strcmp(item, "off", true) == 0)
		{
			if(EstadoRadio[playerid] == 0) return Mensaje(playerid,-1,"Tu radio ya esta apagada.");
			new string[60];
			format(string, 60, "%s apagó su radio comunicador.", NombreJugador(playerid));
			EstadoRadio[playerid] = 0;
			if(Specteando[playerid] == 0) ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
		}
	}
