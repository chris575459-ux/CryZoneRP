stock EstaEnP(playerid,id,Float:rang)
{
	new Float:Pos[3];
	GetPlayerPos(id,Pos[0],Pos[1],Pos[2]);
	if(EstaEn(playerid,rang,Pos[0],Pos[1],Pos[2])) return 1;
	return 0;
}

