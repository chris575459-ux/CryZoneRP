				new resta = GetVehicleMaxCombustible(autoid)-VehicleFuel[autoid];
				if(resta > 5)
				{
					VehicleFuel[autoid] += 5;
					QuitarBidon(playerid);
					Mensaje(playerid, -1,"{DBED15}El tanque de combustible fue cargado con el bidón de 5 litros.");
				}
				else if(resta < 5)
				{
					VehicleFuel[autoid] += resta;
					QuitarBidon(playerid);
					Mensaje(playerid, -1,"{DBED15}El tanque de combustible fue cargado con el bidón de 5 litros.");
				}
			}
		}
	}
	return 1;
}

CMD:cubrir(playerid,params[])
{
	new id,precio,Float:x,Float:y,Float:z;
	if(PlayerInfo[playerid][jJob] != GUARDAESPALDAS && PlayerInfo[playerid][jJob2] != GUARDAESPALDAS) return Mensaje(playerid,-1,"Necesitas ser guardaespaldas para utilizar este comando.");
	if(PlayerInfo[playerid][pTiempoCarcel] > 0 || PlayerInfo[playerid][pTiempoJail] > 0 || VariableCaido[playerid] == 1) return Mensaje(playerid,-1,"No puedes usar este comando en este momento.");
	if(EnCombate[playerid] == 1) return Mensaje(playerid,-1,"No puedes hacer esto en combate, espera unos segundos y reintenta.");
	if(!sscanf(params, "dd", id,precio))
	{
		if(IsPlayerConnected(id))
		{
			GetPlayerPos(id, x,y,z);
			if(id == playerid) return 1;
			if(PlayerTemp[playerid][pt_CooldownCubrir] > gettime()) return MensajeEx(playerid,-1,"Debes esperar {FF3300}%d{FFFFFF} segundos para volver a ofrecer protección.", PlayerTemp[playerid][pt_CooldownCubrir] - gettime());
			if(!EstaEn(playerid, 5.0, x,y,z) || Specteando[id] == 1) return Mensaje(playerid, -1, "No estas cerca de este jugador.");
			if(EnCombate[id] == 1) return Mensaje(playerid,-1,"No puedes ofrecer chaleco a un jugador que esta en combate.");
			if(precio >= 400 && precio <= 2000)
