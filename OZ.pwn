public OnPlayerEnterVehicle(playerid, vehicleid, ispassenger)
{
	#if DEBUG_GM == 1
	printf("(OnPlayerEnterVehicle) PlayerID: %d | VehicleID: %d | IsPassenger: %d", playerid, vehicleid, ispassenger);
	#endif
	if(GetPlayerScore(playerid) == 0) return ExpulsarJugador(playerid, "Sube a vehiculo sin nivel");
	if(LogeoCorrecto[playerid] == 0) return ExpulsarJugador(playerid, "Logeo incorrecto (3)");
	//
	new playerState = GetPlayerState(playerid);
	Seatbelt[playerid] = 0;
	if(recogiendo[playerid] > 0)
	{
		RemovePlayerFromVehicle(playerid);
	}
	//Anti super gun
	if(IsPlayerInAnyVehicle(playerid))
	{
		new string[200];
		format(string,sizeof(string),"%s(ID:%d) posible cheater, super gun (3). (Nivel:%d - %dhs)",NombreJugador(playerid),playerid,PlayerInfo[playerid][jNivel],PlayerInfo[playerid][horasjugadas]);
		MensajeAdmin(COLOR_YELLOW, string, 3);
		ExpulsarJugador(playerid, "Posible cheater de super gun (3)");
		return true;
	}
	//Anti subir a vehiculos lejanos
	if(!ispassenger)
	{
		new Float:Vida;
		GetVehicleHealth(vehicleid,Vida);
		VidaVehiculo[vehicleid] = Vida;
		new Float:Pos[3];
		GetVehiclePos(vehicleid,Pos[0],Pos[1],Pos[2]);
		if(GetPlayerDistanceFromPoint(playerid,Pos[0],Pos[1],Pos[2]) > 20 && !VehiculoOcupado(vehicleid))
		{
			new string[130];
			format(string,sizeof(string),"%s(ID:%d) posible cheater, sube a vehículos a larga distancia. (Nivel:%d - %dhs)",NombreJugador(playerid),playerid,PlayerInfo[playerid][jNivel],PlayerInfo[playerid][horasjugadas]);
			MensajeAdmin(COLOR_YELLOW, string, 3);
			ExpulsarJugador(playerid, "Posible cheater de subir a vehículos a larga distancia.");
			return true;
		}
		if(!EstaEn(playerid,1,Pos[0],Pos[1],Pos[2]))
		{
			Entrando[playerid] = gettime() + 1;
		}
	}
	// Anticheat super gun 4
	if(!ispassenger)
	{
		if((GetTickCount()-TiempoCarSpam[playerid]) < 1000)
	 	{
			if(IDcarSpam[playerid] > 0 && IDcarSpam[playerid] != vehicleid)
			{
		  		CountCarSpam[playerid] ++;
				if(CountCarSpam[playerid] >= 5)
			 	{
			 		Cheater[playerid] = 1;
					new string[200];
					format(string,sizeof(string),"%s(ID:%d) posible cheater, super gun (4). (Nivel:%d - %dhs)",NombreJugador(playerid),playerid,PlayerInfo[playerid][jNivel],PlayerInfo[playerid][horasjugadas]);
					MensajeAdmin(COLOR_YELLOW, string, 3);
					ExpulsarJugador(playerid, "Posible cheater de super gun (4)");
				}
			}
		}
	 	TiempoCarSpam[playerid] = GetTickCount();
	 	IDcarSpam[playerid] = vehicleid;
 	}
	/*
	if(!IsValidVehicle(vehicleid))
	{
		new string[128];
		if(PlayerInfo[playerid][jAdminNvl] >= ADMIN_LVL_JUEGO2) return 1;
		format(string,sizeof(string),"%s(ID:%d) posible cheater, sube a un vehículo inválido (Nivel:%d - %dhs)",NombreJugador(playerid),playerid,PlayerInfo[playerid][jNivel],PlayerInfo[playerid][horasjugadas]);
		for(new i, j = GetPlayerPoolSize() ;i <= j;i++)
		{
			if(PlayerInfo[i][jAdminNvl] >= ADMIN_LVL_JUEGO1 && VerAdmin[i] == 1) Mensaje(i,COLOR_YELLOW,string);
		}
		ExpulsarJugador(playerid, "Cheat de vehiculos (3)");
	}
	*/
	if(!ispassenger)
	{
		if((GetTickCount()-PlayerTemp[playerid][pt_CarTime]) < 1000)
	 	{
			if(PlayerTemp[playerid][pt_CarID] > 0 && PlayerTemp[playerid][pt_CarID] != vehicleid)
			{
		  		PlayerTemp[playerid][pt_CarSpam]++;
				if(PlayerTemp[playerid][pt_CarSpam] >= 3)
			 	{
					new string2[100];
					format(string2,sizeof(string2),"%s(ID:%d) posible cheater de CarSPAM (Nivel:%d - %dhs)",NombreJugador(playerid),playerid,PlayerInfo[playerid][jNivel],PlayerInfo[playerid][horasjugadas]);
					MensajeAdmin(COLOR_YELLOW, string2, 3);
					ExpulsarJugador(playerid, "Posible cheater de CarSPAM");
				}
			}
		}
		PlayerTemp[playerid][pt_CarTime] = GetTickCount();
	 	PlayerTemp[playerid][pt_CarID] = vehicleid;
		EntrandoAV[playerid] = vehicleid;
		TimerSiEntro[playerid] = SetTimerEx("VersiEntro",3500,0,"d",playerid);
		new congelar;
		for(new i, j = GetPlayerPoolSize(); i <= j; i++)
		{
			if(IsPlayerInVehicle(i, vehicleid) && GetPlayerState(i) == PLAYER_STATE_DRIVER || EntrandoAV[i] == vehicleid && i != playerid)
			{
				if(!ispassenger)
				{
					congelar = 1; break;
				}
			}
		}
		if(congelar == 1 && CJ[playerid] == 0)
		{
			CJ[playerid] = 1;
			RemovePlayerFromVehicle(playerid);
			new Float:sx, Float:sy, Float:sz;
			GetPlayerPos(playerid, sx, sy, sz);
			SetPlayerPos(playerid, sx, sy, sz);
			if(VariableCaido[playerid] != 1) CongelarEx(playerid);
			VWTemporal[playerid] = GetPlayerVirtualWorld(playerid);
			SetPlayerVirtualWorld(playerid, 99);
			SetTimerEx("AntiCJ",2000,false,"d",playerid);
			EnAuto[playerid] = 0;
			Contador[playerid]++;
			new string[150];
			format(string,sizeof(string),"No intentes robar vehículos sin rol, quedas congelado por 2 segundos. ({FF0000}Advertencia: %d/5{FFFFFF})",Contador[playerid]);
			Mensaje(playerid,-1,string);
			if(Contador[playerid] >= 5) ExpulsarJugador(playerid, "Anti CJ");
			return true;
		}
	}
	if(ispassenger == 0 && AVION(vehicleid) && PlayerInfo[playerid][pLicencia] == 0)
	{
		Mensaje(playerid,-1,"Necesitas una licencia de piloto para poder manejar este tipo de vehículo.");
		RemovePlayerFromVehicle(playerid);
		ClearAnimations(playerid);
	}
	if(PlayerInfo[playerid][Agonizando] == 1)
	{
		RemovePlayerFromVehicle(playerid);
	}
	if(playerState == PLAYER_STATE_DRIVER)
	{
		SetPlayerArmedWeapon(playerid, 0);
	}
	if(bloqueado[vehicleid] == 0)
	{
		EnAuto[playerid] = vehicleid;
		BajandoV[playerid] = 0;
	}
	else
	{
		Mensaje(playerid,-1,"El vehículo está cerrado, para destrabarlo el dueño tiene que {DBED15}/abrir");
		ClearAnimations(playerid);
		return 1;
	}
	if(ispassenger == 1 && TAXI(vehicleid))
	{
		foreach(new i : Player)
		{
			if(IsPlayerInVehicle(i,vehicleid) && GetPlayerState(i) == PLAYER_STATE_DRIVER)
			{
				Mensaje(playerid,-1,"Has subido al taxi.");
				Taxista[playerid] = i;
				Pasajero[i] = playerid;
				LlamadaTaxi[playerid] = 0;
				SetPlayerColor(playerid, COLORCARGO0);
				TM[playerid] = SetTimerEx("Taximetro",15000,true,"d",playerid);
				TextDrawShowForPlayer(playerid,TAXIMET);
				TextDrawShowForPlayer(playerid,TAXIMET2[i]);
				return 1;
			}
		}
	}
	if(ispassenger == 0)
	{
		if(BICI(vehicleid) && VehiculosInfo[vehicleid][vcomprable] == 0)
		{
			GetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);
			SetVehicleParamsEx(vehicleid, 1, lights, alarm, doors, bonnet, boot, objective);
		}
	}
	if(ispassenger == 0)
	{
		if(VehiculosInfo[vehicleid][vtrabajo] == 99 && PlayerInfo[playerid][pCargos] > 0)
		{
			Mensaje(playerid,-1,"No puedes utilizar este vehículo si te persigue la policía.");
			SacarVehiculo(playerid);
		}
	}
	if(ispassenger == 0)
	{
		if(VehiculosInfo[vehicleid][vtrabajo] == 10 && !Pescadores(playerid))
		{
			Mensaje(playerid,-1,"Necesitas ser pescador para utilizar este vehículo.");
			SacarVehiculo(playerid);
		}
	}
	// Basurero
	if(ispassenger == 0)
	{
		if(BASU(vehicleid))
		{
			if(PlayerInfo[playerid][jJob] != BASURERO && PlayerInfo[playerid][jJob2] != BASURERO)
			{
				Mensaje(playerid, -1, "Necesitas obtener el trabajo de basurero antes de usar este vehículo.");
				SacarVehiculo(playerid);
				return 1;
			}
			if(RecorridoBasu[playerid] == 0)
			{
				if(RopaBasu[playerid] == 1 || PlayerInfo[playerid][VIP] == 1)
				{
					PBasura[playerid] = 1;
					Mensaje(playerid,-1,"{999966}Consigue a un compañero basurero y comienza a trabajar.");
					SetPlayerCheckpoint(playerid, -178.6163, -1618.1124, 3.4908,5.0);
					PuntoBaX[playerid] = -178.6163; PuntoBaY[playerid] = -1618.1124; PuntoBaZ[playerid] = 3.4908; PuntoBaA[playerid] = 5.0;
					return 1;
				}
				else
				{
					Mensaje(playerid,-1,"{999966}Las cuentas comunes necesitan la vestimenta de basurero para trabajar.");
					Mensaje(playerid,-1,"{999966}Si quieres trabajar con cualquier ropa, necesitas una cuenta VIP.");
				}
			}
			else return Mensaje(playerid,-1,"{999966}Consigue un compañero basurero y termina el trabajo.");
		}
	}
	if(ispassenger == 0)
	{
		if(VehiculosInfo[vehicleid][vfaccion] > 0 && PlayerInfo[playerid][jMiembro] != VehiculosInfo[vehicleid][vfaccion])
		{
			Mensaje(playerid,-1,"Este vehículo es de una facción a la cual no perteneces.");
			SacarVehiculo(playerid);
		}
	}
	if(ispassenger == 0)
	{
		if(SAPD(vehicleid))
		{
			if(PlayerInfo[playerid][pDuty] != 1)
			{
				Mensaje(playerid, -1, "Necesitas ser policía y estar en servicio para utilizar este vehículo.");
				SacarVehiculo(playerid);
			}
		}
	}
	if(ispassenger && TREN(vehicleid))
	{
		if(Boletos[playerid] == 0)
		{
			RemovePlayerFromVehicle(playerid);
			new Float:slx, Float:sly, Float:slz;
			GetPlayerPos(playerid, slx, sly, slz);
			SetPlayerPos(playerid, slx+3, sly+3, slz+2);
			SetCameraBehindPlayer(playerid);
			Mensaje(playerid, -1, "Te expulsaron del tren porque no has comprado un boleto.");
		}
		else
		{
			Boletos[playerid] = 0;
			Mensaje(playerid, -1, "Gracias por viajar con nosotros, disfrute de su viaje.");
		}
	}
	if(ispassenger == 0)
	{
		if(HELISAPD(vehicleid))
		{
			if(PlayerInfo[playerid][pDuty] != 1)
			{
				Mensaje(playerid, -1, "Necesitas ser policía y estar en servicio para utilizar este vehículo.");
				SacarVehiculo(playerid);
				return 1;
			}
			if(PlayerInfo[playerid][pRango] < 2)
			{
				Mensaje(playerid, -1, "Necesitas tener el rango de Oficial para utilizar este vehículo.");
				SacarVehiculo(playerid);
				return 1;
			}
		}
	}
	if(ispassenger == 0)
	{
		if(AMBU(vehicleid))
		{
			if(PlayerInfo[playerid][jJob] != MEDICO && PlayerInfo[playerid][jJob2] != MEDICO)
			{
				Mensaje(playerid, -1, "Necesitas ser médico para utilizar este vehículo.");
				SacarVehiculo(playerid);
			}
		}
	}
	if(ispassenger == 0)
	{
		if(TAXI(vehicleid))
		{
			if(PlayerInfo[playerid][jJob] != TAXISTA && PlayerInfo[playerid][jJob2] != TAXISTA)
			{
				Mensaje(playerid, -1, "Necesitas ser taxista para utilizar este vehículo.");
				SacarVehiculo(playerid);
			}
		}
	}
	if(ispassenger == 0)
	{
		if(COSECHA(vehicleid))
		{
			if(Cosechando[playerid] == 0)
			{
				Mensaje(playerid, -1, "Necesitas obtener el trabajo de cosechador antes de usar este vehículo.");
				SacarVehiculo(playerid);
			}
			else if(COSECHA(vehicleid))
			{
				if(Cosechando[playerid] == 1 && PCosechar[playerid] == 1)
				{
					// COSECHADOR LS
					if(TipoCosecha[playerid] == 1)
					{
						Cosechando[playerid] = 1;
						PCosechar[playerid] = 1;
						Mensaje(playerid,-1,"Sigue tu recorrido con tu cosechadora.");
						SetPlayerRaceCheckpoint(playerid,0,-330.7316,-1559.3604,16.3868,-235.0713,-1545.3926,5.6836,7);
						PCosechar[playerid] = 2;
						return 1;
					}
					// COSECHADOR BB
					if(TipoCosecha[playerid] == 2)
					{
						Cosechando[playerid] = 1;
						PCosechar[playerid] = 1;
						Mensaje(playerid,-1,"Sigue tu recorrido con tu cosechadora.");
						SetPlayerRaceCheckpoint(playerid,0,-124.6551,57.7169,4.0930,-217.0756,95.2052,3.4693,7);
						PCosechar[playerid] = 2;
						return 1;
					}
				}
			}
		}
	}
	if(ispassenger == 0)
	{
		if(CAMION(vehicleid))
		{
			new accesoa=0;
			if(PlayerInfo[playerid][jJob] == CAMIONERO) accesoa=1;
			if(PlayerInfo[playerid][jJob2] == CAMIONERO) accesoa=1;
			if(PlayerInfo[playerid][pDuty] == 1) accesoa=1;
			if(accesoa == 0) { Mensaje(playerid, -1, "No puedes utilizar este vehículo."); SacarVehiculo(playerid); return 1; }
			new string[256];
			switch(CamionCargado[vehicleid])
			{
				case 0:
				{
					format(string, sizeof(string), "Camión de Carga Número: {00CC00}%d{FFFFFF} | Contenido: {00CC00}Ninguna {FFFFFF}| Chofer: {DBED15}Ninguno", vehicleid-28);
					Mensaje(playerid, -1,string);
					Mensaje(playerid, -1, "Usa {DBED15}/cargar camion{FFFFFF} para cargar el camion.");
				}
				case 1:
				{
					format(string, sizeof(string), "Camión de Carga Número: {00CC00}%d{FFFFFF} | Contenido: {00CC00}Productos 24/7{FFFFFF} {FFFFFF}| Chofer: {DBED15}%s", vehicleid-28,NombreJugador(Chofer[vehicleid]));
					Mensaje(playerid, -1,string);
				}
				case 2:
				{
					format(string, sizeof(string), "Camión de Carga Número: {00CC00}%d{FFFFFF} | Contenido: {00CC00}Ropa{FFFFFF} | Chofer: {DBED15}%s", vehicleid-28,NombreJugador(Chofer[vehicleid]));
					Mensaje(playerid, -1,string);
				}
				case 3:
