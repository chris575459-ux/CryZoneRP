public OnDialogResponse(playerid, dialogid, response, listitem, inputtext[])
{
	#if DEBUG_GM == 1
	printf("(OnDialogResponse) PlayerID: %d | DialogID: %d | Response: %d | ListItem: %d", playerid, dialogid, response, listitem);
	#endif
	new string[500], Query[1024];
	switch(dialogid)
	{
	    case DIALOGO_MUERTO:
	    {
	        if(response)
	        {
	            if(Muriendo[playerid] == 1)
	            {
	                if(listitem == 0)
	                {
                        LlamarHospital(playerid);
			            TextDrawShowForPlayer(playerid, Textdraws[textdraw_INDI]);
		                PlayerTextDrawShow(playerid, PlayerTextdraws[playerid][ptextdraw_INDI]);
	                }
	                else
	                {
		   				SetPlayerColor(playerid, COLORCARGO0);
						ApareceHospital(playerid);
						Muriendo[playerid] = 0;
						suicidio[playerid] = 0;
						MarcadoMedicos[playerid] = 0;
						LlamadaMedicos[playerid] = 0;
	                }
	            }
	        }
	        else
	        {
	            TextDrawShowForPlayer(playerid, Textdraws[textdraw_INDI]);
                PlayerTextDrawShow(playerid, PlayerTextdraws[playerid][ptextdraw_INDI]);
	        }
	    }
		case SEXO:
		{
			if(response)
			{
				PlayerInfo[playerid][jSexo] = listitem+1;
				new day, month, year;
				getdate(year,month,day);

				new rplName[32];
				strcpy(rplName, NombreJugador(playerid));
				for(new i = 0; i < strlen(rplName); i++)
				{
					if(rplName[i] == '_')
					{
						rplName[i] = ' ';
					}
				}

				mysql_format(Conecction, Query, sizeof(Query), "INSERT INTO `usuarios` (username, password,posX,posY,posZ,vida,money,banco,skin,Registro,Email,EMS,Edad) VALUES ('%s',md5('%s'),'1715.5295','-1900.1307','13.5664','100','6000','10000','250','%02d/%02d/%02d','%s',1,%d)", NombreJugador(playerid),PlayerInfo[playerid][pPassword],day,month,year,PlayerInfo[playerid][pEmail],PlayerInfo[playerid][jEdad]);
				mysql_pquery(Conecction, Query, "UsuarioRegistrado", "d", playerid);

				mysql_format(Conecction2, Query, sizeof(Query), "INSERT INTO `smf_members` (member_name, passwd, real_name, date_registered, id_group, email_address, is_activated) VALUES ('%s', SHA1(CONCAT(LOWER('%s'), '%s')),'%s',UNIX_TIMESTAMP(now()), 0, '%s', '1')",NombreJugador(playerid),NombreJugador(playerid),PlayerInfo[playerid][pPassword],rplName,PlayerInfo[playerid][pEmail]);
				mysql_pquery(Conecction2, Query, "UpdateUltimoUsuarioForo", "s", NombreJugador(playerid));

				new query[200];
				Usuarios[Cantidad]++;
				mysql_format(Conecction, query, sizeof(query), "UPDATE `registros` SET `Cantidad`='%d'", Usuarios[Cantidad]);
				mysql_pquery(Conecction, query);

				mysql_format(Conecction2, query, sizeof(query), "UPDATE `smf_settings` SET `Value`='%s' WHERE `Variable`='latestRealName'", rplName);
				mysql_pquery(Conecction2, query);

				mysql_format(Conecction2, query, sizeof(query), "UPDATE `smf_settings` SET `Value`='%d' WHERE `Variable`='totalMembers'", Usuarios[Cantidad]);
				mysql_pquery(Conecction2, query);

				mysql_format(Conecction2, query, sizeof(query), "UPDATE `smf_settings` SET `Value`='%d' WHERE `Variable`='latestMember'", Usuarios[Cantidad]);
				mysql_pquery(Conecction2, query);

				new escapepass[100];
				mysql_escape_string(PlayerInfo[playerid][pPassword], escapepass);

