			if(PlayerInfo[i][jExperiencia] < PlayerInfo[i][NivelExp])
			{
				PlayerInfo[i][jExperiencia]++;
				BarraExp(i, 1);
			}
		}
	}
	return 1;
}

function PayDay()
{
	ProcesarFiestaCerveza();

	new string[350];
	for(new i, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if(JugadorLogeado[i] == 1)
		{
			/* // */
			new paga = PlayerInfo[i][jNivel]*100+50;
			new suma,intereses,string2[350],string3[350],seguro,sumat,seguro2,seguro3,seguro4,seguros;
			seguro = PlayerInfo[i][vPrecio]/100;
			seguro2 = PlayerInfo[i][v2Precio]/100;
			seguro3 = PlayerInfo[i][v3Precio]/100;
			seguro4 = PlayerInfo[i][v4Precio]/100;
			seguros = seguro+seguro2+seguro3+seguro4;
			if(PlayerInfo[i][VIP] == 1)
			{
				intereses = PlayerInfo[i][pBanco]/500;
				if(intereses > 4000) { intereses = 4000; }
			}
			else
			{
				intereses = PlayerInfo[i][pBanco]/1000;
				if(intereses > 1500){ intereses = 1500; }
			}
			suma = paga-seguros;
			sumat = suma+intereses;
			//
			new gastosest;
			new gananciasest;
			if(IsVehicleInEstacionamiento(autos1[i])  == 1) { gastosest += 150; }
			if(IsVehicleInEstacionamiento(autos2[i]) == 1)  { gastosest += 150; }
			if(IsVehicleInEstacionamiento(autos3[i]) == 1)  { gastosest += 150; }
			if(IsVehicleInEstacionamiento(autos4[i]) == 1)  { gastosest += 150; }
			for(new e = 0; e < MAX_ESTACIONA; e++)
			{
				if(InfoEst[e][estDuenoID] == PlayerInfo[i][p_DBID])
				{
					for(new v, h = GetVehiclePoolSize(); v <= h; v++)
					{
						if(GetVehicleVirtualWorld(v) == InfoEst[e][estVirW] &&  GetVehicleInterior(v) == InfoEst[e][estInt])
						{
							gananciasest += 150;
						}
					}
				}
			}
			PlayerInfo[i][pBanco] -= gastosest;
			PlayerInfo[i][pBanco] += gananciasest;
			//
			new ganaciasresta;
			for(new p=1; p < MAX_PROPIEDADES; p++)
			{
				if(PropInfo[p][propiedad_ID] == PlayerInfo[i][CasaID] && PropInfo[p][pTipo] == 3)
				{
					ganaciasresta += PropInfo[p][pGanancias];
					PropInfo[p][pGanancias] = 0;
				}
				if(PropInfo[p][propiedad_ID] == PlayerInfo[i][CasaID2] && PropInfo[p][pTipo] == 3)
				{
					ganaciasresta += PropInfo[p][pGanancias];
					PropInfo[p][pGanancias] = 0;
				}
			}
			PlayerInfo[i][pBanco] += ganaciasresta;
			//
			Mensaje(i, COLORCARGO0, "{F0F0F0}/_____________ RESUMEN DE TU CUENTA _____________/");
			if(gastosest > 0)
			{
				format(string, sizeof(string), " Gastos por estacionamiento privado: {FF3300}$%s", ConvertirNumero(gastosest));
				Mensaje(i, -1, string);
			}
			if(gananciasest > 0)
			{
				format(string, sizeof(string), " Ganancias por estacionamiento privado: {00CC00}$%s", ConvertirNumero(gananciasest));
				Mensaje(i, -1, string);
			}
			if(ganaciasresta > 0)
			{
				format(string, sizeof(string), " Ganancias por restaurante: {00CC00}$%s", ConvertirNumero(ganaciasresta));
				Mensaje(i, -1, string);
			}
			if(seguros > 0)
			{
				format(string, sizeof(string), " Seguro de vehículos: {FF3300}$%s",ConvertirNumero(seguros));
				Mensaje(i, -1,string);
			}
			if(PlayerInfo[i][pDuty] == 1)
			{
				format(string2, sizeof(string2), " Pago por servir a la Ley: {00CC00}$3.000");
				Mensaje(i, -1,string2);
				sumat=sumat+3000;
			}
			if(suma >= 0)
			{
				format(string3, sizeof(string3), " Pago diario: {00CC00}$%s {FFFFFF}| Gastos: {FF3300}$%s {FFFFFF} | Total: {00CC00}$%s", ConvertirNumero(paga),ConvertirNumero(seguros),ConvertirNumero(suma));
				Mensaje(i, -1,string3);
				format(string2, sizeof(string2), " Saldo Bancario: {00CC00}$%s{FFFFFF} | Ganancias (intereses): {00CC00}$%s",ConvertirNumero(PlayerInfo[i][pBanco]+suma), ConvertirNumero(intereses));
				Mensaje(i, -1,string2);
			}
			else if(suma < 0)
			{
				format(string3, sizeof(string3), " Pago diario: {00CC00}$%s {FFFFFF}| Gastos: {FF3300}$%s {FFFFFF} | Total: {FF3300}$%s", ConvertirNumero(paga),ConvertirNumero(seguros),ConvertirNumero(suma));
				Mensaje(i, -1,string3);
				format(string2, sizeof(string2), " Saldo Bancario: {00CC00}$%s{FFFFFF} | Ganancias (intereses): {00CC00}$%s",ConvertirNumero(PlayerInfo[i][pBanco]-suma), ConvertirNumero(intereses));
				Mensaje(i, -1,string2);
			}
			if(Emisoras[i][TransEmisora] == 1)
			{
				new emisoraid, pagalocutor = 0, oyentes = 0;
				emisoraid = Emisoras[i][CabinaOcupada];
				oyentes = GetOyentesEmisora(emisoraid);
				pagalocutor = 3000+(oyentes*500);
				sumat += pagalocutor;
				format(string, sizeof(string), "Ganancias por la emisora de radio: {00CC00}$%s", ConvertirNumero(pagalocutor));
				Mensaje(i, -1, string);
			}
			Mensaje(i, COLORCARGO0, "{F0F0F0}/________________ FIN DEL RESUMEN ________________/");
			PlayerInfo[i][pBanco] += sumat;
			if(TiempoPD[i] < 900)
			{
				TiempoPD[i] = 0;
				Mensaje(i, COLOR_GREY, "No jugaste el tiempo suficiente para recibir puntos de experiencia.");
				continue;
			}
			TiempoPD[i] = 0;
			PlayerInfo[i][horasjugadas]++;
			/*
			if(PlayerInfo[i][horasjugadas] == 6)
			{
				new Sente[100];
				mysql_format(Conecction2, Sente, sizeof(Sente), "UPDATE `smf_members` SET `is_activated`='1' WHERE `member_name`='%s'", NombreJugador(i));
				mysql_pquery(Conecction2, Sente);
			}
			*/
			if(PlayerInfo[i][jExperiencia] < PlayerInfo[i][NivelExp])
			{
				PlayerInfo[i][jExperiencia]++;
				BarraExp(i, 1);
			}
		}
	}
	return 1;
}

