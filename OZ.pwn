ResetearCarreraCross()
{
	UpdateDynamic3DTextLabelText(TextoCross,COLOR_BLANCO,"{FF3300}Boleteria cerrada\n{DBED15}Horarios de los eventos:{FFFFFF} Carreras a cada xx:32 horas!");
	HabilitarCarrerasCross = 0;
	InicioCross = 0;
	PuestoCross = 0;
	GanadoresCross = 0;
	TiempoCross[0] = 0;
	TiempoCross[1] = 0;
	for(new v; v < sizeof(VehCross); v++)
	{
		if(VehCross[v] > 0) DestroyVehicle(VehCross[v]),VehCross[v] = 0;
	}
	for(new a; a < sizeof(InfoCPCross); a++)
	{
		for(new b; b < sizeof(InfoCPCross[]); b++) InfoCPCross[a][b] = 0;
	}
	return true;
}

stock SetPosEx(playerid, Float:X, Float:Y, Float:Z, Float:A, interiorid, worldid)
{
	//if(interiorid > 0) CargandoEx(playerid);
	//else if(worldid > 0) CargandoEx(playerid);
	CargandoEx(playerid);

	SetPlayerPos(playerid, X, Y, Z);
	SetPlayerFacingAngle(playerid, A);
	SetPlayerInterior(playerid, interiorid);
	SetPlayerVirtualWorld(playerid, worldid);
	return 1;
}

stock SacarCarreraCross(playerid,a = 0)
{
	if(MotoCross[playerid] == 0) return false;
	if(InicioCross == 1)
	{
		if(VehCross[MotoCross[playerid]-1] > 0) DestroyVehicle(VehCross[MotoCross[playerid]-1]),VehCross[MotoCross[playerid]-1] = 0;
	}
	for(new td; td < sizeof(TDCross); td++) TextDrawHideForPlayer(playerid, TDCross[td]);
	for(new td; td < sizeof(PTDCross[]); td++) PlayerTextDrawHide(playerid, PTDCross[playerid][td]);
	MotoCross[playerid] = 0;
	VueltasCross[playerid] = 0;
	CPCross[playerid] = 0;
	DisablePlayerRaceCheckpoint(playerid);
	if(a == 1)
	{
		SetPosEx(playerid,-1434.9067,-563.9880,1061.2275,4.5716,4,4);
	}
	if(JugadoresCross() <= 0)
	{
		ResetearCarreraCross();
	}
	return true;
}

IniciarCarrera()
{
	if(JugadoresCross() <= 1)
	{
		for(new i,j = GetPlayerPoolSize(); i <= j; i++)
		{
			if(MotoCross[i] > 0) SacarCarreraCross(i);
			if(!IsPlayerInRangeOfPoint(i,150.0,-1424.1583,-561.9344,1067.7369)) continue;
			if(GetPlayerInterior(i) != 4) continue;
			if(GetPlayerVirtualWorld(i) != 4) continue;
			SendClientMessage(i, -1, "La carrera se cancelo automaticamente por la falta de corredores.");
		}
		ResetearCarreraCross();
		return true;
	}
	new playerScores[MAX_PLAYERS][rankingEnum],index,puesto;
 	for(new i, j = GetPlayerPoolSize(); i <= j; ++i)
  	{
   		if(IsPlayerConnected(i) && !IsPlayerNPC(i) && MotoCross[i] > 0)
	 	{
			playerScores[index][player_Score] = MotoCross[i];
	 		playerScores[index++][player_ID] = i;
		}
	}
 	OrdenUsuarios(playerScores, index, 0);
 	InicioCross = 1;
  	for(new i; i < index; ++i)
   	{
		if(i <= index)
	 	{
	 		new playerid = playerScores[i][player_ID];
			puesto ++;
			MotoCross[playerid] = puesto;
			switch(puesto)
