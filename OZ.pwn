function BajarTiempoAgresor()
{
	gettime(Hora, Minuto, Segundo);
	if(Minuto == 2 && PaydayDado == 0)
	{
		PayDay();
		PaydayDado = 1;
		// Iniciar Carrera LV
		VerEnCarreras();
		ResetearCarrera();
		HabilitarCarreras = 1;
		UpdateDynamic3DTextLabelText(TextoCarrera,COLOR_BLANCO,"{00CC00}Boleteria abierta\n{FFFFFF}Costo de la entrada: {00CC00}$600\n{DBED15}/comprar entrada");
		//ConteoCarrera = 10;
		ConteoCarrera = 3;
		SetTimer("EmpezarCarrera",60*1000,0);
		new string[250];
		format(string,sizeof(string),"En {00CC00}%d minutos{FFFFFF} comienza la carrera, ya se inscribieron {DBED15}%d{FFFFFF} corredores.",ConteoCarrera,ParticipantesCarrera);
		for(new i, j = GetPlayerPoolSize(); i <= j; i++) if(EstaEn(i,250,-1408.8688,-304.5082,1056.4351) && GetPlayerInterior(i) == 7) Mensaje(i,-1,string);
	}
	if(Minuto == 3 && PaydayDado == 1)
	{
		PaydayDado = 0;
	}
	//
	if(Minuto == 32 && CarreraDado == 0)
	{
	    CarreraDado = 1;
		// Iniciar Carrera SF
		if(CrearCarreraCrossPayday)
		{
			ResetearCarreraCross();
			HabilitarCarrerasCross = 1;
			UpdateDynamic3DTextLabelText(TextoCross,COLOR_BLANCO,"{00CC00}Boleteria abierta\n{FFFFFF}Costo de la entrada: {00CC00}$600\n{DBED15}/comprar entrada");
			//IniciarCarrera2((1000*60*10));
			IniciarCarrera2((1000*60*3));
		}
	}
	if(Minuto == 33 && CarreraDado == 1)
	{
		CarreraDado = 0;
	}
	//
	if(EmpezoMision == 1)
	{
		for(new t; t < 3; t++)
		{
			if(TiempoRobo[t][0] > 0)
			{
				TiempoRobo[t][0] --;
				ActualizarMision(3);
				if(TiempoRobo[t][0] <= 0)
				{
					TiempoRobo[t][0] = 0;
					TiempoRobo[t][1] ++;
					if(t == 0) // Ladrones
					{
						if(TiempoRobo[t][1] == 1) // Si fue al estacionamiento
						{
							for(new a; a < sizeof(OrdenGrupo); a++)
							{
   								if(OrdenGrupo[a] > -1)
								{
	   								new i = OrdenGrupo[a];
	   								if(CondicionRobo[i] != 1 && GrupoRobo[i] == 1)
	   								{
	   									SetPlayerVirtualWorld(i,0);
	   									SetPlayerInterior(i,0);
   										SetPlayerPos(i,2232.8591,-1999.3622,13.5469);
   										SetPlayerFacingAngle(i,90.0000);
								   		SendClientMessage(i,-1,"No seguiste las instruciones, no podrás participar en el asalto.");
								   		ActualizarMision(2);
								   		SacarDeMision(i);
	   								}
								}
							}
							SetTimerEx("DespuesTimerAsalto",1000,0,"d",1);
						}
						else if(TiempoRobo[t][1] == 2) // Si llego a la emboscada
						{
							for(new a; a < sizeof(OrdenGrupo); a++)
							{
   								if(OrdenGrupo[a] > -1)
								{
									new i = OrdenGrupo[a];
									if(CondicionRobo[i] != 2  && GrupoRobo[i] == 1)
									{
	   									SetPlayerVirtualWorld(i,0);
	   									SetPlayerInterior(i,0);
										SendClientMessage(i,-1,"Fuiste expulsado de la misión por no seguir las instrucciones.");
								   		ActualizarMision(2);
								   		SacarDeMision(i);
									}
								}
							}
						}
						else if(TiempoRobo[t][1] == 3)
						{
							for(new a; a < sizeof(OrdenGrupo); a++)
							{
   								if(OrdenGrupo[a] > -1)
								{
									new i = OrdenGrupo[a];
									if(GrupoRobo[i] == 1) SendClientMessage(i,-1,"Misión fallida.");
   									SetPlayerVirtualWorld(i,0);
   									SetPlayerInterior(i,0);
						   			ActualizarMision(2);
							   		SacarDeMision(i);
								}
							}
							ResetearMision();
						}
					}
					else if(t == 1) // Guardias
					{
						if(TiempoRobo[t][1] == 1) // La bodega
						{
							for(new a; a < sizeof(OrdenGrupo); a++)
							{
   								if(OrdenGrupo[a] > -1)
								{
									new i = OrdenGrupo[a];
									if(CondicionRobo[i] != 1 && GrupoRobo[i] == 2)
									{
										TextDrawHideForPlayer(i,MinutoAsalto[1]);
										SacarDeMision(i);
										DisablePlayerCheckpoint(i);
										Mensaje(i,-1,"Fuiste expulsado.");
			 							SetPlayerPos(i,1957.9030,952.9955,10.8203);
										SetPlayerFacingAngle(OrdenGrupo[a],180.0000);
										SetPlayerInterior(i,0);
										SetPlayerVirtualWorld(i,0);
									}
									else if(CondicionRobo[i] == 1 && GrupoRobo[i] == 2)
									{
										Mensaje(i,-1,"Entra a la bóveda y agarra un bolso con dinero.");
										PlayerTextDrawSetString(i,IndicacionesT[OrdenGrupo[a]],"Agarra_un_~w~bolso_con_dinero.");
		  								if(ShowSound[i] == 1) PlayAudioStreamForPlayer(OrdenGrupo[a],"https://"URL"/Boveda.mp3",1400.6600, 514.1200, 1001.6600,25.0,1);
									}
								}
							}
							if(GetPlayersGrupo(2,1) == 0)
							{
								for(new a; a < sizeof(OrdenGrupo); a++)
								{
									if(OrdenGrupo[a] > -1)
									{
										SendClientMessage(OrdenGrupo[a],-1,"Los guardias no cumplieron con su misión.");
					 					SetPlayerVirtualWorld(OrdenGrupo[a],0);
	 									SetPlayerInterior(OrdenGrupo[a],0);
	 									SacarDeMision(OrdenGrupo[a]);
										if(GrupoRobo[OrdenGrupo[a]] == 1)
										{
  											SetPlayerPos(OrdenGrupo[a],2232.8591,-1999.3622,13.5469);
   											SetPlayerFacingAngle(OrdenGrupo[a],90.0000);
										}
										else
										{
											SetPlayerPos(OrdenGrupo[a],1957.9030,952.9955,10.8203);
											SetPlayerFacingAngle(OrdenGrupo[a],180.0000);
										}
									}
								}
								ResetearMision();
							}
							else
							{
								TiempoRobo[1][0] = (2 * 60);
								ActualizarMision(3);
								MoveObject(ObjetosRobo[0],1400.6600, 514.1200, 1001.6600,1.0,0.0000, 0.0000, 62.0000);//boveda
								MoveObject(ObjetosRobo[8],1407.4141, 490.7064, 1009.6200,1.0,0.0000, 0.0000, 18.0000);//puerta guardias
								if(ObjetosRobo[1] == 0) ObjetosRobo[1] = CreateObject(1550, 1400.83740, 523.96204, 1000.34271,   0.00000, 0.00000, 0.00000);
								if(ObjetosRobo[2] == 0) ObjetosRobo[2] = CreateObject(1550, 1401.17798, 523.53619, 1000.34271,   0.00000, 0.00000, 0.00000);
								if(ObjetosRobo[3] == 0) ObjetosRobo[3] = CreateObject(1550, 1401.58252, 524.00391, 1000.34271,   0.00000, 0.00000, 0.00000);
							 	if(ObjetosRobo[4] == 0) ObjetosRobo[4] = CreateObject(1550, 1401.93250, 523.55212, 1000.34271,   0.00000, 0.00000, 0.00000);
								if(ObjetosRobo[5] == 0) ObjetosRobo[5] = CreateObject(1550, 1402.35901, 523.95135, 1000.34271,   0.00000, 0.00000, 0.00000);
							}
						}
						else if(TiempoRobo[t][1] == 2)
						{
							for(new a; a < sizeof(OrdenGrupo); a++)
							{
   								if(OrdenGrupo[a] > -1 && GrupoRobo[OrdenGrupo[a]] == 2)
								{
									TextDrawHideForPlayer(OrdenGrupo[a],MinutoAsalto[1]);
								}
							}
							SetTimerEx("DespuesTimerAsalto",3000,0,"i",2);
						}
						else if(TiempoRobo[t][1] == 3)
						{
							for(new a; a < sizeof(OrdenGrupo); a++)
							{
   								if(OrdenGrupo[a] > -1)
								{
									new i = OrdenGrupo[a];
									if(i > -1)
									{
										if(GrupoRobo[i] == 2) Mensaje(i,-1,"Misión fallida.");
										else Mensaje(i,-1,"Los guardias no cumplieron con su misión.");
		  								TextDrawHideForPlayer(i,MinutoAsalto[0]);
				  						SacarDeMision(i);
										if(GetPlayerVirtualWorld(i) != 100)
										{
	  										SetPlayerInterior(i,0);
											SetPlayerPos(i,2232.8591,-1999.3622,13.5469);
			  							}
					 					SetPlayerVirtualWorld(i,0);
						  			}
  								}
							}
							ResetearMision();
						}
						else if(TiempoRobo[t][1] == 5)
						{
							for(new a; a < sizeof(OrdenGrupo); a++)
							{
   								if(OrdenGrupo[a] > -1)
								{
									new i = OrdenGrupo[a];
									if(GrupoRobo[i] == 2) SendClientMessage(i,-1,"Mision fallida.");
									else Mensaje(i,-1,"Lo guardias no cumplieron con su misión.");
									SacarDeMision(i);
								}
							}
							ResetearMision();
						}
					}
					else if(t == 2) // Mafiosos
					{
						if(TiempoRobo[t][1] == 2)
						{
							SetTimerEx("DespuesTimerAsalto",5000,0,"i",3);
							for(new a; a < sizeof(OrdenGrupo); a++)
	   						{
	  							if(OrdenGrupo[a] > -1 && GrupoRobo[OrdenGrupo[a]] == 3)
		 						{
	   								new i = OrdenGrupo[a];
									TextDrawHideForPlayer(i,MinutoAsalto[2]);
								}
							}
	 					}
					}
				}
			}
		}
	}
	if(TiempoIniciarAsalto > 0)
	{
		TiempoIniciarAsalto --;
		new string[8];
		format(string,sizeof(string),"00:%02d",TiempoIniciarAsalto);
		TextDrawSetString(MinutoAsalto[0],string);
		if(TiempoIniciarAsalto <= 0)
		{
			new asd[3];
			for(new i, j = GetPlayerPoolSize(); i <= j; i++)
			{
				if(PlayerConectado(i) && GrupoRobo[i] > 0)
				{
					if(AceptacionBlindado[i] == 1)
					{
						ShowPlayerDialog(i,-1,DIALOG_STYLE_MSGBOX,"Titulo","pelotudo","Aceptar","Cancelar");
						SacarDeMision(i,2);
						SacarDeMision(i);
						AceptacionBlindado[i] = 0;
						TextDrawHideForPlayer(i,MinutoAsalto[0]);
						Mensaje(i,-1,"La misión fue rechazada automáticamente porque no respondiste a tiempo.");
	 					for(new p; p < sizeof(PuntosRobo); p++)
					 	{
				 			if(PuntosRobo[p] == 0)
					 		{
	   							PuntosRobo[p] = 2;
	   							ActualizarMision(1);
								break;
							}
						}
						ActualizarMision();
					}
					else if(AceptacionBlindado[i] == 2)
					{
						asd[GrupoRobo[i]-1] ++;
					}
				}
			}
			if(asd[0] >= NecesariosLadron && asd[1] >= NecesariosGuardia && asd[2] >= NecesariosMafia)
			{
				EmpezarMision();
			}
			else
			{
				for(new i, j = GetPlayerPoolSize(); i <= j; i++)
				{
					if(PlayerConectado(i) && GrupoRobo[i] > 0 && AceptacionBlindado[i] == 2)
					{
						MensajeEx(i,-1,"Aceptaron {FF3300}%d{ffffff} personas de las {00CC00}%d{ffffff} necesarias, aguarda mientras formamos el grupo.",(asd[0] + asd[1] + asd[2]),(NecesariosLadron + NecesariosGuardia + NecesariosMafia));
						AceptacionBlindado[i] = 0;
						TextDrawHideForPlayer(i,MisionT2[1]);
						TextDrawHideForPlayer(i,MisionT2[2]);
						TextDrawHideForPlayer(i,MisionT2[3]);
						TextDrawHideForPlayer(i,MinutoAsalto[0]);
					}
				}
				ActualizarMision();
				for(new o; o < sizeof(OrdenGrupo); o++) OrdenGrupo[o] = -1;
				if(GetPlayersGrupo(1) >= NecesariosLadron && GetPlayersGrupo(2) >= NecesariosGuardia && GetPlayersGrupo(3) >= NecesariosMafia)
				{
					AceptarMision();
				}
			}
		}
	}
	foreach(new i : Player)
	{
		if(MarcarCPCamion[i] == 1 && !EstaEnV(i,VehiculoAsalto[4],80))
		{
			new Float:Pos[3];
 			GetVehiclePos(VehiculoAsalto[4],Pos[0],Pos[1],Pos[2]);
   			SetPlayerCheckpoint(i,Pos[0],Pos[1],Pos[2],5);
		}
		if(TiempoAgresor[i] > 0 && Agresor[i] != INVALID_PLAYER_ID)
		{
			TiempoAgresor[i]--;
		}
		else if(TiempoAgresor[i] <= 0 && Agresor[i] != INVALID_PLAYER_ID)
		{
			TiempoAgresor[i] = 0;
			Agresor[i] = INVALID_PLAYER_ID;
		}
	}
}

function CargarUsers()
{
	new Query[30];
	mysql_format(Conecction, Query, sizeof(Query), "SELECT * FROM `registros`");
	mysql_pquery(Conecction, Query, "CargarRegistros");
	return 1;
}

function CargarRegistros()
{
	new row_count;
	cache_get_row_count(row_count);
	for(new i = 0; i < row_count; i++)
	{
		cache_get_value_name_int(0, "Cantidad",	Usuarios[Cantidad]);
		cache_get_value_name_int(0, "Stats",	StatsStatus);
		cache_get_value_name_int(0, "Arbol",	ArbolStatus);
		cache_get_value_name_int(0, "Cerveza",	CervezaStatus);
		cache_get_value_name_int(0, "Blindado",	StatusBlindado);
		cache_get_value_name_int(0, "Armas",	ArmasEstado);
		cache_get_value_name_int(0, "Disable",	DisableStatus);
	}
	return 1;
}

