				{
					if(PropInfo[p][pEnVentaPor] == 0) format(string,sizeof(string),"Ubicación: %s. (Valor: {00CC00}$%s{FFFFFF}) [ID: %d]",PropInfo[p][pLocalizacion],ConvertirNumero(PropInfo[p][pPrecio]), p);
