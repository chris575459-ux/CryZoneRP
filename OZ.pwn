							new Float:X,Float:Y,Float:Z;
							GetPlayerPos(i, X, Y, Z);
							SetPlayerPos(i, X, Y, Z+5);
							Mensaje(i, -1, "A causa del fuerte impacto, saliste despedido del vehículo.");
						}
						new string[256], playerState = GetPlayerState(i);
						format(string, 256,"El jugador {FF3300}%s(ID: %d){FFFFFF} fue baneado por el anticheat. Razón: {FF3300}Super Choque (%d/%d/%d).", NombreJugador(i), i, GetVehicleModel(GetPlayerVehicleID(i)), ObtenerVelocidad(i),playerState);
						for(new l, p = GetPlayerPoolSize(); l <= p; l++)
						{
							if(PlayerConectado(l) && ShowMutes[l] == 1) Mensaje(l,-1,string);
						}
						/* /////////////////// */
						new Float:Pos[3], zone[MAX_ZONE_NAME], players[MAX_PLAYERS], str[200], count = GetClosestPlayers(i, players, 30.0);
						GetPlayerPos(i, Pos[0],Pos[1],Pos[2]);
						str = "Jugadores cercanos:";
						for(new t; t < count; t++)
						{
							new str2[5];
							valstr(str2, players[t]);
							strcat(str, " ");
							strcat(str, str2);
						}
						GetPlayer2DZone(i, zone, MAX_ZONE_NAME);
						format(string, sizeof(string), "Ubicacion: %s (%f %f %f) | %s", zone, Pos[0], Pos[1], Pos[2], str);
						MensajeAdmin(COLOR_YELLOW, string, 3);
						/* /////////////////// */
						ExpulsarJugador(i, "Super choque");
					}
				}
			}
		}
	}
	if(TiempoCarrera[0] > 0 || TiempoCarrera[1] > 0)
	{
	 	TiempoCarrera[0] ++;
	 	if(TiempoCarrera[0] >= 60)
	 	{
	 		TiempoCarrera[0] = 0;
	 		TiempoCarrera[1] ++;
	 	}
	 	new string[50];
	 	format(string,sizeof(string),"%d:%d",TiempoCarrera[1],TiempoCarrera[0]);
	 	TextDrawSetString(CarreraT[7],string);
	}
	if(TiempoCross[0] > 0 || TiempoCross[1] > 0)
	{
		TiempoCross[0] ++;
		if(TiempoCross[0] >= 60)
		{
			TiempoCross[0] = 0;
			TiempoCross[1] ++;
		}
		new string[20];
	 	format(string,sizeof(string),"%02d:%02d", TiempoCross[1], TiempoCross[0]);
		TextDrawSetString(TDCross[5], string);
	}
	return 1;
}

function BajarTiempos()
{
	foreach(new i : Player)
	{
		if(IsPlayerPaused(i)) TiempoAFK[i] ++;
		else if(TiempoAFK[i] > 0) TiempoAFK[i] = 0;

		if(!IsPlayerPaused(i)) TiempoPD[i]++;
		if(!IsPlayerPaused(i)) TiempoConectado[i]++;

		if(TiempoDuda[i] > 0) TiempoDuda[i]--;
		if(TiempoAnuncio[i] > 0) TiempoAnuncio[i]--;

		if(NotificacionConectado[i] > 0)
		{
			NotificacionConectado[i] --;
			if(NotificacionConectado[i] == 2)
			{
				PlayerTextDrawBoxColor(i, NotiConnect[i][0], 70);
				PlayerTextDrawColor(i, NotiConnect[i][1], 9044129);
				PlayerTextDrawColor(i, NotiConnect[i][2], -926365496);
				PlayerTextDrawColor(i, NotiConnect[i][3], -926365496);

				PlayerTextDrawShow(i, NotiConnect[i][0]);
				PlayerTextDrawShow(i, NotiConnect[i][1]);
				PlayerTextDrawShow(i, NotiConnect[i][2]);
				PlayerTextDrawShow(i, NotiConnect[i][3]);
			}
			else if(NotificacionConectado[i] == 1)
			{
				PlayerTextDrawBoxColor(i, NotiConnect[i][0], 50);
				PlayerTextDrawColor(i, NotiConnect[i][1], 11075654);
				PlayerTextDrawColor(i, NotiConnect[i][2], -926365496);
				PlayerTextDrawColor(i, NotiConnect[i][3], -926365666);

				PlayerTextDrawShow(i, NotiConnect[i][0]);
				PlayerTextDrawShow(i, NotiConnect[i][1]);
				PlayerTextDrawShow(i, NotiConnect[i][2]);
				PlayerTextDrawShow(i, NotiConnect[i][3]);
			}
			else if(NotificacionConectado[i] <= 0)
			{
				PlayerTextDrawBoxColor(i, NotiConnect[i][0], 255);
				PlayerTextDrawColor(i, NotiConnect[i][1], 8388863);
				PlayerTextDrawColor(i, NotiConnect[i][2], -1);
				PlayerTextDrawColor(i, NotiConnect[i][3], -1);
				PlayerTextDrawHide(i, NotiConnect[i][0]);
				PlayerTextDrawHide(i, NotiConnect[i][1]);
				PlayerTextDrawHide(i, NotiConnect[i][2]);
				PlayerTextDrawHide(i, NotiConnect[i][3]);
			}
		}

		if(EnSalonEvento(i) && Alquilado == true && Boleto[i] == false && Specteando[i] == 0)
		{
			if(strcmp(Alquilador,NombreJugador(i),true) == 1)
			{
				Mensaje(i,-1,"Los guardias del salón te expulsaron por no haber pagado la entrada.");
				SetPlayerPos(i,1423.4301,-1180.1801,25.9922);
				SetPlayerVirtualWorld(i,0);
				SetPlayerInterior(i,0);
	  		 }
		}
		if(EnSalonEvento(i) && Escuchando[i] == false && Alquilado == true)
		{
			Escuchando[i] = true;
			new mensa[52];
			format(mensa,sizeof(mensa),"http://radio.omegazone.net:8000/listen.pls?sid=%d", PuertoSalon);
			if(ShowSound[i] == 1) PlayAudioStreamForPlayer(i, mensa);
		}
		else if(!EnSalonEvento(i) && Escuchando[i] == true)
		{
			Escuchando[i] = false;
			StopAudioStreamForPlayer(i);
		}
		if(Refuerzos[i] > 0)
		{
			Refuerzos[i] --;
			if(PlayerInfo[i][jMiembro] == 1)
			{
				MarcarJugador(i,0x3399FFFF,1);
				if(Refuerzos[i] == 90 || Refuerzos[i] == 70 || Refuerzos[i] == 50) TextDrawHideForPlayer(i,RefuerzosT[3]);
				if((Refuerzos[i] == 80 || Refuerzos[i] == 60 || Refuerzos[i] == 40) && Refuerzos[i] > 40) TextDrawShowForPlayer(i,RefuerzosT[3]);
				if(Refuerzos[i] == 40)
				{
					for(new td; td < sizeof(RefuerzosT); td++) TextDrawHideForPlayer(i,RefuerzosT[td]);
				}
			}
			else
			{
				MarcarJugador(i,0x7141C5FF,PlayerInfo[i][jMiembro]);
			}
			if(Refuerzos[i] == 0) MarcarJugador(i,0xFFFFFF00,PlayerInfo[i][jMiembro]);
		}
	}
}

