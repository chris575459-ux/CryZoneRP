function DarEstrella(playerid, star)
{
	if(star == 0)
	{
		TextDrawHideForPlayer(playerid, estrellas2);
	   	TextDrawHideForPlayer(playerid, estrellas[playerid]);
		SetPlayerWantedLevel(playerid, 0);
	   	return 1;
	}
	if(star > 0 && star < 7)
	{
		TextDrawHideForPlayer(playerid, estrellas2);
	   	TextDrawHideForPlayer(playerid, estrellas[playerid]);
		SetPlayerWantedLevel(playerid, star);
	   	return 1;
	}
	else if(star > 6)
	{
		SetPlayerWantedLevel(playerid, 0);
		TextDrawShowForPlayer(playerid, estrellas2);
		new cargos[5];
		format(cargos, sizeof(cargos), "%d", star);
		TextDrawSetString(estrellas[playerid], cargos);
		TextDrawShowForPlayer(playerid, estrellas[playerid]);
		return 1;
	}
	return 1;
}

function BajarCargo(playerid)
{
	if(PlayerInfo[playerid][pCargos] == 1)
	{
		PlayerInfo[playerid][pCargos] = 0;
		DarEstrella(playerid,PlayerInfo[playerid][pCargos]);
		CagoSeguro[playerid] = 0;
	}
}

