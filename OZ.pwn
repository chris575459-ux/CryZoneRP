CMD:numero(playerid,params[])
{
	new ID,string[256];
	if(PlayerInfo[playerid][pAgenda] != 1) return Mensaje(playerid, -1, "Tu no tienes una agenda telefonica.");
	if(sscanf(params,"r", ID)) return Mensaje(playerid, -1, "Utiliza {DBED15}/numero [ID]");
