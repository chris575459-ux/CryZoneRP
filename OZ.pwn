function IngresarAuto(playerid)
{
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
	{
		for(new c = 1; c < MAX_PROPIEDADES; c++)
		{
			if(PropInfo[c][pEnUso] == 1)
			{
				if(EstaEn(playerid,4.5,PropInfo[c][pGX],PropInfo[c][pGY],PropInfo[c][pGZ]) && PropInfo[c][pGX] != 0) // Garaje
				{
					if(PlayerInfo[playerid][p_DBID] == PropInfo[c][pIDPropietario])
					{
						if(PlayerInfo[playerid][pCargos] >= 2)
						{
							for(new i, j = GetPlayerPoolSize(); i <= j; i++)
							{
								if(PlayerInfo[i][jMiembro] == 1 && PlayerInfo[i][pDuty] == 1 && EstaEnP(playerid,i,25)) return Mensaje(playerid,-1,"No puedes entrar si la policía te está siguiendo.");
							}
						}
						if(autos1[playerid] == GetPlayerVehicleID(playerid) || autos2[playerid] == GetPlayerVehicleID(playerid) || autos3[playerid] == GetPlayerVehicleID(playerid) || autos4[playerid] == GetPlayerVehicleID(playerid))
						{
							if(PropInfo[c][pInterior] == 43)
							{
								GuardarPropiedad(c);
								CargandoEx(playerid);
								SetPosVeh(GetPlayerVehicleID(playerid), 1403.4517,594.0328,1000.7422,178.6772, 99, PropInfo[c][pVW]);
							}
							else
							{
								GuardarPropiedad(c);
								CargandoEx(playerid);
								SetPosVeh(GetPlayerVehicleID(playerid), 303.7281,306.4251,999.2913, 0, 4, PropInfo[c][pVW]);
							}
						}
						else return Mensaje(playerid,-1,"Este vehículo no te pertenece.");
						//return 1;
					}
					else return Mensaje(playerid,-1,"El dueño de la casa puede guardar sus vehículos.");
					//return 1;
				}
				if(EstaEn(playerid,10,303.7281,306.4251,999.2913) && GetPlayerVirtualWorld(playerid) == PropInfo[c][pVW] && GetPlayerInterior(playerid) == 4) // Garaje
				{
					if(PlayerInfo[playerid][p_DBID] == PropInfo[c][pIDPropietario])
					{
					    CargandoEx(playerid);
						new Float:posx, Float:posy, Float:sizex, Float:sizey, Float:sizez;
						GetVehicleModelInfo(GetVehicleModel(GetPlayerVehicleID(playerid)), VEHICLE_MODEL_INFO_SIZE, sizex, sizey, sizez);
						posx = PropInfo[c][pGX] - (floatsqroot(sizey + sizey + sizey) * floatsin(-PropInfo[c][pGA], degrees));
						posy = PropInfo[c][pGY] - (floatsqroot(sizey + sizey + sizey) * floatcos(-PropInfo[c][pGA], degrees));
						SetPosVeh(GetPlayerVehicleID(playerid), posx,posy,PropInfo[c][pGZ], PropInfo[c][pGA]+180, 0, 0);
					}
					else return Mensaje(playerid,-1,"No puedes sacar vehículos de este garaje.");
					//return 1;
				}
				if(EstaEn(playerid,10,1403.6276,600.1758,1001.0373) && GetPlayerVirtualWorld(playerid) == PropInfo[c][pVW] && GetPlayerInterior(playerid) == 99) // Garaje Empresa
				{
					if(PlayerInfo[playerid][p_DBID] == PropInfo[c][pIDPropietario])
					{
					    CargandoEx(playerid);
						new Float:posx, Float:posy, Float:sizex, Float:sizey, Float:sizez;
						GetVehicleModelInfo(GetVehicleModel(GetPlayerVehicleID(playerid)), VEHICLE_MODEL_INFO_SIZE, sizex, sizey, sizez);
						posx = PropInfo[c][pGX] - (floatsqroot(sizey + sizey + sizey) * floatsin(-PropInfo[c][pGA], degrees));
						posy = PropInfo[c][pGY] - (floatsqroot(sizey + sizey + sizey) * floatcos(-PropInfo[c][pGA], degrees));
						SetPosVeh(GetPlayerVehicleID(playerid), posx,posy,PropInfo[c][pGZ], PropInfo[c][pGA]+180, 0, 0);
					}
					else return Mensaje(playerid,-1,"No puedes sacar vehículos de este garaje.");
					//return 1;
				}
			}
		}
		if(EstaEn(playerid, 5.0,1410.7946,-1228.7570,13.5469)) 	// Salon de eventos - vehiculos
		{
			if(Armado(playerid) && PlayerInfo[playerid][pDuty] != 1) return Mensaje(playerid,-1,"No puedes entrar armado al salón de eventos.");
			if(TipoEvento != 3) return Mensaje(playerid,-1,"Solamente podrán ingresar vehículos al salón cuando haya una exposición.");
			CargandoEx(playerid);
			SetPosVeh(GetPlayerVehicleID(playerid), -1165.8710,2118.6238,1246.5457, 182.4401, 13, 1);
		   	return 1;
		}
		if(EstaEn(playerid, 5.0,2409.1680,2297.2205,10.8203)) 	// Academia de policias - vehiculos
		{
		    if(PlayerInfo[playerid][pDuty] == 1 && PlayerInfo[playerid][jMiembro] == 1 && PlayerInfo[playerid][pRango] >= 5)
			{
				CargandoEx(playerid);
				SetPosVeh(GetPlayerVehicleID(playerid), 575.6691,-292.4543,1277.4216,0.2523, 55, 55);
		   		return 1;
			}
			else return Mensaje(playerid, -1, "No puedes usar este comando.");
			//return 1;
		}
		if(EstaEn(playerid,5.0,-1166.5009,2126.8232,1246.5457)) // Salon de eventos - vehiculos
		{
		    CargandoEx(playerid);
			SetPosVeh(GetPlayerVehicleID(playerid), 1410.2614,-1237.0621,13.3828, 177.9267, 0, 0);
			return 1;
		}
		if(EstaEn(playerid,5.0,575.6289,-298.0975,1276.7941)) // Academia de policias - vehiculos
		{
		    CargandoEx(playerid);
			SetPosVeh(GetPlayerVehicleID(playerid), 2414.7583,2297.2627,11.4444,271.8733,0,0);
			return 1;
		}
		// Estacionamiento
		for(new e = 0; e < MAX_ESTACIONA; e++)
		{
			if(EstaEn(playerid,10.0,InfoEst[e][estX],InfoEst[e][estY],InfoEst[e][estZ]))
			{
				if(GetPlayerVehicleID(playerid) == autos1[playerid] || GetPlayerVehicleID(playerid) == autos2[playerid] || GetPlayerVehicleID(playerid) == autos3[playerid] || GetPlayerVehicleID(playerid) == autos4[playerid])
				{
					CargandoEx(playerid);
					if(InfoEst[e][estVIP] == 1) SetPosVeh(GetPlayerVehicleID(playerid), 2057.9192,2427.1948,10.5534,178.4852, InfoEst[e][estInt], InfoEst[e][estVirW]);
					else if(InfoEst[e][estVIP] == 2) SetPosVeh(GetPlayerVehicleID(playerid), -1262.7711,461.1456,7.2091,88.8890, InfoEst[e][estInt], InfoEst[e][estVirW]);
					else SetPosVeh(GetPlayerVehicleID(playerid), 1699.8279,-1876.9562,-49.1702,90.7951, InfoEst[e][estInt], InfoEst[e][estVirW]);
					Mensaje(playerid, COLOR_BLANCO, "Ingresaste a un {DBED15}estacionamiento{FFFFFF}, el costo por día es de {00CC00}$150{FFFFFF}.");
					Mensaje(playerid, COLOR_BLANCO, "Por favor, usa {DBED15}/estacionar{FFFFFF} para dejar tu vehículo guardado en este estacionamiento.");
					Mensaje(playerid, COLOR_BLANCO, "Al guardarlo te aseguras de que nadie pueda sacarlo del estacionamiento.");
				}
				else return Mensaje(playerid,-1,"No puedes entrar con un vehículo que no te pertenece.");
				//return 1;
			}
			if(GetPlayerVirtualWorld(playerid) == InfoEst[e][estVirW] && GetPlayerInterior(playerid) == InfoEst[e][estInt])
			{
				if(EstaEn(playerid,10.0, 1706.9027, -1876.7185, -48.8961) || EstaEn(playerid,10.0, 2058.5505,2437.2556,10.8428) || EstaEn(playerid,10.0, -1257.3156,461.1437,7.2091))
				{
					if(GetPlayerVehicleID(playerid) == autos1[playerid] || GetPlayerVehicleID(playerid) == autos2[playerid] || GetPlayerVehicleID(playerid) == autos3[playerid] || GetPlayerVehicleID(playerid) == autos4[playerid])
					{
					    CargandoEx(playerid);
						new Float:posx, Float:posy, Float:sizex, Float:sizey, Float:sizez;
						GetVehicleModelInfo(GetVehicleModel(GetPlayerVehicleID(playerid)), VEHICLE_MODEL_INFO_SIZE, sizex, sizey, sizez);
						posx = InfoEst[e][estX] - (floatsqroot(sizey + sizey + sizey) * floatsin(-InfoEst[e][estA], degrees));
						posy = InfoEst[e][estY] - (floatsqroot(sizey + sizey + sizey) * floatcos(-InfoEst[e][estA], degrees));
						SetPosVeh(GetPlayerVehicleID(playerid), posx,posy,InfoEst[e][estZ], InfoEst[e][estA]+180, 0, 0);
					}
					else return Mensaje(playerid,-1,"No puedes salir con un vehículo que no te pertenece.");
					//return 1;
				}
			}
		}
	}
	return 1;
}

function ComprarVehiculo(playerid)
{
	if(!IsPlayerInAnyVehicle(playerid)) return 1;
	new carid;
