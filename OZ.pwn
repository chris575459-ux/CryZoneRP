CMD:inscribirse(playerid,params[])
{
	if(EstaEn(playerid,5, 415.2025,2535.5874,19.1484))
	{
		if(ViajeAvion[playerid] == 1) return Mensaje(playerid,-1,"Ya estás inscrito en el curso de paracaidismo.");
		if(PlayerInfo[playerid][jDinero] < 5000) return Mensaje(playerid,-1,"Necesitas {00CC00}$5.000{FFFFFF} para poder inscribirte en el curso de paracaidismo.");
		Mensaje(playerid,-1,"Te inscribiste para participar en el curso de paracaidismo, por favor espera unos minutos.");
		Mensaje(playerid,-1,"Si te alejas mucho de la oficina, no podrás subirte al avión.");
		QuitarDinero(playerid, 5000);
		ViajeAvion[playerid] = 1;
		return 1;
	}
	if(EstaEn(playerid,5,-1447.7421,-313.3544,1052.0969))
	{
		if(HabilitarCarreras == 0) return Mensaje(playerid,-1,"No hay una carrera en curso para poder inscribirte.");
		if(EnCarreras[playerid] != 0) return Mensaje(playerid,-1,"Tu ya estas inscripto en la carrera.");
		if(PlayerInfo[playerid][jDinero] < 2000) return Mensaje(playerid,-1,"Necesitas {00CC00}$2.000{FFFFFF} para poder inscribirte en la carrera.");
		if(EmpezoCarrera == 1) return Mensaje(playerid,-1,"No puedes inscribirte si la carrera ya comenzo.");
		if(ParticipantesCarrera >= 13) return Mensaje(playerid,-1,"No hay mas lugares libres para la carrera, espera a la siguiente.");
		QuitarDinero(playerid,2000);
		lugarcarrera ++;
		EnCarreras[playerid] = lugarcarrera;
		ParticipantesCarrera ++;
		ParticipantesCarrera2 ++;
		Mensaje(playerid,-1,"Te inscribiste para participar en la carrera, por favor espera unos minutos.");
		Mensaje(playerid,-1,"Perderás el puesto si sales del estadio.");
		return 1;
	}
	if(EstaEn(playerid, 5, -1394.2090,-560.8654,1061.2335))
	{
		if(HabilitarCarrerasCross == 0) return Mensaje(playerid,-1,"No hay una carrera en curso para poder inscribirte.");
		if(MotoCross[playerid] != 0) return SendClientMessage(playerid,-1,"Tu ya estas inscripto en la carrera.");
		if(PlayerInfo[playerid][jDinero] < 2000) return Mensaje(playerid,-1,"Necesitas {00CC00}$2.000{FFFFFF} para poder inscribirte en la carrera.");
		if(InicioCross == 1) return SendClientMessage(playerid,-1,"No puedes inscribirte si la carrera ya comenzo.");
		if(JugadoresCross() >= 20) return SendClientMessage(playerid,-1,"No hay mas lugares libres para la carrera, espera a la siguiente.");
		QuitarDinero(playerid,2000);
		PuestoCross ++;
		MotoCross[playerid] = PuestoCross;
		Mensaje(playerid,-1,"Te inscribiste para participar en la carrera, por favor espera unos minutos.");
		Mensaje(playerid,-1,"Perderás el puesto si sales del estadio.");
	}
	return true;
}

