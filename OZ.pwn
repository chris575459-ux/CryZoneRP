CMD:dar(playerid,params[])
{
	new id,cantidad,Float:x,Float:y,Float:z,string[256],item[15];
	if(!sscanf(params,"s[15]dd",item,id,cantidad))
	{
		if(strcmp(item,"dinero", true)==0)
		{
			if(PlayerTemp[playerid][pt_CooldownDarDinero] > gettime()) return MensajeEx(playerid,-1,"Debes esperar {FF3300}%d{FFFFFF} segundos para volver a dar dinero.", PlayerTemp[playerid][pt_CooldownDarDinero] - gettime());
			if(PlayerInfo[playerid][horasjugadas] < 6) return Mensaje(playerid, -1, "Necesitas por lo menos haber jugador 6 horas para dar dinero.");
			if(PlayerInfo[playerid][jNivel] < 3) return Mensaje(playerid,-1,"Necesitas ser nivel 3 para dar dinero.");
			if(!IsPlayerConnected(id)) return Mensaje(playerid,COLORCARGO0,"No se encontró ningún jugador.");
			if(PlayerInfo[playerid][jDinero] < cantidad) return Mensaje(playerid,-1,"Tu no tienes esa cantidad de dinero.");
			if(Specteando[id] == 1) return Mensaje(playerid,-1,"No estas cerca de ese jugador.");
			if(id == playerid) return 1;
			GetPlayerPos(id, x,y,z);
			if(cantidad > 0 && cantidad < 5000000)
			{
				if(EstaEn(playerid, 5.0, x,y,z))
				{
					new day,month,year,Query[200];
					getdate(year,month,day);
					DarDinero(id, cantidad);
					QuitarDinero(playerid, cantidad);
					format(string, sizeof(string), "%s saca su billetera y le da algo de dinero a %s.",NombreJugador(playerid),NombreJugador(id));
					ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
					format(string, sizeof(string), "%s{FFFFFF} te ha dado {00CC00}$%s{FFFFFF}.",NombreJugador(playerid),ConvertirNumero(cantidad));
					Mensaje(id,COLOR_AZUL,string);
					mysql_format(Conecction, Query, sizeof(Query), "INSERT INTO `log_ventas` (Vendedor, item, cantidad, comprador,fecha) VALUES ('%s','Dinero', '%d','%s','%02d/%02d/%02d')",NombreJugador(playerid),cantidad,NombreJugador(id),day,month,year);
					mysql_pquery(Conecction, Query);

					if(cantidad >= 50000)
					{
						format(string, 256, "Jugador %s (ID:{FFFFFF} %d{DBED15} | Nivel: %d) entrega $%s a %s (ID:{FFFFFF} %d{DBED15} | Nivel: %d)", NombreJugador(playerid),playerid,PlayerInfo[playerid][jNivel],ConvertirNumero(cantidad),NombreJugador(id),id,PlayerInfo[id][jNivel]);
						MensajeAdmin(COLOR_YELLOW, string, 5);
					}

					PlayerTemp[playerid][pt_CooldownDarDinero] = gettime() + 60;
					return 1;
				}
				else return Mensaje(playerid,-1,"No estas cerca de ese jugador.");
			}
			else return Mensaje(playerid,-1,"Cantidad invalida.");
		}
	}
	if(!sscanf(params,"s[15]r",item,id))
	{
		if(strcmp(item,"acceso",true) == 0)
		{
			if(Alquilado == false) return 1;
			if(strcmp(Alquilador,NombreJugador(playerid),true) == 0)
			{
				if(IsPlayerConnected(id))
				{
					if(EnSalonEvento(id) || EnSalonEvento2(id))
					{
						if(PermisoSalon[id] == false)
						{
							PermisoSalon[id] = true;
							format(string,sizeof(string),"Le diste acceso al salón a {F6F61D}%s{FFFFFF}, usa {F6F61D}/quitar acceso{FFFFFF} [id] para quitarle el acceso.",NombreJugador(id));
							Mensaje(playerid,-1,string);
							format(string,sizeof(string),"Recibiste una tarjeta de acceso al salón, usa {F6F61D}/ayuda salon{FFFFFF} para recibir información.");
							Mensaje(id,-1,string);
							return 1;
						}
						else Mensaje(playerid,-1,"Ya le diste el acceso a esa persona.");
					}
					else Mensaje(playerid,-1,"El jugador debe estar dentro del salón.");
				}
				else Mensaje(playerid,-1,"Ese jugador está desconectado.");
			}
			else Mensaje(playerid,-1,"No tienes permiso para dar acceso.");
		}
		if(strcmp(item,"microfono",true) == 0)
		{
			if(Emisoras[playerid][TieneEmisora] == 0) return Mensaje(playerid, COLOR_BLANCO, "No tienes una emisora, ve y consigue una en la entrada.");
			if(Emisoras[playerid][TransEmisora] == 0) return Mensaje(playerid, COLOR_BLANCO, "No estas transmitiendo en ninguna cabina de radio.");
			if(IsPlayerConnected(id))
			{
				if(ProxDetectorS(5.0, playerid, id))
				{
					if(TieneMicrofono[id] == Emisoras[playerid][CabinaOcupada]) return Mensaje(playerid, COLOR_BLANCO, "Ya le habias prestado un micrófono a ese jugador.");
					format(string, sizeof(string), "%s le presta un micrófono a %s.", NombreJugador(playerid), NombreJugador(id));
					ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
					SetPlayerAttachedObject(id, 2, 19610, 6, 0.068, 0.022, 0.015, 87.2, 88.1, -7.5, 1, 0.752, 1);
					TieneMicrofono[id] = Emisoras[playerid][CabinaOcupada];
				}	else Mensaje(playerid, COLOR_BLANCO, "Ese jugador esta lejos.");
			}	else Mensaje(playerid, COLOR_BLANCO, "Ese jugador esta desconectado.");
		}
	}
	return 1;
}

CMD:quitar(playerid,params[])
{
	new opcion[15],id,string[147];
	new jugadorid, seleccion[32];
	if(!sscanf(params,"s[15]",opcion))
	{
 		if(strcmp(opcion,"prenda",true) == 0 || strcmp(opcion,"prendas",true) == 0)
		{
		   	new prendas[3], p_e, pe[5] = 0;
		   	// Objeto vacio
			prendas[0] = 11392;
			prendas[1] = 11392;
			// Objeto 1
			if(UsandoObj[playerid]  == 1 && p_e == 0) { prendas[0] = PlayerInfo[playerid][pObj];  p_e = 1; pe[0] = 1; }
			if(UsandoObj2[playerid] == 1 && p_e == 0) { prendas[0] = PlayerInfo[playerid][pObj2]; p_e = 1; pe[1] = 1; }
			if(UsandoObj3[playerid] == 1 && p_e == 0) { prendas[0] = PlayerInfo[playerid][pObj3]; p_e = 1; pe[2] = 1; }
			if(UsandoObj4[playerid] == 1 && p_e == 0) { prendas[0] = PlayerInfo[playerid][pObj4]; p_e = 1; pe[3] = 1; }
			// Objeto 2
			if(UsandoObj[playerid]  == 1 && pe[0] == 0) { prendas[1] = PlayerInfo[playerid][pObj];  }
			if(UsandoObj2[playerid] == 1 && pe[1] == 0) { prendas[1] = PlayerInfo[playerid][pObj2]; }
			if(UsandoObj3[playerid] == 1 && pe[2] == 0) { prendas[1] = PlayerInfo[playerid][pObj3]; }
			if(UsandoObj4[playerid] == 1 && pe[3] == 0) { prendas[1] = PlayerInfo[playerid][pObj4]; }
			ShowModelSelectionMenuEx2(playerid, prendas, 2, "Prendas", OBJETIVOPRENDA3, 141.7, -8.69999, 18.8, 1.0, 0x000000FF, 0x000000FF, 2);//0x0000008A
			return 1;
		}
		if(strcmp(params,"sed",true) == 0)
		{
			if(SedOculta[playerid] == 0) return Mensaje(playerid,-1,"La barra de sed ya esta oculta.");
			SedOculta[playerid] = 0;
			BarraHambre(playerid, 2);
			BarraSed(playerid, 2);
			Mensaje(playerid,-1,"Ocultaste la barra de sed, para volver a mostrarla usa {DBED15}/mostrar sed{FFFFFF}.");
			return 1;
		}
	}
	if(!sscanf(params,"s[15]r",opcion,id))
	{
		if(strcmp(opcion,"acceso",true) == 0)
		{
			if(Alquilado == false) return 1;
			if(strcmp(Alquilador,NombreJugador(playerid),true) == 0 || PermisoSalon[playerid] == true)
			{
				if(IsPlayerConnected(id))
				{
					if(EnSalonEvento(id) || EnSalonEvento2(id))
					{
						if(PermisoSalon[id] == true)
						{
							if(strcmp(NombreJugador(id),Alquilador,true) == 0) return Mensaje(playerid,-1,"No puedes quitarle el acceso a esa persona.");
							PermisoSalon[id] = false;
							format(string,sizeof(string),"Le quitaste el acceso al salón a {F6F61D}%s [%d]{F6F61D}.",NombreJugador(id),id);
							Mensaje(playerid,-1,string);
							format(string,sizeof(string),"Te quitaron tu acceso, ya no tendras los beneficios de la tarjeta.");
							Mensaje(id,-1,string);
							return 1;
						}
						else Mensaje(playerid,-1,"Ese jugador no tiene acceso al salón.");
					}
					else Mensaje(playerid,-1,"El jugador debe estar dentro del salón.");
				}
				else Mensaje(playerid,-1,"Ese jugador está desconectado.");
			}
			else Mensaje(playerid,-1,"No tienes permiso para quitar acceso.");
		}
	}
	if(!sscanf(params, "s[32]u", seleccion, jugadorid))
	{
		if(strcmp(seleccion,"microfono",true) == 0)
		{
			if(Emisoras[playerid][TieneEmisora] == 0) return Mensaje(playerid, COLOR_BLANCO, "No tienes una emisora, ve y consigue una en la entrada.");
			if(Emisoras[playerid][TransEmisora] == 0) return Mensaje(playerid, COLOR_BLANCO, "No estas transmitiendo en ninguna cabina de radio.");
			if(IsPlayerConnected(jugadorid))
			{
				if(ProxDetectorS(5.0, playerid, jugadorid))
				{
					if(TieneMicrofono[jugadorid] == -1) return Mensaje(playerid, COLOR_BLANCO, "Ese jugador no tiene micrófono.");
					format(string, sizeof(string), "%s le quito el micrófono a %s.", NombreJugador(playerid), NombreJugador(jugadorid));
					ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
					RemovePlayerAttachedObject(jugadorid,2);
					TieneMicrofono[jugadorid] = -1;
				}	else Mensaje(playerid, COLOR_BLANCO, "Ese jugador esta lejos.");
			}	else Mensaje(playerid, COLOR_BLANCO, "Ese jugador esta desconectado.");
		}
	}
	return true;
}

CMD:consumir(playerid,params[])
{
	if(PlayerInfo[playerid][Agonizando] > 0 || VariableCaido[playerid] == 1 || Esposando[playerid] == 2 || EnPatrulla[playerid] == 1) return Mensaje(playerid,-1,"No puedes hacer esto en este momento.");

	if(isnull(params)) return SendClientMessage(playerid, 0xFFFFFFAA, "Para consumir usa {DBED15}/consumir crack{FFFFFF} o {DBED15}/consumir medicamentos");

	new string[256];
	if(strcmp(params, "crack", true) == 0)
	{
		new Float:resta,Float:Chaleco;
		GetPlayerArmour(playerid, Chaleco);
		resta = 100-Chaleco;
		if(PlayerInfo[playerid][pCrack] <= 0) return Mensaje(playerid, -1, "No tienes gramos de crack.");
		if(EnCombate[playerid] == 1) return Mensaje(playerid,-1,"No puedes hacer esto en combate, espera unos segundos y reintenta.");
		if(gettime() > TiempoCrack[playerid])
		{
			if(resta >= 10) EstablecerChaleco(playerid, Chaleco+10.0);
			else if(resta < 10) EstablecerChaleco(playerid, Chaleco+resta);
			if(!IsPlayerInAnyVehicle(playerid)) ClearAnimations(playerid);
			PlayerInfo[playerid][pCrack]--;
			TiempoCrack[playerid] = gettime() + 30;
			LoopingAnim(playerid, "FOOD", "EAT_Burger", 3.00, 0, 0, 0, 0, 0);
			format(string, sizeof string, "%s consume crack.", NombreJugador(playerid));
			ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
			RevisarLogroCrack(playerid);
		}
		else
		{
			new numero = TiempoCrack[playerid] - gettime();
			format(string, sizeof string, "Por favor espera {FF3300}%d{FFFFFF} segundos para volver a consumir crack.", numero);
			Mensaje(playerid, -1, string);
		}
	}
	else if(strcmp(params, "medicamento", true) == 0 || strcmp(params, "medicamentos", true) == 0 || strcmp(params, "remedio", true) == 0)
	{
		new Float:resta2,Float:Vida;
		GetPlayerHealth(playerid, Vida);
		resta2 = 100-Vida;
		if(PlayerInfo[playerid][pMedicamentos] <= 0) return Mensaje(playerid, -1, "No tienes medicamentos.");
		if(EnCombate[playerid] == 1) return Mensaje(playerid,-1,"No puedes hacer esto en combate, espera unos segundos y reintenta.");
		if(gettime() > TiempoMedicamentos[playerid])
		{
			if(resta2 >= 10) EstablecerVida(playerid, Vida+10.0);
			else if(resta2 < 10) EstablecerVida(playerid, Vida+resta2);
			if(!IsPlayerInAnyVehicle(playerid)) ClearAnimations(playerid);
			PlayerInfo[playerid][pMedicamentos]--;
			TiempoMedicamentos[playerid] = gettime() + 30;
			LoopingAnim(playerid, "FOOD", "EAT_Burger", 3.00, 0, 0, 0, 0, 0);
			format(string, sizeof string, "%s consume un medicamento.", NombreJugador(playerid));
			ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
			RevisarLogroRemedio(playerid);
		}
		else
		{
			new numero = TiempoMedicamentos[playerid] - gettime();
			format(string, sizeof string, "Por favor espera {FF3300}%d{FFFFFF} segundos para volver a consumir medicamentos.", numero);
			Mensaje(playerid, -1, string);
		}
	}
	else if(strcmp(params, "caramelo", true) == 0 || strcmp(params, "caramelos", true) == 0)
	{
		if(PlayerInfo[playerid][pCaramelos] <= 0) return SendClientMessage(playerid, 0xFFFFFFAA, "No tienes caramelos para consumir.");

		if(gettime() > TiempoCaramelos[playerid])
		{
			LoopingAnim(playerid, "FOOD", "EAT_Burger", 3.00, 0, 0, 0, 0, 0);
			format(string, sizeof string, "%s come un caramelo.", NombreJugador(playerid));
			ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);

			PlayerInfo[playerid][pCaramelos]--;
			TiempoCaramelos[playerid] = gettime() + 30;

			PlayerInfo[playerid][pHambre] -= 5;
			if(PlayerInfo[playerid][pHambre] <= 0) PlayerInfo[playerid][pHambre] = 0;
			if(SedOculta[playerid] == 1) BarraHambre(playerid, 1);
		}
		else
		{
			MensajeEx(playerid, 0xFFFFFFAA, "Por favor espera {FF0000}%d {FFFFFF}segundos para volver a consumir caramelos.", TiempoCaramelos[playerid] - gettime());
		}

	}
	else SendClientMessage(playerid, 0xFFFFFFAA, "Para consumir usa {DBED15}/consumir crack{FFFFFF} o {DBED15}/consumir medicamentos");
	return 1;
}

CMD:abandonar(playerid,params[])
{
	new item[15];
	if(!sscanf(params, "s[15]", item))
	{
		if(strcmp(item, "carga", true) == 0)
		{
			new autoid = GetPlayerVehicleID(playerid);
			if(PlayerInfo[playerid][jJob] != CAMIONERO && PlayerInfo[playerid][jJob2] != CAMIONERO) return Mensaje(playerid, -1, "No eres camionero.");
			if(!IsPlayerInAnyVehicle(playerid)) return Mensaje(playerid, -1, "No estas en un camión.");
			if(!CAMION(autoid) && !CAMIONVIP(autoid)) return Mensaje(playerid, -1, "No estas en un camión.");
			if(CamionCargado[autoid] < 1) return Mensaje(playerid, -1, "Este camión no esta cargado.");
			{
				Chofer[autoid] = INVALID_PLAYER_ID;
				CamionCargado[autoid]= 0;
				DisablePlayerCheckpoint(playerid);
				TimerCamionero[playerid] = 0;
				Mensaje(playerid, -1, "Has abandonado la carga de este camion.");
				return 1;
			}
		}
		if(strcmp(item, "recorrido", true) == 0)
		{
			if(PlayerInfo[playerid][jJob] != BASURERO && PlayerInfo[playerid][jJob2] != BASURERO) return Mensaje(playerid, -1, "No eres basurero.");
			if(Companero[playerid] != -1)
			{
				RecorridoBasu[Companero[playerid]] = 0;
				PBasura[Companero[playerid]] = 0;
				Companero[Companero[playerid]] = -1;
				DisablePlayerCheckpoint(Companero[playerid]);
				TimerBasurero[Companero[playerid]] = 0;
				Mensaje(Companero[playerid], -1, "Tu compañero ha cancelado el recorrido.");
				if(BASU(GetPlayerVehicleID(Companero[playerid])) && GetPlayerVehicleSeat(Companero[playerid]) == 0)
				{
					PBasura[Companero[playerid]] = 1;
					Mensaje(Companero[playerid],-1,"{999966}Consigue a un compañero basurero y comienza a trabajar.");
					SetPlayerCheckpoint(Companero[playerid], -178.6163, -1618.1124, 3.4908,5.0);
					PuntoBaX[Companero[playerid]] = -178.6163; PuntoBaY[Companero[playerid]] = -1618.1124; PuntoBaZ[Companero[playerid]] = 3.4908; PuntoBaA[Companero[playerid]] = 5.0;
				}

				RecorridoBasu[playerid] = 0;
				PBasura[playerid] = 0;
				Companero[playerid] = -1;
				DisablePlayerCheckpoint(playerid);
				TimerBasurero[playerid] = 0;
				Mensaje(playerid, -1, "Cancelaste el recorrido.");
				if(BASU(GetPlayerVehicleID(playerid)) && GetPlayerVehicleSeat(playerid) == 0)
				{
					PBasura[playerid] = 1;
					Mensaje(playerid,-1,"{999966}Consigue a un compañero basurero y comienza a trabajar.");
					SetPlayerCheckpoint(playerid, -178.6163, -1618.1124, 3.4908,5.0);
					PuntoBaX[playerid] = -178.6163; PuntoBaY[playerid] = -1618.1124; PuntoBaZ[playerid] = 3.4908; PuntoBaA[playerid] = 5.0;
				}
				return 1;
			}
			else return Mensaje(playerid, -1, "Actualmente no estas trabajando en ningun recorrido de basurero.");
		}
		if(strcmp(item, "transportista", true) == 0)
		{
			if(tviaje[playerid] != 0)
			{
				if(IsValidDynamicObject(objeto1[playerid])) DestroyDynamicObjectEx(objeto1[playerid]);
				if(IsValidDynamicObject(objeto2[playerid])) DestroyDynamicObjectEx(objeto2[playerid]);
				if(IsValidDynamicObject(objeto3[playerid])) DestroyDynamicObjectEx(objeto3[playerid]);
				if(IsValidDynamicObject(objeto4[playerid])) DestroyDynamicObjectEx(objeto4[playerid]);
				if(IsValidDynamicObject(objeto5[playerid])) DestroyDynamicObjectEx(objeto5[playerid]);
				Viaje[playerid] = 0;
				tviaje[playerid] = 0;
				DisablePlayerCheckpoint(playerid);
				Mensaje(playerid, -1, "Abandonaste tu ruta de transportista.");
				return 1;
			}
			else return Mensaje(playerid, -1, "Actualmente no estas trabajando en ningun recorrido de transportista.");
		}
	}
	return 1;
}


