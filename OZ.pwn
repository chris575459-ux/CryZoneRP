			QuitarDinero(playerid,PrecioArma[playerid]);
			DarDinero(OfrecioArma[playerid],PrecioArma[playerid]);
			new Query[200];
			mysql_format(Conecction, Query, sizeof(Query), "INSERT INTO `log_ventas` (Vendedor, item, arma, precio, comprador,fecha) VALUES ('%s','Arma', '%d','%d','%s','%02d/%02d/%02d')",NombreJugador(OfrecioArma[playerid]),ArmaOfrecida[playerid],PrecioArma[playerid],NombreJugador(playerid),day,month,year);
			mysql_pquery(Conecction, Query);
			OfrecioArma[playerid] = -1;
			ArmaOfrecida[playerid] = 0;
			PrecioArma[playerid] = 0;
			TiempoVentaArmas[playerid] = 0;
		}
		if(strcmp(item,"oz",true) == 0)
		{
			if(!PuedeHoras(playerid)) return 1;
			if(!PlayerConectado(CompradorFZ[playerid])) return Mensaje(playerid,-1,"El jugador que te ofrecio un Oz está desconectado.");
			if(CompradorFZ[playerid] == -1) return Mensaje(playerid,-1,"Nadie te ofreció Oz.");
		 	if(!EstaEnP(playerid,CompradorFZ[playerid],7)) return Mensaje(playerid,-1,"Tienes que estar cerca del jugador que te ofreció.");
		   	if(PlayerInfo[playerid][jDinero] < PrecioFZ[playerid]) return Mensaje(playerid,-1,"No tienes el dinero suficiente para aceptar los Oz.");
		  	if(PlayerInfo[CompradorFZ[playerid]][Moneda] < FZAVender[playerid]) return Mensaje(playerid,-1,"El jugador que te ofreció los Oz ya no tiene esa cantidad.");
   			new count;
	  		if(TiempoVentaFZ[CompradorFZ[playerid]] < gettime()) Mensaje(playerid,-1,"El vendedor se olvidó que te iba a vender.");
			for(new i, j = GetPlayerPoolSize(); i <= j; i++)
		 	{
		  		if(PlayerConectado(i) && CompradorFZ[i] == CompradorFZ[playerid] && i != playerid) count ++;
			}
			if(count != 0) return Mensaje(playerid,-1,"* El vendedor se olvidó que te iba a vender.");
			new string[70];
			format(string,sizeof(string),"%s recibió algo de %s.",NombreJugador(playerid),NombreJugador(CompradorFZ[playerid]));
			ProxDetector(10.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
			PlayerInfo[CompradorFZ[playerid]][Moneda] -= FZAVender[playerid];
			PlayerInfo[playerid][Moneda] += FZAVender[playerid];
			QuitarDinero(playerid,PrecioFZ[playerid]);
			DarDinero(CompradorFZ[playerid],PrecioFZ[playerid]);
