CMD:gh(playerid,params[])
{
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_ADM) return 0;
	new precio,nivel,moneda,Float:Pos[4],query[600],interior;
	if(!sscanf(params,"dddd",precio,nivel,moneda,interior))
	{
		new zone[MAX_ZONE_NAME];
		GetPlayer3DZone(playerid, zone, sizeof(zone));
		new id = CasaDesocupada();
		if(id == -1) return Mensaje(playerid,-1,"No se puede crear mas propiedades.");
		if(interior <= 0 || interior >= 44) return Mensaje(playerid,-1,"Ese interior es inválido.");
		if(moneda < 0 || moneda > 1) return Mensaje(playerid,-1,"Esa moneda es inválida.");
		GetPlayerPos(playerid,Pos[0],Pos[1],Pos[2]);
		GetPlayerFacingAngle(playerid,Pos[3]);
		mysql_format(Conecction, query, sizeof(query),"INSERT INTO `propiedades` (`ID`,`Localizacion`,`PosX`,`PosY`,`PosZ`,`PosA`,`Interior`,`VW`,`Propietario`,`EnUso`) VALUES ('%d','%s','%f','%f','%f','%f','%d','%d','Nadie','1')", id, zone, Pos[0], Pos[1], Pos[2], Pos[3], interior, id+10001);
		mysql_pquery(Conecction, query);
		PropInfo[id][pEnUso] = 1;
		DarInteriorCasa(id,interior);
		PropInfo[id][pEnVenta] = 1;
		PropInfo[id][pPrecio] = precio;
		PropInfo[id][pNivel] = nivel;
		PropInfo[id][pEnVentaPor] = moneda;
		if(interior == 40) PropInfo[id][pTipo] = 2;
		else if(interior == 41) PropInfo[id][pTipo] = 3;
		else if(interior == 43) PropInfo[id][pTipo] = 4;
		else PropInfo[id][pTipo] = 1;
		PropInfo[id][pPosX] = Pos[0];
		PropInfo[id][pPosY] = Pos[1];
		PropInfo[id][pPosZ] = Pos[2];
		PropInfo[id][pPosA] = Pos[3];
		PropInfo[id][pAbierta] = 1;
		PropInfo[id][pRoperoAbierto] = 0;
		PropInfo[id][propiedad_ID] = id;
		PropInfo[id][pVW] = id+10001;
		format(PropInfo[id][pPropietario],40,"Nadie");
		format(PropInfo[id][pLocalizacion],80,zone);
		PropInfo[id][pInterior] = interior;
		CargarPropiedad(id);
		GuardarPropiedad(id);
		if(moneda == 0)	format(query,sizeof(query),"Creaste la casa ID: {DBED15}%d (Precio: {00CC00}$%s{DBED15}) {FFFFFF}-{DBED15} (Interior: %d).",id,ConvertirNumero(precio),interior);
		else format(query,sizeof(query),"Creaste la casa ID: {DBED15}%d (Precio: %s Oz) {FFFFFF}-{DBED15} (Interior: %d).",id,ConvertirNumero(precio),interior);
		Mensaje(playerid,-1,query);
		format(query,sizeof(query),"Usa {DBED15}/ge %d{FFFFFF} para crear el garaje. Recuerda mirar para el lado del portón.", id);
		Mensaje(playerid,-1,query);
	}
	else Mensaje(playerid,-1,"Por favor, usa {DBED15}/gh [precio] [nivel] [moneda(0-1)] [interior]");
	return true;
}

CMD:cg(playerid,params[])
{
	new casa,string[200];
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_ADM) return 0;
	if(sscanf(params, "d", casa))return Mensaje(playerid,COLOR_GRAD2,"Uso /cg [ID]");
	{
		if(PropInfo[casa][pGX] != 0)
		{
			new Float:X,Float:Y,Float:Z,Float:A;
			GetPlayerPos(playerid, X,Y,Z);
			GetPlayerFacingAngle(playerid,A);
			PropInfo[casa][pGX] = X;
			PropInfo[casa][pGY] = Y;
			PropInfo[casa][pGZ] = Z;
			PropInfo[casa][pGA] = A;
			format(string,sizeof(string),"Editaste la entrada del garaje con id: {DBED15}%d", casa);
			Mensaje(playerid,-1,string);
			mysql_format(Conecction, string, sizeof(string),"UPDATE `propiedades` SET `GX`='%f',`GY`='%f',`GZ`='%f',`GA`='%f' WHERE `ID`='%d'", PropInfo[casa][pGX],PropInfo[casa][pGY],PropInfo[casa][pGZ],PropInfo[casa][pGA],PropInfo[casa][propiedad_ID]);
			mysql_pquery(Conecction, string);
			BorrarLabelCasa(casa);
			CargarPropiedad(casa);
		}
		else Mensaje(playerid,-1,"Esa propiedad no tiene un garage.");
	}
	return 1;
}

CMD:ge(playerid,params[])
{
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_ADM) return 0;
	if(!sscanf(params,"d",params[0]))
	{
		new Float:Pos[4],p = params[0],string[500];
		GetPlayerPos(playerid,Pos[0],Pos[1],Pos[2]);
		GetPlayerFacingAngle(playerid,Pos[3]);
		if(PropInfo[p][pEnUso] == 1)
		{
			if(PropInfo[p][pGX] == 0)
			{
				PropInfo[p][pGX] = Pos[0];
				PropInfo[p][pGY] = Pos[1];
				PropInfo[p][pGZ] = Pos[2];
				PropInfo[p][pGA] = Pos[3];
				format(string,sizeof(string),"Creaste un garaje a la casa con id: {DBED15}%d",p);
				Mensaje(playerid,-1,string);
				mysql_format(Conecction, string, sizeof(string),"UPDATE `propiedades` SET `GX`='%f',`GY`='%f',`GZ`='%f',`GA`='%f' WHERE `ID`='%d'", PropInfo[p][pGX],PropInfo[p][pGY],PropInfo[p][pGZ],PropInfo[p][pGA],PropInfo[p][propiedad_ID]);
				mysql_pquery(Conecction, string);
				BorrarLabelCasa(p);
				CargarPropiedad(p);
			}
			else Mensaje(playerid,-1,"Esa propiedad ya tiene un garage.");
		}
		else Mensaje(playerid,-1,"Esa propiedad no existe.");
	}
	else Mensaje(playerid,-1,"Por favor, usa {DBED15}/ge [ID]");
	return true;
}

CMD:quitarcasa(playerid, params[])
{
  	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_ADM) return 0;
	new giveplayerid, casadi, Query[100], string[100];
	if(sscanf(params, "dd", giveplayerid, casadi)) return Mensaje(playerid, -1, "USA: /quitarcasa [playerid] [casa id(1-2)]");
	switch (casadi)
	{
		case 1:
		{
			if(PlayerInfo[giveplayerid][CasaID] > 0)
			{
				for(new p=1; p < MAX_PROPIEDADES; p++)
				{
					if(PropInfo[p][propiedad_ID] == PlayerInfo[giveplayerid][CasaID] && PropInfo[p][pPosX] != 0)
					{
						PlayerInfo[giveplayerid][CasaID] = 0;
						mysql_format(Conecction, Query, sizeof(Query),"UPDATE `usuarios` SET `CasaID`='0' WHERE `ID`='%d'", PlayerInfo[giveplayerid][p_DBID]);
						mysql_pquery(Conecction, Query);
						PropInfo[p][pEnVenta] = 1;
						format(PropInfo[p][pPropietario],40,"Nadie");
						PropInfo[p][pAbierta] = 1;
						PropInfo[p][pRoperoAbierto] = 0;
						PropInfo[p][pIDPropietario] = 0;
						mysql_format(Conecction, Query, sizeof(Query),"UPDATE `propiedades` SET `Propietario`='Nadie',`IDPropietario`='0',`EnVenta`='1' WHERE `ID`='%d'", p);
						mysql_pquery(Conecction, Query);
						BorrarLabelCasa(p);
						GuardarPropiedad(p);
						CargarPropiedad(p);
						format(string, sizeof(string), "%s, Le quitaste el Casa ID 1 a %s.", NombreJugador(playerid), NombreJugador(giveplayerid));
						Mensaje(playerid, -1, string);
						Log("logs/quitarcasa.log", string);
						break;
					}
				}
			}
			else Mensaje(playerid, -1, "No existe casa 1.");
			return 1;
		}
		case 2:
		{
			if(PlayerInfo[giveplayerid][CasaID2] != 0)
			{
				for(new p=1; p < MAX_PROPIEDADES; p++)
				{
					if(PropInfo[p][propiedad_ID] == PlayerInfo[giveplayerid][CasaID2])
					{
						PlayerInfo[giveplayerid][CasaID2] = 0;
						mysql_format(Conecction, Query, sizeof(Query),"UPDATE `usuarios` SET `CasaID2`='0' WHERE `ID`='%d'", PlayerInfo[giveplayerid][p_DBID]);
						mysql_pquery(Conecction, Query);
						PropInfo[p][pEnVenta] = 1;
						format(PropInfo[p][pPropietario],40,"Nadie");
						PropInfo[p][pAbierta] = 1;
						PropInfo[p][pRoperoAbierto] = 0;
						PropInfo[p][pIDPropietario] = 0;
						mysql_format(Conecction, Query, sizeof(Query),"UPDATE `propiedades` SET `Propietario`='Nadie',`IDPropietario`='0',`EnVenta`='1' WHERE `ID`='%d'", p);
						mysql_pquery(Conecction, Query);
						BorrarLabelCasa(p);
						GuardarPropiedad(p);
						CargarPropiedad(p);
						format(string, sizeof(string), "%s, Le quitaste el Casa ID 2 a %s.", NombreJugador(playerid), NombreJugador(giveplayerid));
						Mensaje(playerid, -1, string);
						Log("logs/quitarcasa.log", string);
						break;
					}
				}
			}
			else Mensaje(playerid, -1, "No existe casa 2.");
			return 1;
		}
	}
   	return 1;
}

CMD:ec(playerid,params[])
{
	if(PlayerInfo[playerid][jAdminNvl] < ADMIN_LVL_ADM) return 0;
	new id,precio,nivel,moneda,comprable,string[70];
	if(!sscanf(params,"ddddd",id,precio,nivel,moneda,comprable))
	{
