stock ObtenerVelocidad(playerid) // Formula original
{
	new Float:velx, Float:vely, Float:velz;
	GetVehicleVelocity(GetPlayerVehicleID(playerid), velx, vely, velz);
	new Float:vel = floatmul(floatadd(floatadd(floatpower(velx, 2), floatpower(vely, 2)),  floatpower(velz, 2)), 172.0);
	return floatround(vel);
}

stock ObtenerVelocidadVehicle(vehicleid) // Formula original
{
	new Float:velx, Float:vely, Float:velz;
	GetVehicleVelocity(vehicleid, velx, vely, velz);
	new Float:vel = floatmul(floatadd(floatadd(floatpower(velx, 2), floatpower(vely, 2)),  floatpower(velz, 2)), 172.0);
	return floatround(vel);
}

stock MensajeAdmin(color, string[], level)
{
	if(level < 2) level = 3;
	if(level > 7) return 1;
	foreach(new i : Player)
	{
		if(PlayerInfo[i][jAdminNvl] >= level && VerAdmin[i] == 1)
		{
			Mensaje(i, color, string);
		}
	}
	return 1;
}

public OnVehiclePaintjob(playerid, vehicleid, paintjobid)
{
	#if DEBUG_GM == 1
	printf("(OnVehiclePaintjob) PlayerID: %d | VehicleID: %d | PaintJobID: %d", playerid, vehicleid, paintjobid);
	#endif
	if(vehicleid == autos1[playerid])
	{
		new query[90];
		PlayerInfo[playerid][vPJ] = paintjobid;
		ChangeVehiclePaintjob(vehicleid, paintjobid);
		mysql_format(Conecction, query, sizeof(query), "UPDATE `usuarios` SET `PaintJob`='%d' WHERE `ID`='%d'",paintjobid, PlayerInfo[playerid][p_DBID]);
		mysql_pquery(Conecction, query);
	}
	if(vehicleid == autos2[playerid])
	{
		new query[90];
