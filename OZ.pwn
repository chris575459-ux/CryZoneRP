		if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT) return Ingresar(playerid, 2);
		else if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER) return IngresarAuto(playerid);
		else return 1;
	}
	else Mensaje(playerid, -1, "No puedes hacer esto si estas en prisión.");
	return 1;
}

