	format(url, sizeof(url), "127.0.0.1/logof.php?&name=%s", NombreJugador(playerid));
	HTTP(playerid, HTTP_GET, url, "", "");
	*/

	if(IsPlayerNPC(playerid))
	{
		new string[32];
		switch(reason)
		{
			case 0: format(string, sizeof string, "Timeout (perdió conexión)");
			case 1: format(string, sizeof string, "Quiteo por si solo (??)");
			case 2: format(string, sizeof string, "El GM lo kickeó");
			default: format(string, sizeof string, "Indefinido %d (wtf)");
		}
		printf("El NPC ID %d se ha desconectado! Razón: %s", string);
	}

	#if DEBUG_GM == 1
	printf("(OnPlayerDisconnect) PlayerID: %d | Reason: %d", playerid, reason);
	#endif
	//Fix NPC
	if(IsPlayerNPC(playerid))
	{
		if(!strcmp(NombreJugador(playerid),"Tren_Uno"))
		{
			ReJoinNPC(0);
		}
		else if(!strcmp(NombreJugador(playerid),"Tren_Dos"))
		{
			ReJoinNPC(1);
		}
		else if(!strcmp(NombreJugador(playerid),"Tren_Tres"))
		{
			ReJoinNPC(2);
		}
		else if(!strcmp(NombreJugador(playerid),"Tren_Cuatro"))
		{
			ReJoinNPC(3);
		}
		else if(!strcmp(NombreJugador(playerid), "Alexis", true))
		{
			ReJoinNPC(4);
		}
		else if(!strcmp(NombreJugador(playerid), "Eobard", true))
		{
			ReJoinNPC(5);
		}
		else if(!strcmp(NombreJugador(playerid), "Leao", true))
		{
			ReJoinNPC(6);
		}
		else if(!strcmp(NombreJugador(playerid), "Angelica", true))
		{
			DestroyDynamic3DTextLabel(TextBot[0]);
			ReJoinNPC(7);

		}
		else if(!strcmp(NombreJugador(playerid), "Marcelo", true))
		{
